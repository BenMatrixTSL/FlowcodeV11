						<param name='NumChars' type='u8' description='Number of characters in the string data, 0 for auto, 2 for 0x00, 4 for 0x0000' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Prefix' type='b1' description='Generate hexadecimal prefix 0x e.g. 1= 0xFF, 0= FF' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
