						<command class_type='decision' title='Decision' exp='.NumChars = 0' swap='0' >
							<flowline >
								<command class_type='decision' title='Decision' exp='.Number &lt; 256' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='.NumChars = 2' />
										</command>
									</flowline>
									<flowline >
										<command class_type='decision' title='Decision' exp='.Number &lt; 65536' swap='0' >
											<flowline >
												<command class_type='calculation' title='Calculation' >
													<exp exp='.NumChars = 4' />
												</command>
											</flowline>
											<flowline >
												<command class_type='calculation' title='Calculation' >
													<exp exp='.NumChars = 8' />
												</command>
											</flowline>
										</command>
									</flowline>
								</command>
							</flowline>
							<flowline />
						</command>
						<command class_type='decision' title='Decision' exp='.NumChars &gt; 8' swap='0' >
