																</flowline>
															</command>
														</flowline>
													</command>
												</flowline>
											</case>
											<case exp='1' >
												<flowline >
													<command class_type='decision' title='Decision' exp='.char = &apos;x&apos;' swap='0' >
														<flowline >
															<command class_type='calculation' title='Calculation' >
																<exp exp='.state = .state + 1' />
															</command>
														</flowline>
														<flowline >
															<command class_type='decision' title='Decision' exp='.char = &apos;0&apos;' swap='0' >
