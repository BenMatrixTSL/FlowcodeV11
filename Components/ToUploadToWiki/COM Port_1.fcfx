								</command>
								<command class_type='calculation' title='Calculation' >
									<exp exp='.idx = .idx + 1' />
								</command>
							</flowline>
						</command>
						<command class_type='loop' title='Send characters starting with most significant' type='0' exp='.idx &gt; 0' >
							<flowline >
								<command class_type='calculation' title='Calculation' >
									<exp exp='.idx = .idx - 1' />
								</command>
