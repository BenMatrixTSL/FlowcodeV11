																<flowline >
																	<command class_type='decision' title='Decision' exp='.IgnoreData' swap='0' >
																		<flowline >
																			<command class_type='calculation' title='Calculation' >
																				<exp exp='.state = .state + 1' />
																			</command>
																		</flowline>
																		<flowline >
																			<command class_type='calculation' title='Not a valid char - return' >
																				<exp exp='.done = 1' />
																			</command>
																		</flowline>
																	</command>
																</flowline>
															</command>
														</flowline>
													</command>
												</flowline>
											</case>
											<case exp='2' >
												<flowline >
													<command class_type='decision' title='Decision' exp='(.char &gt;= &apos;0&apos; &amp;&amp; .char &lt;= &apos;9&apos;) || (.char &gt;= &apos;A&apos; &amp;&amp; .char &lt;= &apos;F&apos;)' swap='0' >
