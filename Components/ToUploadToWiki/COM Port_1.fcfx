						<command class_type='calculation' title='Calculation' >
							<exp exp='.Return = 0' />
						</command>
						<command class_type='decision' title='Decision' exp='.Prefix = 0' swap='0' >
							<flowline >
								<command class_type='calculation' title='Calculation' >
									<exp exp='.state = 2' />
								</command>
							</flowline>
							<flowline />
						</command>
						<command class_type='loop' title='Loop' type='0' exp='.done = 0' >
							<flowline >
								<command class_type='call' title='User Macro' macro='ReceiveByte' >
									<return exp='.char' />
									<argument exp='.Timeout' />
								</command>
								<command class_type='decision' title='Decision' exp='.char &gt;= Tout_Val' swap='0' >
									<flowline >
										<command class_type='calculation' title='Timeout - Return' >
											<exp exp='.done = 1' />
										</command>
									</flowline>
									<flowline >
										<command class_type='sim' title='Component Macro' component='LibraryComponent1' macro='FlashRx' />
										<command class_type='switch' title='Switch' textarea='36,25,36,25' exp='.state' >
											<case >
												<flowline >
													<command class_type='decision' title='Decision' exp='.char = &apos;0&apos;' swap='0' >
														<flowline >
															<command class_type='calculation' title='Calculation' >
																<exp exp='.state = .state + 1' />
															</command>
														</flowline>
														<flowline >
															<command class_type='decision' title='Decision' exp='.IgnoreData' swap='0' >
																<flowline />
																<flowline >
																	<command class_type='calculation' title='Not a valid char - return' >
