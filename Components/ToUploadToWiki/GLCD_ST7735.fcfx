				<macro >
					<flowline name='DrawRoundedRectangleSim' description='Draws a rectangle with rounded corners' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
