							</flowline>
							<flowline >
								<command class_type='decision' title='Draw the background colour?' exp='.Transparent = 1' swap='0' >
									<flowline >
										<command class_type='label' title='Connection Point' labelid='1' labelname='B' />
										<command class_type='calculation' title='Calculate Decision Variable' >
											<exp exp='.D = 3 - (.Radius &lt;&lt; 1)' />
										</command>
										<command class_type='calculation' title='Start Locations' >
											<exp exp='.CurX = 0' />
											<exp exp='.CurY = .Radius' />
										</command>
										<command class_type='loop' title='Loop' type='0' exp='.CurX &lt;= .CurY' >
											<flowline >
												<command class_type='call' title='Call Macro' macro='Plot' >
													<argument exp='.X + .CurX' />
													<argument exp='.Y - .CurY' />
												</command>
												<command class_type='call' title='Call Macro' macro='Plot' >
													<argument exp='.X - .CurX' />
													<argument exp='.Y - .CurY' />
												</command>
												<command class_type='call' title='Call Macro' macro='Plot' >
													<argument exp='.X + .CurY' />
													<argument exp='.Y - .CurX' />
												</command>
												<command class_type='call' title='Call Macro' macro='Plot' >
													<argument exp='.X - .CurY' />
													<argument exp='.Y - .CurX' />
												</command>
												<command class_type='call' title='Call Macro' macro='Plot' >
													<argument exp='.X + .CurX' />
													<argument exp='.Y + .CurY' />
												</command>
												<command class_type='call' title='Call Macro' macro='Plot' >
													<argument exp='.X - .CurX' />
													<argument exp='.Y + .CurY' />
												</command>
												<command class_type='call' title='Call Macro' macro='Plot' >
													<argument exp='.X + .CurY' />
													<argument exp='.Y + .CurX' />
												</command>
												<command class_type='call' title='Call Macro' macro='Plot' >
													<argument exp='.X - .CurY' />
													<argument exp='.Y + .CurX' />
												</command>
												<command class_type='decision' title='Decision' exp='.D &lt; 0' swap='0' >
													<flowline >
														<command class_type='calculation' title='Calculation' >
															<exp exp='.D = .D + ((.CurX &lt;&lt; 2) + 6)' />
														</command>
													</flowline>
													<flowline >
														<command class_type='calculation' title='Calculation' >
															<exp exp='.D = .D + (((.CurX - .CurY) &lt;&lt; 2) + 10)' />
														</command>
														<command class_type='calculation' title='Calculation' >
															<exp exp='.CurY = .CurY - 1' />
														</command>
													</flowline>
												</command>
												<command class_type='calculation' title='Calculation' >
													<exp exp='.CurX = .CurX + 1' />
												</command>
											</flowline>
										</command>
									</flowline>
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='.FGCol = foreground_col_var' />
											<exp exp='.BGCol = background_col_var' />
										</command>
										<command class_type='calculation' title='Calculation' >
