										<command class_type='call' title='Call Component Macro' disable='1' component='cal_spi1' macro='Master_Byte' >
											<argument exp='background_col_var &gt;&gt; 8' />
										</command>
										<command class_type='call' title='Call Component Macro' disable='1' component='cal_spi1' macro='Master_Byte' >
											<argument exp='background_col_var' />
										</command>
										<command class_type='call' title='Component Macro' component='cal_spi1' macro='Master_Transaction' >
											<argument exp='.data' />
											<argument exp='2' />
										</command>
