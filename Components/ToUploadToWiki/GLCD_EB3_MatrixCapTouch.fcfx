												<command class_type='loop' title='Read and Print a row' textarea='36,0,511,300' type='0' exp='.width != .WStop' >
													<flowline >
														<command class_type='decision' title='Check we can read the data' exp='.ColourDepth = 24' swap='0' >
															<flowline >
																<command class_type='sim' title='Built-in Function' macro='File.ReadArray' >
																	<argument exp='.Filehandle' />
																	<argument exp='.BGR' />
																	<argument exp='3' />
																</command>
																<command class_type='calculation' title='Calculation' >
																	<exp exp='.B = .BGR[0]' />
																	<exp exp='.G = .BGR[1]' />
																	<exp exp='.R = .BGR[2]' />
																</command>
																<command class_type='calculation' title='Calculation' >
																	<exp exp='.Address = .Address + 3' />
																</command>
															</flowline>
															<flowline >
																<command class_type='decision' title='Decision' exp='.ColourDepth = 8' swap='0' >
																	<flowline >
																		<command class_type='sim' title='Built-in Function' macro='File.ReadArray' >
																			<argument exp='.Filehandle' />
																			<argument exp='.BGR' />
																			<argument exp='1' />
																		</command>
																		<command class_type='calculation' title='Calculation' >
																			<exp exp='.Address = .Address + 1' />
																		</command>
																		<command class_type='calculation' title='Skip to correct colour location and skip transparancy byte' >
																			<exp exp='.ColIdx = .BGR[0] * 4' />
																		</command>
																		<command class_type='calculation' title='Read pallet data' >
																			<exp exp='.B = .PalletBuffer[.ColIdx]' />
																			<exp exp='.G = .PalletBuffer[.ColIdx + 1]' />
																			<exp exp='.R = .PalletBuffer[.ColIdx + 2]' />
																		</command>
																	</flowline>
																	<flowline >
																		<command class_type='decision' title='Decision' exp='.ColourDepth = 4' swap='0' >
																			<flowline >
																				<command class_type='decision' title='Decision' exp='(.count &amp; 0x01) = 0' swap='0' >
																					<flowline >
																						<command class_type='sim' title='Built-in Function' macro='File.ReadArray' >
																							<argument exp='.Filehandle' />
																							<argument exp='.BGR' />
																							<argument exp='1' />
																						</command>
																						<command class_type='calculation' title='Calculation' >
																							<exp exp='.Address = .Address + 1' />
																						</command>
																						<command class_type='calculation' title='Calculation' >
																							<exp exp='.MaskedColIdx = (.BGR[0] &amp; 0xF0) &gt;&gt; 4' />
																						</command>
																					</flowline>
																					<flowline >
																						<command class_type='calculation' title='Calculation' >
																							<exp exp='.MaskedColIdx = .BGR[0] &amp; 0x0F' />
																						</command>
																					</flowline>
																				</command>
																				<command class_type='calculation' title='Skip to correct colour location and skip transparancy byte' >
																					<exp exp='.MaskedColIdx = .MaskedColIdx * 4' />
																				</command>
																				<command class_type='calculation' title='Read pallet data' >
																					<exp exp='.B = .PalletBuffer[.MaskedColIdx]' />
																					<exp exp='.G = .PalletBuffer[.MaskedColIdx + 1]' />
																					<exp exp='.R = .PalletBuffer[.MaskedColIdx + 2]' />
																				</command>
																			</flowline>
																			<flowline >
																				<command class_type='decision' title='Decision' exp='.ColourDepth = 1' swap='0' >
																					<flowline >
																						<command class_type='decision' title='Decision' exp='(.count &amp; 0x07) = 0' swap='0' >
																							<flowline >
																								<command class_type='sim' title='Built-in Function' macro='File.ReadArray' >
																									<argument exp='.Filehandle' />
																									<argument exp='.BGR' />
																									<argument exp='1' />
																								</command>
																								<command class_type='calculation' title='Calculation' >
																									<exp exp='.Address = .Address + 1' />
																								</command>
																							</flowline>
																							<flowline />
																						</command>
																						<command class_type='calculation' title='Calculate Bit Mask' >
																							<exp exp='.R = 0x80 &gt;&gt; (.count &amp; 0x07)' />
																						</command>
																						<command class_type='calculation' title='Mask Off Pixel Data' >
																							<exp exp='.B = .BGR[0] &amp; .R' />
																						</command>
																						<command class_type='decision' title='Decision' exp='.B' swap='0' >
																							<flowline >
																								<command class_type='calculation' title='Calculation' >
																									<exp exp='.R = 255' />
																									<exp exp='.G = 255' />
																									<exp exp='.B = 255' />
																								</command>
																							</flowline>
																							<flowline >
																								<command class_type='calculation' title='Calculation' >
																									<exp exp='.R = 0' />
																									<exp exp='.G = 0' />
																									<exp exp='.B = 0' />
																								</command>
																							</flowline>
																						</command>
																					</flowline>
																					<flowline />
																				</command>
																			</flowline>
																		</command>
																	</flowline>
																</command>
															</flowline>
														</command>
														<command class_type='decision' title='Transparent mode enabled?' exp='.Transparency' swap='0' >
															<flowline >
																<command class_type='decision' title='Does Colour Match Transparent colour' exp='(TransRGB[0] = .R) &amp;&amp; (TransRGB[1] = .G) &amp;&amp; (TransRGB[2] = .B)' swap='0' >
																	<flowline >
																		<command class_type='goto' title='Skip Drawing Routine' labelid='0' labelname='' />
																	</flowline>
																	<flowline />
																</command>
															</flowline>
															<flowline />
														</command>
														<command class_type='sim' title='Set Pixel Colour' component='Base_GLCD' macro='SetForegroundColour' >
															<argument exp='.R' />
															<argument exp='.G' />
															<argument exp='.B' />
														</command>
														<command class_type='decision' title='Decision' exp='.Orientation &amp; 0x01' swap='0' >
															<flowline >
																<command class_type='sim' title='Set Pixel Colour' component='Base_GLCD' macro='Plot' >
																	<argument exp='.X1 + .Height' />
																	<argument exp='.Y1 + .Width' />
																</command>
															</flowline>
															<flowline >
																<command class_type='sim' title='Set Pixel Colour' component='Base_GLCD' macro='Plot' >
																	<argument exp='.X1 + .Width' />
																	<argument exp='.Y1 + .Height' />
																</command>
															</flowline>
														</command>
														<command class_type='label' title='Connection Point' labelid='0' labelname='A' />
														<command class_type='calculation' title='Move to next pixel in row' textarea='36,0,511,300' >
															<exp exp='.width = .width + .WInc' />
														</command>
														<command class_type='calculation' title='Calculation' >
															<exp exp='.count = .count + 1' />
														</command>
													</flowline>
												</command>
												<command class_type='loop' title='Loop to next 4 byte offset' type='0' exp='.Address &amp; 0x03' >
													<flowline >
														<command class_type='sim' title='Built-in Function' macro='File.ReadArray' >
															<argument exp='.Filehandle' />
															<argument exp='.BGR' />
															<argument exp='1' />
														</command>
														<command class_type='calculation' title='Calculation' >
															<exp exp='.Address = .Address + 1' />
														</command>
													</flowline>
												</command>
												<command class_type='calculation' title='Move to next row' textarea='36,0,511,300' >
													<exp exp='.height = .height + .Hinc' />
												</command>
											</flowline>
										</command>
									</flowline>
									<flowline />
								</command>
								<command class_type='calculation' title='Close File' >
									<exp exp='.Filehandle = 0' />
								</command>
							</flowline>
							<flowline />
						</command>
					</flowline>
				</macro>
