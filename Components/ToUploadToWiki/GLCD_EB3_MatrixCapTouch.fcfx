							<exp exp='CommandTX[3] = .Y' />
							<exp exp='CommandTX[4] = .Y &gt;&gt; 8' />
							<exp exp='CommandTX[5] = .XRadius' />
							<exp exp='CommandTX[6] = .XRadius &gt;&gt; 8' />
							<exp exp='CommandTX[7] = .YRadius' />
							<exp exp='CommandTX[8] = .YRadius &gt;&gt; 8' />
