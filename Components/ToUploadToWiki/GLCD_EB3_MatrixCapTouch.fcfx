															<exp exp='.WStop = .Width' />
														</command>
													</flowline>
												</command>
											</flowline>
											<flowline >
												<command class_type='decision' title='Decision' exp='.WInc = 1' swap='0' >
													<flowline >
														<command class_type='calculation' title='Std Width' >
															<exp exp='.WStart = 0' />
															<exp exp='.WStop = .Width' />
														</command>
													</flowline>
													<flowline >
														<command class_type='calculation' title='Std Width' >
															<exp exp='.WStart = .Width - 1' />
															<exp exp='.WStop = -1' />
														</command>
													</flowline>
												</command>
											</flowline>
										</command>
										<command class_type='decision' title='Flip Height' exp='.Flip &amp; 0x02' swap='0' >
											<flowline >
												<command class_type='decision' title='Decision' exp='.HInc = 1' swap='0' >
													<flowline >
														<command class_type='calculation' title='Flip Height' >
															<exp exp='.HInc = -1' />
															<exp exp='.HStart = .Height - 1' />
															<exp exp='.HStop = -1' />
														</command>
													</flowline>
													<flowline >
														<command class_type='calculation' title='Flip Height' >
															<exp exp='.HInc = 1' />
															<exp exp='.HStart = 0' />
															<exp exp='.HStop = .Height' />
														</command>
													</flowline>
												</command>
											</flowline>
											<flowline >
												<command class_type='decision' title='Decision' exp='.HInc = 1' swap='0' >
													<flowline >
														<command class_type='calculation' title='Std Height' >
															<exp exp='.HStart = 0' />
															<exp exp='.HStop = .Height' />
														</command>
													</flowline>
													<flowline >
														<command class_type='calculation' title='Std Height' >
															<exp exp='.HStart = .Height - 1' />
															<exp exp='.HStop = -1' />
														</command>
													</flowline>
												</command>
											</flowline>
										</command>
										<command class_type='calculation' title='Calculation' >
											<exp exp='.h = .HStart' />
										</command>
										<command class_type='comment' title='' comment='BMP files run from left to right 
working upwards a row at a time
from bottom to top.
' textarea='12,0,12,0' />
										<command class_type='loop' title='Loop through data' textarea='36,0,511,300' type='0' exp='.h != .HStop' >
											<flowline >
												<command class_type='calculation' title='Reset Row Idx' textarea='36,0,511,300' >
													<exp exp='.w = .WStart' />
												</command>
												<command class_type='calculation' title='Calculation' >
													<exp exp='.wacc = 1.0' />
													<exp exp='.count = 0' />
												</command>
												<command class_type='sim' title='Move to start of data' macro='File.SetPos' >
													<argument exp='.Filehandle' />
													<argument exp='.DataOffset + .Address' />
													<argument exp='::File.Seek_Begin' />
												</command>
												<command class_type='calculation' title='Calculation' >
													<exp exp='.Address_Save = .Address' />
												</command>
												<command class_type='loop' title='Read and Print a row' textarea='36,0,511,300' type='0' exp='.w != .WStop' >
													<flowline >
														<command class_type='loop' title='read or skip until we hit the next valid pixel of the source' type='0' exp='.wacc &gt;= 1.0' >
															<flowline >
																<command class_type='decision' title='Check we can read the data' exp='.ColourDepth = 24' swap='0' >
																	<flowline >
																		<command class_type='sim' title='Built-in Function' macro='File.ReadArray' >
																			<argument exp='.Filehandle' />
																			<argument exp='.BGR' />
																			<argument exp='3' />
																		</command>
																		<command class_type='calculation' title='Calculation' >
																			<exp exp='.B = .BGR[0]' />
																			<exp exp='.G = .BGR[1]' />
																			<exp exp='.R = .BGR[2]' />
																		</command>
																		<command class_type='calculation' title='Calculation' >
																			<exp exp='.Address = .Address + 3' />
																		</command>
																	</flowline>
																	<flowline >
																		<command class_type='decision' title='Decision' exp='.ColourDepth = 8' swap='0' >
																			<flowline >
																				<command class_type='sim' title='Built-in Function' macro='File.ReadArray' >
																					<argument exp='.Filehandle' />
																					<argument exp='.BGR' />
																					<argument exp='1' />
																				</command>
																				<command class_type='calculation' title='Calculation' >
																					<exp exp='.Address = .Address + 1' />
																				</command>
																				<command class_type='calculation' title='Skip to correct colour location and skip transparancy byte' >
																					<exp exp='.ColIdx = .BGR[0] * 4' />
																				</command>
																				<command class_type='calculation' title='Read pallet data' >
																					<exp exp='.B = .PalletBuffer[.ColIdx]' />
																					<exp exp='.G = .PalletBuffer[.ColIdx + 1]' />
																					<exp exp='.R = .PalletBuffer[.ColIdx + 2]' />
																				</command>
																			</flowline>
																			<flowline >
																				<command class_type='decision' title='Decision' exp='.ColourDepth = 4' swap='0' >
																					<flowline >
																						<command class_type='decision' title='Decision' exp='(.count &amp; 0x01) = 0' swap='0' >
																							<flowline >
																								<command class_type='sim' title='Built-in Function' macro='File.ReadArray' >
																									<argument exp='.Filehandle' />
																									<argument exp='.BGR' />
																									<argument exp='1' />
																								</command>
																								<command class_type='calculation' title='Calculation' >
																									<exp exp='.Address = .Address + 1' />
																								</command>
																								<command class_type='calculation' title='Calculation' >
																									<exp exp='.MaskedColIdx = (.BGR[0] &amp; 0xF0) &gt;&gt; 4' />
																								</command>
																							</flowline>
																							<flowline >
																								<command class_type='calculation' title='Calculation' >
																									<exp exp='.MaskedColIdx = .BGR[0] &amp; 0x0F' />
																								</command>
																							</flowline>
																						</command>
																						<command class_type='calculation' title='Skip to correct colour location and skip transparancy byte' >
																							<exp exp='.MaskedColIdx = .MaskedColIdx * 4' />
																						</command>
																						<command class_type='calculation' title='Read pallet data' >
																							<exp exp='.B = .PalletBuffer[.MaskedColIdx]' />
																							<exp exp='.G = .PalletBuffer[.MaskedColIdx + 1]' />
																							<exp exp='.R = .PalletBuffer[.MaskedColIdx + 2]' />
																						</command>
																					</flowline>
																					<flowline >
																						<command class_type='decision' title='Decision' exp='.ColourDepth = 1' swap='0' >
																							<flowline >
																								<command class_type='decision' title='Decision' exp='(.count &amp; 0x07) = 0' swap='0' >
																									<flowline >
																										<command class_type='sim' title='Built-in Function' macro='File.ReadArray' >
																											<argument exp='.Filehandle' />
																											<argument exp='.BGR' />
																											<argument exp='1' />
																										</command>
																										<command class_type='calculation' title='Calculation' >
																											<exp exp='.Address = .Address + 1' />
																										</command>
																									</flowline>
																									<flowline />
																								</command>
																								<command class_type='calculation' title='Calculate Bit Mask' >
																									<exp exp='.R = 0x80 &gt;&gt; (.count &amp; 0x07)' />
																								</command>
																								<command class_type='calculation' title='Mask Off Pixel Data' >
																									<exp exp='.B = .BGR[0] &amp; .R' />
																								</command>
																								<command class_type='decision' title='Decision' exp='.B' swap='0' >
																									<flowline >
																										<command class_type='calculation' title='Calculation' >
																											<exp exp='.R = 255' />
																											<exp exp='.G = 255' />
																											<exp exp='.B = 255' />
																										</command>
																									</flowline>
																									<flowline >
																										<command class_type='calculation' title='Calculation' >
																											<exp exp='.R = 0' />
																											<exp exp='.G = 0' />
																											<exp exp='.B = 0' />
																										</command>
																									</flowline>
																								</command>
																							</flowline>
																							<flowline />
																						</command>
																					</flowline>
																				</command>
																			</flowline>
																		</command>
																	</flowline>
																</command>
																<command class_type='calculation' title='Decrement the width accumulator' >
																	<exp exp='.wacc = .wacc - 1.0' />
																</command>
															</flowline>
														</command>
														<command class_type='decision' title='Transparent mode enabled?' exp='.Transparency' swap='0' >
															<flowline >
																<command class_type='decision' title='Does Colour Match Transparent colour' exp='(TransRGB[0] = .R) &amp;&amp; (TransRGB[1] = .G) &amp;&amp; (TransRGB[2] = .B)' swap='0' >
																	<flowline >
																		<command class_type='goto' title='Skip Drawing Routine' labelid='0' labelname='' />
																	</flowline>
																	<flowline />
																</command>
															</flowline>
															<flowline />
														</command>
														<command class_type='sim' title='Set Pixel Colour' component='Base_GLCD' macro='SetForegroundColour' >
															<argument exp='.R' />
															<argument exp='.G' />
															<argument exp='.B' />
														</command>
														<command class_type='decision' title='Decision' exp='.Orientation &amp; 0x01' swap='0' >
															<flowline >
																<command class_type='sim' title='Set Pixel Colour' component='Base_GLCD' macro='Plot' >
																	<argument exp='.X1 + .h' />
																	<argument exp='.Y1 + .w' />
																</command>
															</flowline>
															<flowline >
																<command class_type='sim' title='Set Pixel Colour' component='Base_GLCD' macro='Plot' >
																	<argument exp='.X1 + .w' />
																	<argument exp='.Y1 + .h' />
																</command>
															</flowline>
														</command>
														<command class_type='label' title='Connection Point' labelid='0' labelname='A' />
														<command class_type='calculation' title='Move to next pixel in row' textarea='36,0,511,300' >
															<exp exp='.w = .w + .WInc' />
														</command>
														<command class_type='calculation' title='Calculation' >
															<exp exp='.count = .count + 1' />
														</command>
														<command class_type='calculation' title='Calculation' >
															<exp exp='.wacc = .wacc + .sc_w' />
														</command>
													</flowline>
												</command>
												<command class_type='calculation' title='Calculation' >
													<exp exp='.hacc = .hacc + .sc_h' />
												</command>
												<command class_type='calculation' title='Jump back to beginning of row' >
													<exp exp='.Address = .Address_Save' />
												</command>
												<command class_type='decision' title='Decision' exp='.hacc &gt;= 1.0' swap='0' >
													<flowline >
														<command class_type='loop' title='Loop' type='0' exp='.hacc &gt;= 1.0' >
															<flowline >
																<command class_type='switch' title='Switch' textarea='36,25,36,25' exp='.ColourDepth' >
																	<case >
																		<flowline />
																	</case>
																	<case exp='1' >
																		<flowline >
																			<command class_type='calculation' title='Calculation' >
																				<exp exp='.Address = .Address + (.FileWidth &gt;&gt; 3)' />
																			</command>
																			<command class_type='decision' title='If we are partially through a data byte' exp='(.count &amp; 0x07) &gt; 0' swap='0' >
																				<flowline >
																					<command class_type='calculation' title='Calculation' >
																						<exp exp='.Address = .Address + 1' />
																					</command>
																				</flowline>
																				<flowline />
																			</command>
																		</flowline>
																	</case>
																	<case exp='4' >
																		<flowline >
																			<command class_type='calculation' title='Calculation' >
																				<exp exp='.Address = .Address + (.FileWidth &gt;&gt; 1)' />
																			</command>
																			<command class_type='decision' title='If half way through a data byte' exp='(.count &amp; 0x01) = 1' swap='0' >
																				<flowline >
																					<command class_type='calculation' title='Calculation' >
																						<exp exp='.Address = .Address + 1' />
																					</command>
																				</flowline>
																				<flowline />
																			</command>
																		</flowline>
																	</case>
																	<case exp='8' >
																		<flowline >
																			<command class_type='calculation' title='Calculation' >
																				<exp exp='.Address = .Address + .FileWidth' />
																			</command>
																		</flowline>
																	</case>
																	<case exp='24' >
																		<flowline >
																			<command class_type='calculation' title='Calculation' >
																				<exp exp='.Address = .Address + (.FileWidth * 3)' />
																			</command>
																		</flowline>
																	</case>
																</command>
																<command class_type='loop' title='Loop to next 4 byte offset' type='0' exp='.Address &amp; 0x03' >
																	<flowline >
																		<command class_type='calculation' title='Calculation' >
																			<exp exp='.Address = .Address + 1' />
																		</command>
																	</flowline>
																</command>
																<command class_type='calculation' title='Calculation' >
																	<exp exp='.hacc = .hacc - 1.0' />
																</command>
															</flowline>
														</command>
													</flowline>
													<flowline />
												</command>
												<command class_type='calculation' title='Move to next row' textarea='36,0,511,300' >
													<exp exp='.h = .h + .HInc' />
												</command>
											</flowline>
										</command>
									</flowline>
									<flowline />
								</command>
								<command class_type='calculation' title='Close File' >
									<exp exp='.Filehandle = 0' />
								</command>
							</flowline>
							<flowline />
						</command>
					</flowline>
				</macro>
				<macro >
					<flowline name='DrawBitmap' description='Draws a bitmap image loaded from the SD card
FileInit must be called first to ensure the File System is setup.' statediag='0' >
						<return name='Return' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Filename' type='T8*' description='Bitmap file to open from current directory on SD card 8.3 format' isconst='0' isHidden='0' isinit='0' usrinit='&quot;&quot;' setinit='' >
							<array size='20' />
						</param>
						<param name='X1' type='u16' description='X Axis coordinate, specifies the location of the left edge of the bitmap image' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Y1' type='u16' description='Y Axis coordinate, specifies the location of the top edge of the bitmap image' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Orientation' type='u8' description='0=Normal, 1=90 Degrees CW, 2=180 Degrees, 3=270 Degrees CW' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Transparency' type='u8' description='0=Off, 1=On Don&apos;t Draw Any Pixels which match the transparent colour' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Flip' type='u8' description='0=Normal, 1=FlipWidth, 2=FlipHeight, 3=FlipBoth' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<command class_type='calculation' title='Calculation' >
							<exp exp='CommandTX[0] = &apos;R&apos;' />
							<exp exp='CommandTX[1] = .X1' />
							<exp exp='CommandTX[2] = .X1 &gt;&gt; 8' />
							<exp exp='CommandTX[3] = .Y1' />
							<exp exp='CommandTX[4] = .Y1 &gt;&gt; 8' />
							<exp exp='CommandTX[5] = .Orientation' />
							<exp exp='CommandTX[6] = .Transparency' />
							<exp exp='CommandTX[7] = .Flip' />
							<exp exp='TXCount = 8' />
							<exp exp='RXCount = 0' />
						</command>
						<command class_type='call' title='User Macro' macro='SendCommandWithString' >
							<argument exp='.Filename' />
						</command>
						<command class_type='calculation' title='Calculation' >
							<exp exp='.Return = RXCB :: GetByte ()' />
						</command>
					</flowline>
				</macro>
				<macro >
					<flowline name='PrintFloat' description='This macro prints a floating point number to the Graphical LCD.' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
						<param name='Number' type='f32' description='Floating point number to send to the display.' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<param name='NumDP' type='u8' description='Number of decimal points' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
