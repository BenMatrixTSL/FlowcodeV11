							<array size='20' />
						</local>
						<local name='UART_Str' type='T8' description='' isconst='0' isHidden='0' isinit='1' usrinit='&quot;&quot;' setinit='&quot;&quot;' >
							<array size='20' />
						</local>
						<command class_type='calculation' title='Calculation' >
							<exp exp='.UART_Str = &quot;//UART Receive Interrupt for EB3 display module\n&quot;' />
							<exp exp='.UART_Str = .UART_Str + &quot;MX_UART_INT_CH&quot; + STRING UARTidx + &quot;_FUNC\n&quot;' />
							<exp exp='.UART_Str = .UART_Str + &quot;  MX_UINT8 FCL_RX;\n&quot;' />
