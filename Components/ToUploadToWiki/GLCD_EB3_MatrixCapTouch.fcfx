						<command class_type='calculation' title='Init Variable - Get rid of sim error messages' >
							<exp exp='.BGR[0] = 0' />
							<exp exp='.header[0] = 0' />
						</command>
						<command class_type='decision' title='Decision' exp='.Filehandle' swap='0' >
							<flowline >
								<command class_type='sim' title='Built-in Function' macro='File.SetPos' >
									<argument exp='.Filehandle' />
