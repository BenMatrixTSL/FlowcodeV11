										<command class_type='calculation' title='Initialise idx to start of data' textarea='36,0,511,300' >
											<exp exp='.Address = 0' />
										</command>
										<command class_type='switch' title='Switch' textarea='36,25,36,25' exp='.Orientation' >
											<case >
												<flowline >
													<command class_type='calculation' title='Specify Start Stop and Increment Values' >
														<exp exp='.WInc = 1' />
														<exp exp='.HInc = -1' />
													</command>
												</flowline>
											</case>
											<case exp='1' >
												<flowline >
