											<exp exp='.DataOffset = .DataOffset &lt;&lt; 8' />
											<exp exp='.DataOffset = .DataOffset + .header[0x0A + 1]' />
											<exp exp='.DataOffset = .DataOffset &lt;&lt; 8' />
											<exp exp='.DataOffset = .DataOffset + .header[0x0A]' />
										</command>
										<command class_type='calculation' title='Read FileWidth' >
											<exp exp='.FileWidth = .Header[0x12 + 3]' />
											<exp exp='.FileWidth = .FileWidth &lt;&lt; 8' />
											<exp exp='.FileWidth = .FileWidth + .Header[0x12 + 2]' />
											<exp exp='.FileWidth = .FileWidth &lt;&lt; 8' />
											<exp exp='.FileWidth = .FileWidth + .Header[0x12 + 1]' />
											<exp exp='.FileWidth = .FileWidth &lt;&lt; 8' />
											<exp exp='.FileWidth = .FileWidth + .Header[0x12]' />
										</command>
										<command class_type='calculation' title='Read File Height' >
											<exp exp='.FileHeight = .Header[0x16 + 3]' />
											<exp exp='.FileHeight = .FileHeight &lt;&lt; 8' />
											<exp exp='.FileHeight = .FileHeight + .Header[0x16 + 2]' />
											<exp exp='.FileHeight = .FileHeight &lt;&lt; 8' />
											<exp exp='.FileHeight = .FileHeight + .Header[0x16 + 1]' />
											<exp exp='.FileHeight = .FileHeight &lt;&lt; 8' />
											<exp exp='.FileHeight = .FileHeight + .Header[0x16]' />
										</command>
										<command class_type='calculation' title='Read Pixel Data Format' >
											<exp exp='.ColourDepth = .Header[0x1C + 1]' />
											<exp exp='.ColourDepth = .ColourDepth &lt;&lt; 8' />
											<exp exp='.ColourDepth = .ColourDepth + .Header[0x1C]' />
										</command>
										<command class_type='calculation' title='Work out scaled step, &lt;1 = scaled up, &gt;1 = scaled down' >
											<exp exp='.sc_w = (FLOAT .FileWidth + 0.05) / FLOAT .Width' />
											<exp exp='.sc_h = (FLOAT .FileHeight + 0.05) / FLOAT .Height' />
										</command>
										<command class_type='decision' title='Read 8-bit Colour Pallette' exp='.ColourDepth = 8' swap='0' >
											<flowline >
												<command class_type='sim' title='Built-in Function' macro='File.SetPos' >
													<argument exp='.Filehandle' />
													<argument exp='54' />
													<argument exp='::File.Seek_Begin' />
												</command>
												<command class_type='sim' title='Read Header' macro='File.ReadArray' >
													<argument exp='.Filehandle' />
													<argument exp='.PalletBuffer' />
													<argument exp='1024' />
												</command>
											</flowline>
											<flowline >
												<command class_type='decision' title='Read 4-bit Colour Pallette' exp='.ColourDepth = 4' swap='0' >
													<flowline >
