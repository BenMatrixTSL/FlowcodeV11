				<macro >
					<flowline name='CalculateAltitudeOrg' description='Calculates the altitude based on difference between actual pressure and the pressure at the sea level P0=1013.25 hPa.
Formula used from Bosch sensor BMP180 Datasheet Page 16.
Altitude = 44330 * (1 - (pow ((P / P0),1 / 5.255)))' statediag='0' >
						<return name='Return' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<local name='P0' type='f32' description='The pressure at sea level 1013.25 hPa' isconst='1' isHidden='0' isinit='1' usrinit='1013.25' setinit='1013.25' />
						<local name='P' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<command class_type='comment' title='' comment='Removed fro API as does not work too well.
' textarea='12,0,12,0' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' />
						<command class_type='call' title='Call Macro' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='ReadPressureFloat' >
							<return exp='.P' />
						</command>
						<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
							<exp exp='.Return = 44330 * (1 - (pow ((.P / .P0),1 / 5.255)))' />
						</command>
					</flowline>
				</macro>
