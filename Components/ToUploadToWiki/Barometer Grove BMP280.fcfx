									<exp exp='.T3 = T3' />
								</command>
								<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='.adc_T = .adc_T &gt;&gt; 4' />
								</command>
								<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='Compensation = 1' swap='0' >
									<flowline >
										<command class_type='native' title='C Code' ccode='//Returns temperature in DegC, double precision. Output value of w51.23&quot; equals 51.23 DegC.
//t_fine carries fine temperature as global value
//BME280 Datasheet Page 49
#define BME280_S32_t MX_SINT32

double var1, var2, T;

var1 = (((double)FCL_ADC_T)/16384.0 - ((double)FCL_T1)/1024.0) * ((double)FCL_T2);
var2 = ((((double)FCL_ADC_T)/131072.0 - ((double)FCL_T1)/8192.0) *
(((double)FCL_ADC_T)/131072.0 - ((double) FCL_T1)/8192.0)) * ((double)FCL_T3);
FCL_TFINE = (BME280_S32_t)(var1 + var2);
T = (var1 + var2) / 5120.0;' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' language='C' />
										<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
											<exp exp='TFINE = .TFINE' />
										</command>
										<command class_type='native' title='C Code' ccode='return T;' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' language='C' />
									</flowline>
									<flowline >
										<command class_type='native' title='C Code' ccode='//Returns temperature in DegC, resolution is 0.01 DegC. Output value of &quot;5123&quot; equals 51.23 DegC.
//t_fine carries fine temperature as global value
//BME280 Datasheet Page 23 or Page 50
#define BME280_S32_t MX_SINT32
             
BME280_S32_t var1, var2, T;

var1 = ((((FCL_ADC_T&gt;&gt;3) - ((BME280_S32_t)FCL_T1 &lt;&lt;1))) * ((BME280_S32_t)FCL_T2)) &gt;&gt; 11;           
var2 = (((((FCL_ADC_T&gt;&gt;4) - ((BME280_S32_t)FCL_T1)) * ((FCL_ADC_T&gt;&gt;4) - ((BME280_S32_t)FCL_T1))) &gt;&gt; 12) * ((BME280_S32_t)FCL_T3)) &gt;&gt; 14;
FCL_TFINE = var1 + var2;
T = (FCL_TFINE * 5 + 128) &gt;&gt; 8;' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' language='C' />
										<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
											<exp exp='TFINE = .TFINE' />
										</command>
										<command class_type='native' title='C Code' ccode='//return T;
FCR_RETVAL = T/100.0;' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' language='C' />
									</flowline>
								</command>
							</flowline>
							<flowline >
								<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
