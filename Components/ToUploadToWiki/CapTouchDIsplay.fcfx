							<exp exp='.Return = 0' />
						</command>
					</flowline>
				</macro>
				<macro >
					<flowline name='SendCommand' description='' statediag='0' >
						<return name='Return' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='RxDone' type='u8' description='' isconst='0' isHidden='0' isinit='1' usrinit='0' setinit='0' />
						<local name='Rx' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='RxMode' type='u8' description='' isconst='0' isHidden='0' isinit='1' usrinit='0' setinit='0' />
						<local name='Retry' type='u8' description='' isconst='0' isHidden='0' isinit='1' usrinit='0' setinit='0' />
						<local name='tout' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<command class_type='comment' title='' comment='Send command packet' textarea='12,0,12,0' />
						<command class_type='call' title='Command Start Byte' component='cal_uart1' macro='Send' >
							<argument exp='&apos;:&apos;' />
						</command>
						<command class_type='call' title='Send Command Data' component='cal_uart1' macro='SendByteArray' >
							<argument exp='CommandTX' />
							<argument exp='TXCount' />
						</command>
						<command class_type='call' title='Command End Byte 1' component='cal_uart1' macro='Send' >
							<argument exp='&apos;;&apos;' />
						</command>
						<command class_type='call' title='Command End Byte 2' component='cal_uart1' macro='Send' >
							<argument exp='&apos;\n&apos;' />
						</command>
						<command class_type='comment' title='' comment='Receive Command Reply' textarea='12,0,12,0' />
						<command class_type='call' title='Component Macro' component='RXCB' macro='FlushBuffer' />
						<command class_type='calculation' title='Calculation' >
							<exp exp='.tout = RxTimeout * 100' />
						</command>
						<command class_type='loop' title='Loop' type='0' exp='(.RxDone = 0) &amp;&amp; (.Retry &lt; RxRetryCount)' >
							<flowline >
								<command class_type='decision' title='Decision' exp='.RxMode = 0' swap='0' >
									<flowline >
										<command class_type='call' title='Component Macro' component='RXCB' macro='LookForValue' >
											<return exp='.Rx' />
											<argument exp='&quot;:&quot;' />
											<argument exp='1' />
											<argument exp='1' />
											<argument exp='1' />
										</command>
										<command class_type='decision' title='Start Of Command Received?' exp='.Rx' swap='0' >
											<flowline >
												<command class_type='calculation' title='Calculation' >
													<exp exp='.RxMode = 1' />
												</command>
											</flowline>
											<flowline >
												<command class_type='decision' title='Decision' exp='.tout = 0' swap='0' >
													<flowline >
														<command class_type='calculation' title='Calculation' >
															<exp exp='.tout = RxTimeout * 100' />
														</command>
														<command class_type='calculation' title='Calculation' >
															<exp exp='.Retry = .Retry + 1' />
														</command>
													</flowline>
													<flowline >
														<command class_type='delay' title='Delay' exp='10' type='0' />
													</flowline>
												</command>
											</flowline>
										</command>
									</flowline>
									<flowline />
								</command>
								<command class_type='decision' title='Decision' exp='.RxMode = 1' swap='0' >
									<flowline >
										<command class_type='call' title='Component Macro' component='RXCB' macro='LookForValue' >
											<return exp='.Rx' />
											<argument exp='&quot;;\n&quot;' />
											<argument exp='2' />
											<argument exp='0' />
											<argument exp='1' />
										</command>
										<command class_type='decision' title='End Of Command Received?' exp='.Rx' swap='0' >
											<flowline >
												<command class_type='calculation' title='Calculation' >
													<exp exp='.RxDone = 1' />
												</command>
											</flowline>
											<flowline >
												<command class_type='decision' title='Decision' exp='.tout = 0' swap='0' >
													<flowline >
														<command class_type='calculation' title='Calculation' >
															<exp exp='.tout = RxTimeout * 100' />
														</command>
														<command class_type='calculation' title='Calculation' >
															<exp exp='.Retry = .Retry + 1' />
														</command>
													</flowline>
													<flowline >
														<command class_type='delay' title='Delay' exp='10' type='0' />
													</flowline>
												</command>
											</flowline>
										</command>
									</flowline>
									<flowline />
								</command>
								<command class_type='calculation' title='Calculation' >
									<exp exp='.tout = .tout - 1' />
								</command>
							</flowline>
						</command>
						<command class_type='calculation' title='Calculation' >
							<exp exp='.Return = 0' />
						</command>
						<command class_type='decision' title='Decision' exp='.RxDone' swap='0' >
							<flowline >
								<command class_type='call' title='Component Macro' component='RXCB' macro='GetByte' >
									<return exp='.Rx' />
								</command>
								<command class_type='decision' title='Reply matches command?' exp='.Rx = CommandTX[0]' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='.Return = 1' />
										</command>
									</flowline>
									<flowline />
								</command>
							</flowline>
							<flowline />
						</command>
					</flowline>
				</macro>
