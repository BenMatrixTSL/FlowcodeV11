																<command class_type='decision' title='Decision' exp='.Orientation' swap='0' >
																	<flowline >
																		<command class_type='call' title='Call Component Macro' component='Glcd_Col_Dummy1' macro='Window' >
																			<argument exp='.X + .h' />
																			<argument exp='.Y' />
																			<argument exp='.X + .h' />
																			<argument exp='.Y + .Width - 1' />
																		</command>
																	</flowline>
																	<flowline >
																		<command class_type='call' title='Call Component Macro' component='Glcd_Col_Dummy1' macro='Window' >
																			<argument exp='.X' />
																			<argument exp='.Y + .h' />
																			<argument exp='.X + .Width - 1' />
																			<argument exp='.Y + .h' />
																		</command>
																	</flowline>
																</command>
															</flowline>
														</command>
													</flowline>
													<flowline />
												</command>
											</flowline>
											<flowline />
										</command>
										<command class_type='calculation' title='Reset Row Idx' textarea='36,0,511,300' >
											<exp exp='.w = .WStart' />
										</command>
										<command class_type='calculation' title='Calculation' >
											<exp exp='.wacc = 1.0' />
											<exp exp='.count = 0' />
										</command>
										<command class_type='call' title='Jump to start of row' textarea='36,0,36,47' component='FATDummy1' macro='ReadByteFromIndex' >
											<argument exp='(DataOffset - 1) + .Address' />
										</command>
										<command class_type='calculation' title='Calculation' >
											<exp exp='.Address_Save = .Address' />
										</command>
										<command class_type='loop' title='Read and Print a row' textarea='36,0,511,300' type='0' exp='.w != .WStop' >
											<flowline >
												<command class_type='loop' title='read or skip until we hit the next valid pixel of the source' type='0' exp='.wacc &gt;= 1.0' >
													<flowline >
														<command class_type='switch' title='Switch' textarea='36,25,36,25' exp='ColourDepth' >
															<case >
																<flowline />
															</case>
															<case exp='1' >
																<flowline >
																	<command class_type='decision' title='Decision' exp='(.count &amp; 0x07) = 0' swap='0' >
																		<flowline >
																			<command class_type='call' title='Read Colour Data Byte' textarea='36,0,36,47' component='FATDummy1' macro='ReadByteFromFile' >
																				<return exp='.G' />
																			</command>
																			<command class_type='calculation' title='Calculation' >
																				<exp exp='.Address = .Address + 1' />
																			</command>
																		</flowline>
																		<flowline />
																	</command>
																	<command class_type='calculation' title='Calculate Bit Mask' >
																		<exp exp='.R = 0x80 &gt;&gt; (.count &amp; 0x07)' />
																	</command>
																	<command class_type='calculation' title='Mask Off Pixel Data' >
																		<exp exp='.B = .G &amp; .R' />
																	</command>
																	<command class_type='decision' title='Decision' exp='.B' swap='0' >
																		<flowline >
																			<command class_type='calculation' title='Calculation' >
																				<exp exp='.R = 255' />
																				<exp exp='.G = 255' />
																				<exp exp='.B = 255' />
																			</command>
																		</flowline>
																		<flowline >
																			<command class_type='calculation' title='Calculation' >
																				<exp exp='.R = 0' />
																				<exp exp='.G = 0' />
																				<exp exp='.B = 0' />
																			</command>
																		</flowline>
																	</command>
																</flowline>
															</case>
															<case exp='4' >
																<flowline >
																	<command class_type='decision' title='Decision' exp='UsePalletBuffer' swap='0' >
																		<flowline >
																			<command class_type='decision' title='Decision' exp='(.count &amp; 0x01) = 0' swap='0' >
																				<flowline >
																					<command class_type='call' title='Read colour index Data Byte' textarea='36,0,36,47' component='FATDummy1' macro='ReadByteFromFile' >
																						<return exp='.ColIdx' />
																					</command>
																					<command class_type='calculation' title='Calculation' >
																						<exp exp='.Address = .Address + 1' />
																					</command>
																					<command class_type='calculation' title='Calculation' >
																						<exp exp='.MaskedColIdx = (.ColIdx &amp; 0xF0) &gt;&gt; 4' />
																					</command>
																				</flowline>
																				<flowline >
																					<command class_type='calculation' title='Calculation' >
																						<exp exp='.MaskedColIdx = .ColIdx &amp; 0x0F' />
																					</command>
																				</flowline>
																			</command>
																			<command class_type='calculation' title='Skip to correct colour location and skip transparancy byte' >
																				<exp exp='.MaskedColIdx = .MaskedColIdx * 3' />
																			</command>
																			<command class_type='calculation' title='Read pallet data' >
																				<exp exp='.B = PalletBuffer[.MaskedColIdx]' />
																				<exp exp='.G = PalletBuffer[.MaskedColIdx + 1]' />
																				<exp exp='.R = PalletBuffer[.MaskedColIdx + 2]' />
																			</command>
																		</flowline>
																		<flowline />
																	</command>
																</flowline>
															</case>
															<case exp='8' >
																<flowline >
																	<command class_type='decision' title='Decision' exp='UsePalletBuffer' swap='0' >
																		<flowline >
																			<command class_type='call' title='Read colour index Data Byte' textarea='36,0,36,47' component='FATDummy1' macro='ReadByteFromFile' >
																				<return exp='.ColIdx' />
																			</command>
																			<command class_type='calculation' title='Calculation' >
																				<exp exp='.Address = .Address + 1' />
																			</command>
																			<command class_type='calculation' title='Skip to correct colour location and skip transparancy byte' >
																				<exp exp='.ColIdx = .ColIdx * 3' />
																			</command>
																			<command class_type='calculation' title='Read pallet data' >
																				<exp exp='.B = PalletBuffer[.ColIdx]' />
																				<exp exp='.G = PalletBuffer[.ColIdx + 1]' />
																				<exp exp='.R = PalletBuffer[.ColIdx + 2]' />
																			</command>
																		</flowline>
																		<flowline />
																	</command>
																</flowline>
															</case>
															<case exp='24' >
																<flowline >
																	<command class_type='call' title='Read Blue Data Byte' textarea='36,0,36,47' component='FATDummy1' macro='ReadByteFromFile' >
																		<return exp='.B' />
																	</command>
																	<command class_type='call' title='Read Green Data Byte' textarea='36,0,36,47' component='FATDummy1' macro='ReadByteFromFile' >
																		<return exp='.G' />
																	</command>
																	<command class_type='call' title='Read Red Data Byte' textarea='36,0,36,47' component='FATDummy1' macro='ReadByteFromFile' >
																		<return exp='.R' />
																	</command>
																	<command class_type='calculation' title='Calculation' >
																		<exp exp='.Address = .Address + 3' />
																	</command>
																</flowline>
															</case>
														</command>
														<command class_type='calculation' title='Decrement the width accumulator' >
															<exp exp='.wacc = .wacc - 1.0' />
														</command>
													</flowline>
												</command>
												<command class_type='decision' title='Decision' exp='Colour_Display = 1' swap='0' >
													<flowline >
														<command class_type='decision' title='Decision' exp='WSLED = 1' swap='0' >
															<flowline />
															<flowline >
																<command class_type='call' title='Set Pixel Colour' component='Glcd_Col_Dummy1' macro='SetForegroundColour' >
																	<argument exp='.R' />
																	<argument exp='.G' />
																	<argument exp='.B' />
																</command>
																<command class_type='call' title='Fast Write Pixel To Display' component='Glcd_Col_Dummy1' macro='FastPlot' />
															</flowline>
														</command>
													</flowline>
													<flowline >
														<command class_type='decision' title='Decision' exp='(.R &gt; 128) || (.G &gt; 128) || (.B &gt; 128)' swap='0' >
															<flowline >
																<command class_type='calculation' title='Calculation' >
																	<exp exp='.Mono = 1' />
																</command>
															</flowline>
															<flowline >
																<command class_type='calculation' title='Calculation' >
																	<exp exp='.Mono = 0' />
																</command>
															</flowline>
														</command>
													</flowline>
												</command>
												<command class_type='calculation' title='Move to next pixel in row' textarea='36,0,511,300' >
													<exp exp='.w = .w + .Winc' />
												</command>
												<command class_type='calculation' title='Calculation' >
													<exp exp='.count = .count + 1' />
												</command>
												<command class_type='calculation' title='Calculation' >
													<exp exp='.wacc = .wacc + .sc_w' />
												</command>
											</flowline>
										</command>
										<command class_type='calculation' title='Calculation' >
											<exp exp='.hacc = .hacc + .sc_h' />
										</command>
										<command class_type='calculation' title='Jump back to beginning of row' >
											<exp exp='.Address = .Address_Save' />
										</command>
										<command class_type='decision' title='Decision' exp='.hacc &gt;= 1.0' swap='0' >
											<flowline >
												<command class_type='loop' title='Loop' type='0' exp='.hacc &gt;= 1.0' >
													<flowline >
														<command class_type='switch' title='Switch' textarea='36,25,36,25' exp='ColourDepth' >
															<case >
																<flowline />
															</case>
															<case exp='1' >
																<flowline >
																	<command class_type='calculation' title='Calculation' >
																		<exp exp='.Address = .Address + (FileWidth &gt;&gt; 3)' />
																	</command>
																	<command class_type='decision' title='If we are partially through a data byte' exp='(.count &amp; 0x07) &gt; 0' swap='0' >
																		<flowline >
																			<command class_type='calculation' title='Calculation' >
																				<exp exp='.Address = .Address + 1' />
																			</command>
																		</flowline>
																		<flowline />
																	</command>
																</flowline>
															</case>
															<case exp='4' >
																<flowline >
																	<command class_type='calculation' title='Calculation' >
																		<exp exp='.Address = .Address + (FileWidth &gt;&gt; 1)' />
																	</command>
																	<command class_type='decision' title='If half way through a data byte' exp='(.count &amp; 0x01) = 1' swap='0' >
																		<flowline >
																			<command class_type='calculation' title='Calculation' >
																				<exp exp='.Address = .Address + 1' />
																			</command>
																		</flowline>
																		<flowline />
																	</command>
																</flowline>
															</case>
															<case exp='8' >
																<flowline >
																	<command class_type='calculation' title='Calculation' >
																		<exp exp='.Address = .Address + FileWidth' />
																	</command>
																</flowline>
															</case>
															<case exp='24' >
																<flowline >
																	<command class_type='calculation' title='Calculation' >
																		<exp exp='.Address = .Address + (FileWidth * 3)' />
																	</command>
																</flowline>
															</case>
														</command>
														<command class_type='loop' title='Loop to next 4 byte offset' type='0' exp='.Address &amp; 0x03' >
															<flowline >
																<command class_type='calculation' title='Calculation' >
																	<exp exp='.Address = .Address + 1' />
																</command>
															</flowline>
														</command>
														<command class_type='calculation' title='Calculation' >
															<exp exp='.hacc = .hacc - 1.0' />
														</command>
													</flowline>
												</command>
											</flowline>
											<flowline />
										</command>
										<command class_type='calculation' title='Move to next row' textarea='36,0,511,300' >
											<exp exp='.h = .h + .Hinc' />
										</command>
										<command class_type='sim' title='Component Macro' component='LibraryComponent1' macro='FlashTx' />
									</flowline>
								</command>
							</flowline>
							<flowline />
						</command>
						<command class_type='call' title='Component Macro' component='FATDummy1' macro='CloseFile' />
						<command class_type='decision' title='Decision' exp='IsSim' swap='0' >
							<flowline >
								<command class_type='sim' title='Built-in Function' macro='Component.Property.SetValue' >
									<argument exp='.Handle' />
									<argument exp='&quot;AutoRefresh&quot;' />
									<argument exp='1' />
								</command>
							</flowline>
							<flowline />
						</command>
					</flowline>
				</macro>
				<macro >
