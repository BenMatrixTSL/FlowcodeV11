				<macro >
					<flowline name='Read_4_Byte_Packet' description='' statediag='0' >
						<return name='Return' type='u32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='idx' type='s16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='dat0' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='dat1' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='dat2' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='dat3' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='Data' type='T8' description='' isconst='0' isHidden='0' isinit='0' usrinit='&quot;&quot;' setinit='' >
							<array size='2' />
						</local>
						<command class_type='call' title='Call Component Macro' component='FATDummy1' macro='ReadByteFromIndex' >
							<return exp='.dat0' />
							<argument exp='.idx' />
						</command>
						<command class_type='call' title='Call Component Macro' component='FATDummy1' macro='ReadByteFromIndex' >
							<return exp='.dat1' />
							<argument exp='.idx + 1' />
						</command>
						<command class_type='call' title='Call Component Macro' component='FATDummy1' macro='ReadByteFromIndex' >
							<return exp='.dat2' />
							<argument exp='.idx + 2' />
						</command>
						<command class_type='call' title='Call Component Macro' component='FATDummy1' macro='ReadByteFromIndex' >
							<return exp='.dat3' />
							<argument exp='.idx + 3' />
						</command>
						<command class_type='calculation' title='Position bytes into long variable' >
							<exp exp='.Return = .dat3' />
							<exp exp='.Return = .Return &lt;&lt; 8' />
							<exp exp='.Return = .Return | .dat2' />
							<exp exp='.Return = .Return &lt;&lt; 8' />
							<exp exp='.Return = .Return | .dat1' />
							<exp exp='.Return = .Return &lt;&lt; 8' />
							<exp exp='.Return = .Return | .dat0' />
						</command>
						<command class_type='sim' title='Component Macro' component='LibraryComponent1' macro='FlashRx' />
					</flowline>
				</macro>
				<macro >
					<flowline name='ParseBitmapFile' description='' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
						<local name='TwoByteData' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='Idx' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='Data' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='RamIdx' type='u16' description='' isconst='0' isHidden='0' isinit='1' usrinit='0' setinit='0' />
						<command class_type='call' title='Read Header Field' macro='Read_2_Byte_Packet' >
							<return exp='.TwoByteData' />
							<argument exp='0' />
						</command>
						<command class_type='decision' title='Valid Bitmap file header?' exp='.TwoByteData = ((&apos;M&apos; &lt;&lt; 8) | &apos;B&apos;)' swap='0' >
							<flowline >
								<command class_type='call' title='Read Data Offset' macro='Read_4_Byte_Packet' >
									<return exp='DataOffset' />
									<argument exp='0x0A' />
								</command>
								<command class_type='call' title='Read File Width' macro='Read_4_Byte_Packet' >
									<return exp='FileWidth' />
									<argument exp='0x12' />
								</command>
								<command class_type='call' title='Read File Height' macro='Read_4_Byte_Packet' >
									<return exp='FileHeight' />
									<argument exp='0x16' />
								</command>
								<command class_type='call' title='Read pixel data format' macro='Read_2_Byte_Packet' >
									<return exp='ColourDepth' />
									<argument exp='0x1C' />
								</command>
							</flowline>
							<flowline />
						</command>
						<command class_type='decision' title='Decision' exp='UsePalletBuffer &amp;&amp; ((ColourDepth = 4) || (ColourDepth = 8))' swap='0' >
							<flowline >
								<command class_type='call' title='Jump to byte just before the pallet starts' textarea='36,0,36,47' component='FATDummy1' macro='ReadByteFromIndex' >
									<argument exp='PalletOffset - 1' />
								</command>
								<command class_type='decision' title='Decision' exp='ColourDepth = 8' swap='0' >
									<flowline >
										<command class_type='loop' title='Populate colour pallet RAM' type='2' forvar='.Idx' exp='1024' >
											<flowline >
												<command class_type='call' title='Read Pallet Data Byte' textarea='36,0,36,47' component='FATDummy1' macro='ReadByteFromFile' >
													<return exp='.Data' />
												</command>
												<command class_type='decision' title='Don&apos;t store every 4th byte - Transparancy and not used' exp='(.Idx &amp; 0x03) = 0x03' swap='0' >
													<flowline />
													<flowline >
														<command class_type='sim' title='Tested content - looks ok' disable='1' macro='Debug.PrintLn' >
															<argument exp='STRING .RamIdx + &quot; -&gt; &quot; + STRING .Data' />
														</command>
														<command class_type='calculation' title='Calculation' >
															<exp exp='PalletBuffer[.RamIdx] = .Data' />
														</command>
														<command class_type='calculation' title='Calculation' >
															<exp exp='.RamIdx = .RamIdx + 1' />
														</command>
													</flowline>
												</command>
											</flowline>
										</command>
									</flowline>
									<flowline >
										<command class_type='loop' title='Populate colour pallet RAM' type='2' forvar='.Idx' exp='64' >
											<flowline >
												<command class_type='call' title='Read Pallet Data Byte' textarea='36,0,36,47' component='FATDummy1' macro='ReadByteFromFile' >
													<return exp='.Data' />
												</command>
												<command class_type='decision' title='Don&apos;t store every 4th byte - Transparancy and not used' exp='(.Idx &amp; 0x03) = 0x03' swap='0' >
													<flowline />
													<flowline >
														<command class_type='calculation' title='Calculation' >
															<exp exp='PalletBuffer[.RamIdx] = .Data' />
														</command>
														<command class_type='calculation' title='Calculation' >
															<exp exp='.RamIdx = .RamIdx + 1' />
														</command>
													</flowline>
												</command>
											</flowline>
										</command>
									</flowline>
								</command>
							</flowline>
							<flowline />
						</command>
					</flowline>
				</macro>
				<macro >
					<flowline name='Ev_Property' description='Sent when a property has been changed by the User' statediag='0' >
						<return name='Return' type='u32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='Temp_Str' type='T8' description='' isconst='0' isHidden='0' isinit='0' usrinit='&quot;&quot;' setinit='' >
							<array size='20' />
						</local>
						<local name='Count' type='u32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='GLCDHandle' type='h32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='FATHandle' type='h32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<command class_type='sim' title='Component Macro' component='LibraryComponent1' macro='GetLinkHandle' >
							<return exp='.FATHandle' />
							<argument exp='1' />
