									<argument exp='.Dst_IP0' />
								</command>
								<command class_type='call' title='Set up the destination IP address' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='MAC_WriteByte' >
									<argument exp='.dipr + 1' />
									<argument exp='.Dst_IP1' />
								</command>
								<command class_type='call' title='Set up the destination IP address' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='MAC_WriteByte' >
									<argument exp='.dipr + 2' />
									<argument exp='.Dst_IP2' />
								</command>
								<command class_type='call' title='Set up the destination IP address' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='MAC_WriteByte' >
									<argument exp='.dipr + 3' />
									<argument exp='.Dst_IP3' />
								</command>
								<command class_type='call' title='Connect' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='MAC_WriteByte' >
