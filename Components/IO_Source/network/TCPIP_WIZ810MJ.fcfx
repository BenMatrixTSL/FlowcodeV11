								</command>
								<command class_type='call' title='Call Macro' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='MAC_WriteByte' >
									<argument exp='.dipr + 2' />
									<argument exp='.Dst_IP2' />
								</command>
								<command class_type='call' title='Call Macro' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='MAC_WriteByte' >
									<argument exp='.dipr + 3' />
									<argument exp='.Dst_IP3' />
								</command>
								<command class_type='call' title='Set Port' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='MAC_WriteByte' >
									<argument exp='.dport' />
									<argument exp='.Dst_Port &gt;&gt; 8' />
								</command>
								<command class_type='call' title='Set Port' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='MAC_WriteByte' >
									<argument exp='.dport + 1' />
									<argument exp='.Dst_Port' />
								</command>
								<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='.Return = 1' />
								</command>
							</flowline>
						</command>
					</flowline>
