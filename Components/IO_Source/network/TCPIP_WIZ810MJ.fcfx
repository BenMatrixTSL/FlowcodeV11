							<exp exp='.dport = .cr + 15' />
						</command>
						<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='.Channel &gt;= 4' swap='0' >
							<flowline >
