											</flowline>
										</command>
									</flowline>
								</command>
								<command class_type='calculation' title='Increment string pointer' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' >
