				<macro >
					<flowline name='WaitUntilLow' description='Waits until the switch is in state &apos;low&apos;
The interpretation of &apos;high&apos; depends on the polarity' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
						<local name='switchval' type='u8' description='The state of the pin' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='del_count' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='old_switchval' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='0' cmdgradient='4' cmdopacity='0.64' exp='Enabled' swap='0' >
							<flowline >
								<command class_type='loop' title='While state active' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='0' cmdgradient='4' cmdopacity='0.64' type='0' exp='.switchval != polarity' start='0' >
									<flowline >
										<command class_type='calculation' title='Set the entry condition' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
											<exp exp='.switchval = pin' />
										</command>
										<command class_type='decision' title='Invert logic if required' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='0' cmdgradient='4' cmdopacity='0.64' exp='polarity = 1' swap='0' >
											<flowline >
												<command class_type='calculation' title='Set the entry condition' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
													<exp exp='.switchval = ! .switchval' />
												</command>
											</flowline>
											<flowline />
										</command>
										<command class_type='calculation' title='Init debounce state' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
											<exp exp='.del_count = 0' />
											<exp exp='.old_switchval = .switchval' />
										</command>
										<command class_type='decision' title='If debounce enabled' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='0' cmdgradient='4' cmdopacity='0.64' exp='debounce &gt; 0' swap='0' >
											<flowline >
												<command class_type='loop' title='Debounce input' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='0' cmdgradient='4' cmdopacity='0.64' type='0' exp='.del_count &lt; debounce' >
													<flowline >
														<command class_type='delay' title='Delay' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='1' type='1' />
														<command class_type='calculation' title='Sample input' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
															<exp exp='.switchval = pin' />
														</command>
														<command class_type='decision' title='Invert logic if required' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='0' cmdgradient='4' cmdopacity='0.64' exp='polarity == 1' swap='0' >
															<flowline >
																<command class_type='calculation' title='Invert' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
																	<exp exp='.switchval = ! .switchval' />
																</command>
															</flowline>
															<flowline />
														</command>
														<command class_type='decision' title='Is switch the same state?' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='0' cmdgradient='4' cmdopacity='0.64' exp='.switchval == .old_switchval' swap='0' >
															<flowline >
																<command class_type='calculation' title='Increment counter' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
																	<exp exp='.del_count = .del_count + 1' />
																</command>
															</flowline>
															<flowline >
																<command class_type='calculation' title='Else reset counter' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
																	<exp exp='.del_count = 0' />
																</command>
															</flowline>
														</command>
														<command class_type='calculation' title='Update the old state' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
															<exp exp='.old_switchval = .switchval' />
														</command>
													</flowline>
												</command>
											</flowline>
											<flowline />
										</command>
									</flowline>
								</command>
							</flowline>
							<flowline />
						</command>
					</flowline>
				</macro>
