								<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='.f1 = (.f1 * .f1)' />
									<exp exp='.f2 = (.f2 * .f2)' />
									<exp exp='.f1 = (.f1 + .f2)' />
								</command>
							</flowline>
							<flowline >
								<command class_type='call' title='Component Macro' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' component='Buff1' macro='get' >
