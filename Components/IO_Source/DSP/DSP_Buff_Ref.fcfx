						<command class_type='comment' title='' comment='First do some clean up delete generated line component if there is one' textarea='12,0,12,0' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' />
