				<macro >
					<flowline name='Prv_DrawArc' description='Draw the arc' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
						<param name='Start' type='f32' description='Start angle of the arc' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<param name='End' type='f32' description='End angle of the arc' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<param name='Radius' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<local name='segments' type='s16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='sweep' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<local name='index' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='step' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<local name='x0' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<local name='x1' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<local name='y0' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<local name='y1' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<local name='angle' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<local name='scale_rad' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<command class_type='sim' title='Simulation' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='Panel.Graphics.SetLineThickness' >
							<argument exp='arc_thick' />
						</command>
						<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
							<exp exp='.sweep = .End - .Start' />
							<exp exp='.segments = ceil (12 * fabs (.sweep))' />
							<exp exp='.step = .sweep / .segments' />
							<exp exp='.angle = .Start' />
							<exp exp='.index = 0' />
							<exp exp='.x0 = px_x_centre + cos (.angle) * .Radius' />
							<exp exp='.y0 = px_y_centre + sin (.angle) * .Radius' />
						</command>
						<command class_type='loop' title='Loop' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='0' cmdgradient='4' cmdopacity='0.64' type='0' exp='.index &lt; .segments' >
							<flowline >
								<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='.angle = .angle + .step' />
									<exp exp='.x1 = px_x_centre + cos (.angle) * .Radius' />
									<exp exp='.y1 = px_y_centre + sin (.angle) * .Radius' />
								</command>
								<command class_type='sim' title='Panel' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='Panel.Graphics.DrawLineA' >
									<argument exp='.x0' />
									<argument exp='.y0' />
									<argument exp='.x1' />
									<argument exp='.y1' />
								</command>
								<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='.x0 = .x1' />
									<exp exp='.y0 = .y1' />
									<exp exp='.index = .index + 1' />
								</command>
							</flowline>
						</command>
					</flowline>
				</macro>
