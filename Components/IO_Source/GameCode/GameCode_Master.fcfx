								</command>
								<command class_type='call' title='CMD' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' component='UART_MP3' macro='Send' >
									<argument exp='.command' />
								</command>
								<command class_type='call' title='feedback' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' component='UART_MP3' macro='Send' >
									<argument exp='0x00' />
								</command>
								<command class_type='call' title='parahigh' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' component='UART_MP3' macro='Send' >
									<argument exp='.Para1_highbyte' />
								</command>
