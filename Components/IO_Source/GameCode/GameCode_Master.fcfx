									<exp exp='.checksum = .versioninfo + .numberofbytes + .command + 0x00 + .Para1_highbyte + .Para2_lowbyte' />
									<exp exp='.checksum = 0 - .checksum' />
									<exp exp='' />
									<exp exp='.checksum_low = .checksum &amp; 0xFF' />
									<exp exp='.checksum_high = (.checksum &amp; 0xFF00) &gt;&gt; 8' />
								</command>
