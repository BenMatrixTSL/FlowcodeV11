						<command class_type='native' title='Javascript' ccode='  if (&quot;serial&quot; in navigator)
  {
    try
    {
      if ((window[&apos;FCPORT_&apos; + FC_THIS] != null) &amp;&amp; (window[&apos;FCPORT_&apos; + FC_THIS].writable != null))
      {
        //close the port
        if (window[&apos;FCRX_&apos; + FC_THIS] != null)
        {
          window[&apos;FCRX_&apos; + FC_THIS].releaseLock();
        }
        window[&apos;FCPORT_&apos; + FC_THIS].close();
      }
      
      //Prompt the user to select a COM port
      const filter = window[&apos;FCFLTR_&apos; + FC_THIS];
      window[&apos;FCPORT_&apos; + FC_THIS] = await navigator.serial.requestPort({ filters: filter });
      const { usbProductId, usbVendorId } = window[&apos;FCPORT_&apos; + FC_THIS].getInfo();
      
      //create the return in the format 0xVVVVPPPP
      var vendor = Number(usbVendorId).toString(16);
      var product = Number(usbProductId).toString(16);
      while (vendor.length &lt; 4)
      {
        vendor = &quot;0&quot; + vendor;
      }
      while (product.length &lt; 4)
      {
        product = &quot;0&quot; + product;
      }
      FCR_RETVAL = &quot;0x&quot; + vendor + product;
    }
    catch (e)
    {
      if (e instanceof DOMException)
      {
        console.log(&quot;No port selected by the user&quot;);
      }
      else
      {
        console.log(&quot;Unknown error&quot;);
      }
    }
  }
  else
  {
    console.log(&quot;Serial not available!&quot;);
  }' customize='1' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' language='C' />
