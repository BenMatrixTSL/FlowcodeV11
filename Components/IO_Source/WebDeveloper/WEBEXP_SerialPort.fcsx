						<command class_type='native' title='JavaScript' ccode='  if (&quot;serial&quot; in navigator)
  {
    if ((window[&apos;FCPORT_&apos; + FC_THIS] != null) &amp;&amp; (window[&apos;FCPORT_&apos; + FC_THIS].readable != null))
    {
      let done = false;
      FCR_RETVAL = &quot;&quot;;
      const bufferSize = 1;

      window[&apos;FCRX_&apos; + FC_THIS] = window[&apos;FCPORT_&apos; + FC_THIS].readable.getReader({ mode: &quot;byob&quot; });

      while (!done)
      {
        const timer = setTimeout(() =&gt; { window[&apos;FCRX_&apos; + FC_THIS].releaseLock(); }, FCL_TIMEOUT);
        let result = {done: false, value: null};
        let buffer = new ArrayBuffer(bufferSize);
        try
        {
          result = await window[&apos;FCRX_&apos; + FC_THIS].read(new Uint8Array(buffer));
          buffer = result.buffer;
        }
        catch (e)
        {
          done = true;
          if (e instanceof TypeError)
          {
            console.log(&quot;Timeout error occurred!&quot;);
          }
          else
          {
            throw(e);
          }
        }
        if (result.value != null)
        {
          const decoder = new TextDecoder();
          FCR_RETVAL = FCR_RETVAL + decoder.decode(result.value);
        
          clearTimeout(timer);
          
          if (FCR_RETVAL.length &gt;= FCL_NUMCHARS)
          {
            done = true;
          }
        }
      }
      
      window[&apos;FCRX_&apos; + FC_THIS].releaseLock();
    }
    else
    {
      console.log(&quot;Not connected!&quot;);
    }
  }' customize='1' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' language='C' />
