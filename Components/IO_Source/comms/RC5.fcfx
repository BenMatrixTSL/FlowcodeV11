													<exp exp='tx_pin = 0' />
												</command>
												<command class_type='native' title='C Code' ccode='delay_10us(FCL_PTIM);' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' language='C' />
