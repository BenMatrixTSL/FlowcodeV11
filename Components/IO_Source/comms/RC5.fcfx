															<exp exp='tx_pin = 1' />
														</command>
														<command class_type='native' title='C Code' ccode='delay_us(FCL_HTIM);' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' language='C' />
														<command class_type='calculation' title='Calculation' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' >
															<exp exp='tx_pin = 0' />
														</command>
														<command class_type='native' title='C Code' ccode='delay_us(FCL_HTIM);' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' language='C' />
													</flowline>
												</command>
											</flowline>
											<flowline >
												<command class_type='loop' title='Loop' cmdcolor='16098537' cmdcolor_sec='9519237' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' type='2' exp='LoopCount' >
													<flowline >
