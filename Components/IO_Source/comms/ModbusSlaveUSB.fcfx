											</flowline>
											<flowline >
												<command class_type='calculation' title='coil count fits into bytes exactly' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
													<exp exp='.Elements = NumAdrCoil / 8' />
