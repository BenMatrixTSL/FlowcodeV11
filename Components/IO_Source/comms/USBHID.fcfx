							<exp exp='.idx = 0' />
						</command>
						<command class_type='loop' title='Look for closing &quot;' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='0' cmdgradient='4' cmdopacity='0.64' type='0' exp='.input[.idx2] != 34' >
							<flowline >
