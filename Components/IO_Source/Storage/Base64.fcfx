			<macros >
				<macro >
					<flowline name='Encode' description='Encodes an array of bytes into a base64 encoded string' statediag='0' >
						<return name='Return' type='T8' description='' isconst='0' isHidden='0' isinit='0' usrinit='&quot;&quot;' setinit='' >
							<array size='2000' />
						</return>
						<param name='BinaryData' type='u8' description='Raw binary data array you wish to encode' isconst='0' isHidden='0' isinit='0' usrinit='&quot;&quot;' setinit='' >
							<array size='10' />
						</param>
						<param name='ByteCount' type='u16' description='Number of bytes of data to encode' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='Data' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='&quot;&quot;' setinit='' >
							<array size='3' />
						</local>
						<local name='idx' type='u16' description='' isconst='0' isHidden='0' isinit='1' usrinit='0' setinit='0' />
						<local name='EncData' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='&quot;&quot;' setinit='' >
							<array size='4' />
						</local>
						<local name='subidx' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='EncDataLen' type='u16' description='' isconst='0' isHidden='0' isinit='1' usrinit='0' setinit='0' />
						<command class_type='loop' title='Loop' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' type='0' exp='.idx &lt; .ByteCount' >
							<flowline >
								<command class_type='comment' title='' comment='Collect 3 Data bytes' textarea='12,0,12,0' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' />
								<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='.Data[0] = .BinaryData[.idx]' />
								</command>
								<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='(.idx + 1) &lt; .ByteCount' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
											<exp exp='.Data[1] = .BinaryData[.idx + 1]' />
										</command>
									</flowline>
									<flowline >
										<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
											<exp exp='.Data[1] = 0' />
										</command>
									</flowline>
								</command>
								<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='(.idx + 2) &lt; .ByteCount' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
											<exp exp='.Data[2] = .BinaryData[.idx + 2]' />
										</command>
									</flowline>
									<flowline >
										<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
											<exp exp='.Data[2] = 0' />
										</command>
									</flowline>
								</command>
								<command class_type='comment' title='' comment='Convert 3 x 8-bit into 4 x 6 bit' textarea='12,0,12,0' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' />
								<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='.EncData[0] = (.Data[0] &amp; 0xFC) &gt;&gt; 2' />
									<exp exp='' />
									<exp exp='.EncData[1] = (.Data[0] &amp; 0x03) &lt;&lt; 4' />
									<exp exp='.EncData[1] = .EncData[1] | ((.Data[1] &amp; 0xF0) &gt;&gt; 4)' />
									<exp exp='' />
									<exp exp='.EncData[2] = (.Data[1] &amp; 0x0F) &lt;&lt; 2' />
									<exp exp='.EncData[2] = .EncData[2] | ((.Data[2] &amp; 0xC0) &gt;&gt; 6)' />
									<exp exp='' />
									<exp exp='.EncData[3] = .Data[2] &amp; 0x3F' />
								</command>
								<command class_type='comment' title='' comment='Convert 4 x 6 bit into Base64 encoded characters' textarea='12,0,12,0' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' />
								<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='.subidx = 0' />
								</command>
								<command class_type='loop' title='Loop' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' type='0' exp='.subidx &lt; 4' >
									<flowline >
										<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='(.EncData[.subidx] &gt;= 0) &amp;&amp; (.EncData[.subidx] &lt;= 25)' swap='0' >
											<flowline >
												<command class_type='calculation' title='0 - 25 = A - Z' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
													<exp exp='.EncData[.subidx] = .EncData[.subidx] + &apos;A&apos;' />
												</command>
											</flowline>
											<flowline >
												<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='(.EncData[.subidx] &gt;= 26) &amp;&amp; (.EncData[.subidx] &lt;= 51)' swap='0' >
													<flowline >
														<command class_type='calculation' title='26 - 51 = a - z' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
															<exp exp='.EncData[.subidx] = .EncData[.subidx] - 26 + &apos;a&apos;' />
														</command>
													</flowline>
													<flowline >
														<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='(.EncData[.subidx] &gt;= 52) &amp;&amp; (.EncData[.subidx] &lt;= 61)' swap='0' >
															<flowline >
																<command class_type='calculation' title='52 - 61 = 0 - 9' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
																	<exp exp='.EncData[.subidx] = .EncData[.subidx] - 52 + &apos;0&apos;' />
																</command>
															</flowline>
															<flowline >
																<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='.EncData[.subidx] = 62' swap='0' >
																	<flowline >
																		<command class_type='calculation' title='62 = +' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
																			<exp exp='.EncData[.subidx] = &apos;+&apos;' />
																		</command>
																	</flowline>
																	<flowline >
																		<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='.EncData[.subidx] = 63' swap='0' >
																			<flowline >
																				<command class_type='calculation' title='63 = /' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
																					<exp exp='.EncData[.subidx] = &apos;/&apos;' />
																				</command>
																			</flowline>
																			<flowline />
																		</command>
																	</flowline>
																</command>
															</flowline>
														</command>
													</flowline>
												</command>
											</flowline>
										</command>
										<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
											<exp exp='.subidx = .subidx + 1' />
										</command>
									</flowline>
								</command>
								<command class_type='comment' title='' comment='If we are at the end of the data then switch encoded chars for padding' textarea='12,0,12,0' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' />
								<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='(.idx + 2) &gt;= .ByteCount' swap='0' >
									<flowline >
										<command class_type='calculation' title='&apos;=&apos; = Padding' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
											<exp exp='.EncData[3] = &apos;=&apos;' />
										</command>
										<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='(.idx + 1) &gt;= .ByteCount' swap='0' >
											<flowline >
												<command class_type='calculation' title='&apos;=&apos; = Padding' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
													<exp exp='.EncData[2] = &apos;=&apos;' />
												</command>
											</flowline>
											<flowline />
										</command>
									</flowline>
									<flowline />
								</command>
								<command class_type='comment' title='' comment='Append Encoded Data to Buffer' textarea='12,0,12,0' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' />
								<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='.Return[.EncDataLen] = .EncData[0]' />
									<exp exp='.Return[.EncDataLen + 1] = .EncData[1]' />
									<exp exp='.Return[.EncDataLen + 2] = .EncData[2]' />
									<exp exp='.Return[.EncDataLen + 3] = .EncData[3]' />
