								<command class_type='loop' title='Loop' type='0' exp='.Idx &lt; .Length' >
									<flowline >
										<command class_type='decision' title='Decision' exp='.started = 0' swap='0' >
											<flowline >
												<command class_type='calculation' title='Calculation' >
													<exp exp='.HeadCode = .HeadCode + &quot;__EEPROM_DATA(&quot;' />
												</command>
												<command class_type='calculation' title='Calculation' >
													<exp exp='.started = 1' />
												</command>
											</flowline>
											<flowline />
										</command>
										<command class_type='decision' title='Decision' exp='InitValsParsed[.Idx] == &apos;,&apos;' swap='0' >
											<flowline >
