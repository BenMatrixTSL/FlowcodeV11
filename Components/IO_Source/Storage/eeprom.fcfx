														<command class_type='calculation' title='Calculation' >
															<exp exp='.SubStr[0] = InitValsParsed[.Idx]' />
															<exp exp='.SubStr[1] = 0' />
														</command>
														<command class_type='calculation' title='Calculation' >
															<exp exp='.HeadCode = .HeadCode + .SubStr' />
														</command>
													</flowline>
												</command>
											</flowline>
											<flowline >
												<command class_type='calculation' title='Calculation' >
													<exp exp='.SubStr[0] = InitValsParsed[.Idx]' />
													<exp exp='.SubStr[1] = 0' />
												</command>
												<command class_type='calculation' title='Calculation' >
													<exp exp='.HeadCode = .HeadCode + .SubStr' />
												</command>
											</flowline>
										</command>
										<command class_type='calculation' title='Calculation' >
											<exp exp='.Idx = .Idx + 1' />
										</command>
									</flowline>
								</command>
								<command class_type='decision' title='Decision' exp='.started = 1' swap='0' >
									<flowline >
										<command class_type='loop' title='Loop' type='0' exp='.CommaCount &lt; 7' >
											<flowline >
												<command class_type='calculation' title='Calculation' >
													<exp exp='.HeadCode = .HeadCode + &quot;,0xFF&quot;' />
												</command>
												<command class_type='calculation' title='Calculation' >
													<exp exp='.CommaCount = .CommaCount + 1' />
												</command>
											</flowline>
										</command>
										<command class_type='calculation' title='Calculation' >
											<exp exp='.HeadCode = .HeadCode + &quot;);\n&quot;' />
										</command>
									</flowline>
