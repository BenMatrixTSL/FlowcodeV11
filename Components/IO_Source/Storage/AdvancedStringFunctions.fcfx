															<exp exp='.StrData[.pos + .lenreplace + .idx] = .StrData[.pos + .lensearch + .idx]' />
														</command>
													</flowline>
												</command>
											</flowline>
											<flowline >
												<command class_type='calculation' title='Add new null termination' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
													<exp exp='.StrData[(.lenstr) + (.lenreplace - .lensearch)] = 0' />
												</command>
												<command class_type='loop' title='Shift remaining string data' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='0' cmdgradient='4' cmdopacity='0.64' type='2' forvar='.idx' exp='.lenstr - (.pos + .lensearch)' >
													<flowline >
														<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
															<exp exp='.StrData[(.lenstr - 1) + (.lenreplace - .lensearch) - .idx] = .StrData[(.lenstr - 1) - .idx]' />
														</command>
													</flowline>
												</command>
												<command class_type='loop' title='Overwite search with replace' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='0' cmdgradient='4' cmdopacity='0.64' type='2' forvar='.idx' exp='.lenreplace' >
													<flowline >
														<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
															<exp exp='.StrData[.pos + .idx] = .StrReplace[.idx]' />
														</command>
													</flowline>
												</command>
											</flowline>
										</command>
									</flowline>
								</command>
								<command class_type='call' title='Find next match' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='PositionOf' >
									<return exp='.pos' />
