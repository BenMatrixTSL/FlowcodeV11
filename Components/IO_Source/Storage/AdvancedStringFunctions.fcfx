						<command class_type='loop' title='Loop' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='0' cmdgradient='4' cmdopacity='0.64' type='0' exp='.pos &gt;= 0' >
							<flowline >
								<command class_type='decision' title='Lengths are the same, simple char swaps' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='0' cmdgradient='4' cmdopacity='0.64' exp='.lensearch = .lenreplace' swap='0' >
