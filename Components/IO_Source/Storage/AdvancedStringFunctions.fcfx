												</command>
												<command class_type='loop' title='Shift remaining string data' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='0' cmdgradient='4' cmdopacity='0.64' type='2' forvar='.idx' exp='.lenstr - (.pos + .lenreplace)' >
													<flowline >
