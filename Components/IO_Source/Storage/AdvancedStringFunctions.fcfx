										</command>
									</flowline>
									<flowline >
										<command class_type='decision' title='Lengths are different, manipulate remaining string' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='0' cmdgradient='4' cmdopacity='0.64' exp='.lensearch &gt; .lenreplace' swap='0' >
											<flowline >
												<command class_type='loop' title='Overwite search with replace' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='0' cmdgradient='4' cmdopacity='0.64' type='2' forvar='.idx' exp='.lenreplace' >
													<flowline >
