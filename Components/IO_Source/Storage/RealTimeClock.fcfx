				<macro >
					<flowline name='SQWandINTcontrol' description='Allows a square wave output to be generated by the DS1307 and DS3231 ICs.
Also allows the DS3231 interrupts to be configured via the INT/SQW pin.
This function doesn&apos;t currently simulate.' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
						<param name='SquareWaveEnable' type='u8' description='0 = Square Wave Output Disabled / 1 = Square Wave Output Enabled' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='FrequencySelect' type='u8' description='Controls the output frequency of the square wave, Range 0-3, Refer to the device datasheet for frequencies' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='InterruptEnable' type='u8' description='Only Available on DS3231 and when SQW is disabled. Range 0-3' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='data' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='&quot;&quot;' setinit='' >
							<array size='2' />
						</local>
						<command class_type='decision' title='Decision' exp='Simulation &amp;&amp; (SimComms == 0)' swap='0' >
							<flowline >
								<command class_type='comment' title='' comment='Simulation - Use DLL' textarea='12,0,12,0' />
							</flowline>
							<flowline >
								<command class_type='decision' title='DS1307' exp='RTCModule = 1' swap='0' >
									<flowline >
										<command class_type='calculation' title='Address' >
											<exp exp='.Data[0] = 7' />
										</command>
										<command class_type='decision' title='Output Enabled?' exp='.SquareWaveEnable &gt; 0' swap='0' >
											<flowline >
												<command class_type='calculation' title='Set control bits' >
													<exp exp='.Data[1] = 0x10 | (.FrequencySelect &amp; 0x03)' />
												</command>
											</flowline>
											<flowline >
												<command class_type='calculation' title='Clear control bits' >
													<exp exp='.Data[1] = 0' />
												</command>
											</flowline>
										</command>
										<command class_type='comment' title='' comment='DS1307' textarea='12,0,12,0' />
										<command class_type='call' title='Component Macro' component='cal_i2c1' macro='Transaction_Write' >
											<argument exp='.data' />
											<argument exp='2' />
										</command>
									</flowline>
									<flowline >
										<command class_type='decision' title='DS3231' exp='RTCModule = 2' swap='0' >
											<flowline >
												<command class_type='calculation' title='Address' >
													<exp exp='.Data[0] = 14' />
												</command>
												<command class_type='decision' title='Output Enabled?' exp='.SquareWaveEnable &gt; 0' swap='0' >
													<flowline >
														<command class_type='calculation' title='Set control bits' >
															<exp exp='.Data[1] = 0x40 | ((.FrequencySelect &amp; 0x03) &lt;&lt; 3)' />
														</command>
													</flowline>
													<flowline >
														<command class_type='decision' title='Decision' exp='.InterruptEnable &gt; 0' swap='0' >
															<flowline >
																<command class_type='calculation' title='Enable Interrupt bits' >
																	<exp exp='.Data[1] = 0x04 | (.InterruptEnable &amp; 0x03)' />
																</command>
															</flowline>
															<flowline >
																<command class_type='calculation' title='Clear control bits' >
																	<exp exp='.Data[1] = 0' />
																</command>
															</flowline>
														</command>
													</flowline>
												</command>
												<command class_type='comment' title='' comment='DS3231 ' textarea='12,0,12,0' />
												<command class_type='call' title='Component Macro' component='cal_i2c1' macro='Transaction_Write' >
													<argument exp='.data' />
													<argument exp='2' />
												</command>
												<command class_type='sim' title='Component Macro' component='LibraryComponent1' macro='FlashTx' />
											</flowline>
											<flowline />
										</command>
									</flowline>
								</command>
							</flowline>
						</command>
					</flowline>
				</macro>
				<macro >
					<flowline name='GetSecs' description='Gets the seconds of the current time as a binary value 0-59.
Simulation returns system time.' statediag='0' >
						<return name='Return' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='WhichClock' type='u8' description='0=Time, 1=Alarm1 DS3231' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='data' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='&quot;&quot;' setinit='' >
							<array size='1' />
						</local>
						<command class_type='decision' title='Decision' exp='Simulation &amp;&amp; (SimComms == 0)' swap='0' >
							<flowline >
								<command class_type='call' title='Read Seconds Directly' macro='ReadTime' >
									<return exp='.Return' />
									<argument exp='2' />
								</command>
							</flowline>
							<flowline >
								<command class_type='decision' title='Internal' exp='RTCModule = 0' swap='0' >
									<flowline >
										<command class_type='native' title='C Code' ccode='FCL_DATA[0] = FC_CAL_RTC_GetSec_1();' language='C' />
									</flowline>
									<flowline >
										<command class_type='decision' title='DS1301 Or DS3231' exp='RTCModule = 1 || RTCModule = 2' swap='0' >
											<flowline >
												<command class_type='comment' title='' comment='DS1307 &amp; DS3231 ' textarea='12,0,12,0' />
												<command class_type='decision' title='DS3231' exp='RTCModule = 2' swap='0' >
													<flowline >
														<command class_type='switch' title='Switch' textarea='36,25,36,25' exp='.WhichClock' >
															<case >
																<flowline >
																	<command class_type='calculation' title='Address' >
																		<exp exp='.data[0] = 0' />
																	</command>
																</flowline>
															</case>
															<case exp='1' >
																<flowline >
																	<command class_type='calculation' title='Address' >
																		<exp exp='.data[0] = 7' />
																	</command>
																</flowline>
															</case>
														</command>
													</flowline>
													<flowline >
														<command class_type='calculation' title='Address' >
															<exp exp='.data[0] = 0' />
														</command>
													</flowline>
												</command>
												<command class_type='call' title='Component Macro' component='cal_i2c1' macro='Transaction_Write' >
													<argument exp='.data' />
													<argument exp='1' />
												</command>
												<command class_type='call' title='Component Macro' component='cal_i2c1' macro='Transaction_Read' >
													<argument exp='.data' />
													<argument exp='1' />
												</command>
												<command class_type='sim' title='Component Macro' component='LibraryComponent1' macro='FlashTx' />
												<command class_type='sim' title='Component Macro' component='LibraryComponent1' macro='FlashRx' />
											</flowline>
											<flowline />
										</command>
									</flowline>
								</command>
								<command class_type='calculation' title='Convert BCD to Binary' >
									<exp exp='.Return = (.Data[0] &amp; 0x0F)' />
									<exp exp='.Data[0] = (.Data[0] &amp; 0x70) &gt;&gt; 4' />
									<exp exp='.Return = .Return + (.Data[0] * 10)' />
								</command>
							</flowline>
						</command>
					</flowline>
				</macro>
				<macro >
