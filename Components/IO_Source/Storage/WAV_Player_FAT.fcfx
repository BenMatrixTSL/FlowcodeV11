								</command>
								<command class_type='decision' title='16-bit Samples?' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='BitsPerSample = 16' swap='0' >
