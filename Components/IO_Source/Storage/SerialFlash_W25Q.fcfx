									<argument exp='.Blocking' />
								</command>
							</flowline>
							<flowline >
								<command class_type='decision' title='Check WEL bit' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='Status1 &amp; 0x02' swap='0' >
									<flowline >
										<command class_type='decision' title='4KB Block' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='.EraseMode = 0' swap='0' >
											<flowline >
												<command class_type='call' title='Sector Erase Command' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' component='cal_spi1' macro='Master_Byte' >
