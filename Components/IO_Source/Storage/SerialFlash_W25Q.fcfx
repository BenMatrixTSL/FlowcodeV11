				<macro >
					<flowline name='EraseBlock_Sim' description='Erases a block of data stored on the chip.
Erase takes between 45 milliseconds and 2000 milliseconds to complete.
Blocking mode is recommended to ensure write cycle is complete.' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
						<param name='EraseMode' type='u8' description='0=4KB / 1=32KB / 2=64KB' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
