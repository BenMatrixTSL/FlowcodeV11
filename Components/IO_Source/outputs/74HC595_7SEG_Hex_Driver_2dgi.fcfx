				<macro >
					<flowline name='ShowDigit' description='Set the number to be displayed in the given digit of the display.
The colon character can also be controlled when writing to digit 1.' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
