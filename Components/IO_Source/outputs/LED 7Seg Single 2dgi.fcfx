					<flowline name='AutoOffColour' description='' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
						<local name='OffColour' type='u32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='blue' type='u32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='green' type='u32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='red' type='u32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='AppDevDetected' swap='1' >
							<flowline >
								<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='.red = (FGColour AND 255) / 4' />
									<exp exp='.green = ((FGColour &gt;&gt; 8) AND 255) / 4' />
									<exp exp='.blue = ((FGColour &gt;&gt; 16) AND 255) / 4' />
									<exp exp='.OffColour = (.blue &lt;&lt; 16) + (.green &lt;&lt; 8) + .red' />
								</command>
							</flowline>
							<flowline >
								<command class_type='comment' title='' comment='Red LED with Yellow bezel difficult to see.
Make off segs same as bezel colour
This matches the 3d version ' textarea='12,0,12,0' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' />
								<command class_type='sim' title='Built-in Function' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='Component.Property.GetValue' >
									<return exp='.OffColour' />
									<argument exp='this' />
									<argument exp='&quot;BGColour&quot;' />
								</command>
							</flowline>
						</command>
						<command class_type='sim' title='Built-in Function' disable='1' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='Debug.PrintLn' >
							<argument exp='&quot;LED off colour = &quot;+ String .OffColour' />
						</command>
						<command class_type='sim' title='Built-in Function' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='Component.Property.SetValue' >
							<argument exp='this' />
							<argument exp='&quot;LEDAutoOffColour&quot;' />
							<argument exp='.OffColour' />
						</command>
					</flowline>
