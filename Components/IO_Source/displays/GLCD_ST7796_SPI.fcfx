						<local name='X' type='u16' description='' isconst='0' isHidden='0' isinit='1' usrinit='0' setinit='0' />
						<local name='OrientationSave' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
