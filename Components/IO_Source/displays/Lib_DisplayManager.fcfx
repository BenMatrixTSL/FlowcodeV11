						<param name='Y' type='s16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Relative' type='b1' description='0=Absolute Positioning, 1=Relative Positioning' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
