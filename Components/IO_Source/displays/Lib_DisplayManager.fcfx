						<param name='DrawHighlight' type='u8' description='Draws the highlight on buttons, also used for thumb width on sliders' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='DrawWidget' type='b1' description='0=Do not draw Widget yet, 1=Draw Widget now  (Does not apply to hidden widgets)' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<command class_type='calculation' title='Calculation' >
							<exp exp='.Return = 0' />
						</command>
						<command class_type='decision' title='Decision' exp='NumWidgets &lt; MaxWidgets' swap='0' >
							<flowline >
								<command class_type='decision' title='Decision' exp='IsSim' swap='0' >
