											<argument exp='&quot;Y&quot;' />
										</command>
										<command class_type='call' title='Component Macro' textarea='36,5,36,5' component='JSONEncoder1' macro='AssignName' >
											<argument exp='4' />
											<argument exp='&quot;Rel&quot;' />
										</command>
										<command class_type='call' title='Identifier' component='JSONEncoder1' macro='AssignDataAsNumber' >
