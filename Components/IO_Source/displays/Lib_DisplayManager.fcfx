				<macro >
					<flowline name='CreateWidget' description='Define an interactive Widget or slider.
Returns the Widget ID.' statediag='0' >
						<return name='Return' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='WidgetType' type='u8' description='0=WidgetHidden, 1=WidgetRect, 2=WidgetRoundedRect, 3=WidgetEllipse, 4=VSlider, 5=HSlider, 6=VSliderRound, 7=HSliderRound, 8=Bitmap' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='X' type='u16' description='X Pixel Coordinate' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Y' type='u16' description='Y Pixel Coordinate' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Width' type='u16' description='Pixel Width or X Radius' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Height' type='u16' description='Pixel Height or Y Radius' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='LineWidth' type='u8' description='Number of pixels thick to draw outer border lines' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Colours' type='u8' description='Byte array containing FgColour RGB [0-2], Bg Colour RGB [3-5], Text Colour RGB [6-8], Hl Colour RGB [9-11]' isconst='0' isHidden='0' isinit='0' usrinit='&quot;&quot;' setinit='' >
