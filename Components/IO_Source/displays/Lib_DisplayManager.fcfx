				<macro >
					<flowline name='Prv_GetDCArray' description='' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
						<param name='data' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='&quot;&quot;' setinit='' >
							<array size='100' />
						</param>
						<param name='Idx' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Count' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='LayoutIndex' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='pointer' type='u16' description='' isconst='0' isHidden='0' isinit='1' usrinit='0' setinit='0' />
						<local name='LayoutStr' type='T8' description='' isconst='0' isHidden='0' isinit='1' usrinit='&quot;&quot;' setinit='&quot;&quot;' >
							<array size='20' />
						</local>
						<local name='PropLayout' type='T8' description='' isconst='0' isHidden='0' isinit='0' usrinit='&quot;&quot;' setinit='' >
							<array size='20' />
						</local>
						<command class_type='calculation' title='Calculation' >
							<exp exp='.PropLayout = &quot;Layout&quot; + ToString$(.LayoutIndex)' />
						</command>
						<command class_type='sim' title='Built-in Function' macro='Component.Property.GetValue' >
							<return exp='.LayoutStr' />
							<argument exp='this' />
							<argument exp='.PropLayout' />
						</command>
						<command class_type='loop' title='Loop' type='0' exp='.Idx' >
							<flowline >
								<command class_type='decision' title='Decision' exp='.LayoutStr[.pointer] == &apos;,&apos;' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='.Idx = .Idx - 1' />
										</command>
									</flowline>
									<flowline />
								</command>
								<command class_type='calculation' title='Calculation' >
									<exp exp='.pointer = .pointer + 1' />
								</command>
							</flowline>
						</command>
						<command class_type='calculation' title='Calculation' >
							<exp exp='.data[.Idx] = 0' />
						</command>
						<command class_type='loop' title='Loop' type='0' exp='.Idx &lt; .Count' >
							<flowline >
								<command class_type='decision' title='Decision' exp='.LayoutStr[.pointer] == &apos;,&apos;' swap='0' >
