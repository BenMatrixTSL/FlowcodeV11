													<command class_type='sim' title='Built-in Function' macro='Console.WriteText' >
														<argument exp='Console' />
														<argument exp='&quot;Hidden &quot;' />
													</command>
												</flowline>
											</case>
											<case exp='1' >
												<flowline >
													<command class_type='sim' title='Built-in Function' macro='Console.WriteText' >
														<argument exp='Console' />
														<argument exp='&quot;Rectangle &quot;' />
													</command>
												</flowline>
											</case>
											<case exp='2' >
												<flowline >
													<command class_type='sim' title='Built-in Function' macro='Console.WriteText' >
														<argument exp='Console' />
														<argument exp='&quot;Rounded Rectangle &quot;' />
													</command>
												</flowline>
											</case>
											<case exp='3' >
												<flowline >
													<command class_type='sim' title='Built-in Function' macro='Console.WriteText' >
														<argument exp='Console' />
														<argument exp='&quot;Ellipse &quot;' />
													</command>
												</flowline>
											</case>
											<case exp='4' >
												<flowline >
													<command class_type='sim' title='Built-in Function' macro='Console.WriteText' >
														<argument exp='Console' />
														<argument exp='&quot;Vertical Slider &quot;' />
													</command>
												</flowline>
											</case>
											<case exp='5' >
												<flowline >
													<command class_type='sim' title='Built-in Function' macro='Console.WriteText' >
														<argument exp='Console' />
														<argument exp='&quot;Horizontal Slider &quot;' />
													</command>
												</flowline>
											</case>
											<case exp='6' >
												<flowline >
													<command class_type='sim' title='Built-in Function' macro='Console.WriteText' >
														<argument exp='Console' />
														<argument exp='&quot;Vertical Slider Rounded &quot;' />
													</command>
												</flowline>
											</case>
											<case exp='7' >
												<flowline >
													<command class_type='sim' title='Built-in Function' macro='Console.WriteText' >
														<argument exp='Console' />
														<argument exp='&quot;Horizontal Slider Rounded &quot;' />
													</command>
												</flowline>
											</case>
											<case exp='8' >
												<flowline >
													<command class_type='sim' title='Built-in Function' macro='Console.WriteText' >
														<argument exp='Console' />
														<argument exp='&quot;Bitmap &quot;' />
													</command>
