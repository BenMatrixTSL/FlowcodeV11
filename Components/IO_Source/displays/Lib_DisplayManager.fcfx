						<param name='ObjectWidth' type='u16' description='Widget width in pixels' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='ObjectHeight' type='u16' description='Object height in pixels' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='SpacingX' type='u16' description='Horizontal spacing between the objects' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='SpacingY' type='u16' description='Vertical spacing between the objects' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='LineWidth' type='u8' description='Number of pixels thick to draw outer border lines' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Colours' type='u8' description='Byte array containing FgColour RGB [0-2], Bg Colour RGB [3-5], Text Colour RGB [6-8]' isconst='0' isHidden='0' isinit='0' usrinit='&quot;&quot;' setinit='' >
							<array size='9' />
						</param>
						<param name='Font' type='u8' description='Font used for the widgets text' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='ObjectText' type='T8*' description='A comma seperated text field with the text for each object e.g. &quot;1,2,3,..&quot;' isconst='0' isHidden='0' isinit='0' usrinit='&quot;&quot;' setinit='' >
