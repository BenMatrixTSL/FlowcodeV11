									<exp exp='Y[MaxObjects + NumWidgets] = .Y' />
									<exp exp='W[MaxObjects + NumWidgets] = .Width' />
									<exp exp='H[MaxObjects + NumWidgets] = .Height' />
									<exp exp='LineWidth[MaxObjects + NumWidgets] = .LineWidth' />
									<exp exp='WidgetColour[NumWidgets][0][0] = .Colours[0]' />
									<exp exp='WidgetColour[NumWidgets][0][1] = .Colours[1]' />
									<exp exp='WidgetColour[NumWidgets][0][2] = .Colours[2]' />
									<exp exp='WidgetColour[NumWidgets][1][0] = .Colours[3]' />
									<exp exp='WidgetColour[NumWidgets][1][1] = .Colours[4]' />
									<exp exp='WidgetColour[NumWidgets][1][2] = .Colours[5]' />
									<exp exp='WidgetColour[NumWidgets][2][0] = .Colours[6]' />
									<exp exp='WidgetColour[NumWidgets][2][1] = .Colours[7]' />
									<exp exp='WidgetColour[NumWidgets][2][2] = .Colours[8]' />
									<exp exp='WidgetColour[NumWidgets][3][0] = .Colours[9]' />
									<exp exp='WidgetColour[NumWidgets][3][1] = .Colours[10]' />
									<exp exp='WidgetColour[NumWidgets][3][2] = .Colours[11]' />
									<exp exp='DrawHighlight[NumWidgets] = .DrawHighlight' />
									<exp exp='Value[NumWidgets] = 0.0' />
									<exp exp='OldValue[NumWidgets] = 2.0' />
