						<local name='Fval' type='f32' description='' isconst='0' isHidden='0' isinit='1' usrinit='0.0' setinit='0' />
						<command class_type='calculation' title='No Press Detected' >
							<exp exp='.Return = 0' />
						</command>
						<command class_type='loop' title='Loop' type='2' forvar='.Idx' exp='NumWidgets' >
							<flowline >
								<command class_type='calculation' title='Calculation' >
									<exp exp='.ID = .Idx + MaxObjects' />
								</command>
								<command class_type='decision' title='Check if X in range' exp='(.X &gt;= X[.ID]) &amp;&amp; (.X &lt;= (X[.ID] + W[.ID]))' swap='0' >
									<flowline >
										<command class_type='decision' title='Check if Y in range' exp='(.Y &gt;= Y[.ID]) &amp;&amp; (.Y &lt;= (Y[.ID] + H[.ID]))' swap='0' >
											<flowline >
												<command class_type='decision' title='Standard Widgets' exp='Type[.ID] &lt; 4' swap='0' >
													<flowline >
														<command class_type='decision' title='Decision' exp='TouchInterval = 0' swap='0' >
															<flowline >
																<command class_type='calculation' title='Touch detected - return Widget index' >
																	<exp exp='LastWidgetPress = .ID + 1' />
																	<exp exp='.Return = LastWidgetPress' />
																	<exp exp='TouchInterval = TouchRepeatRate' />
																</command>
																<command class_type='decision' title='Decision' exp='IsSim' swap='0' >
																	<flowline >
																		<command class_type='sim' title='Built-in Function' macro='Console.WriteText' >
																			<argument exp='Console' />
																			<argument exp='&quot;Touch Detected: ID=&quot; + STRING LastWidgetPress + &quot;\n&quot;' />
																		</command>
																	</flowline>
																	<flowline />
																</command>
															</flowline>
															<flowline >
																<command class_type='calculation' title='Touch previously detected - decrement counter' >
																	<exp exp='TouchInterval = TouchInterval - 1' />
																</command>
																<command class_type='calculation' title='Return detected Widget index' >
																	<exp exp='.Return = 0' />
																</command>
																<command class_type='goto' title='Goto Connection Point' labelid='0' labelname='' />
															</flowline>
														</command>
													</flowline>
													<flowline >
														<command class_type='calculation' title='Touch detected - return Widget index' >
															<exp exp='LastWidgetPress = .ID + 1' />
															<exp exp='.Return = LastWidgetPress' />
															<exp exp='TouchInterval = 0' />
														</command>
														<command class_type='decision' title='Decision' exp='IsSim' swap='0' >
															<flowline >
																<command class_type='sim' title='Built-in Function' macro='Console.WriteText' >
																	<argument exp='Console' />
																	<argument exp='&quot;Touch Detected: ID=&quot; + STRING LastWidgetPress + &quot;\n&quot;' />
																</command>
															</flowline>
															<flowline />
														</command>
													</flowline>
												</command>
												<command class_type='decision' title='Slider V?' exp='(Type[.ID] = 4) || (Type[.ID] = 6)' swap='0' >
													<flowline >
														<command class_type='calculation' title='Calculation' >
															<exp exp='.Coord = .Y - Y[.ID] - (DrawHighlight[.ID - MaxObjects] &gt;&gt; 1)' />
														</command>
														<command class_type='calculation' title='Calculation' >
