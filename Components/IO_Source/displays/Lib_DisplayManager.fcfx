				<macro >
					<flowline name='Prv_BoundsCheck' description='' statediag='0' >
						<return name='Return' type='b1' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='PosInt' type='s16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' >
							<array size='4' />
						</param>
						<param name='Handle' type='h32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='Name' type='T8' description='' isconst='0' isHidden='0' isinit='1' usrinit='&quot;&quot;' setinit='&quot;&quot;' >
							<array size='20' />
						</local>
						<local name='XPix' type='u16' description='' isconst='0' isHidden='0' isinit='1' usrinit='0' setinit='0' />
						<local name='YPix' type='u16' description='' isconst='0' isHidden='0' isinit='1' usrinit='0' setinit='0' />
						<command class_type='calculation' title='Calculation' >
							<exp exp='.Return = 0' />
						</command>
						<command class_type='decision' title='Decision' exp='Orientation &amp; 1' swap='0' >
							<flowline >
								<command class_type='calculation' title='Calculation' >
									<exp exp='.XPix = YPixels' />
									<exp exp='.YPix = XPixels' />
								</command>
							</flowline>
							<flowline >
								<command class_type='calculation' title='Calculation' >
									<exp exp='.XPix = XPixels' />
									<exp exp='.YPix = YPixels' />
								</command>
							</flowline>
						</command>
						<command class_type='sim' title='Built-in Function' macro='Tree.GetName' >
							<return exp='.Name' />
							<argument exp='.Handle' />
						</command>
						<command class_type='decision' title='Check for -ve X dimension' exp='.PosINT[0] &lt; 0' swap='0' >
							<flowline >
								<command class_type='calculation' title='Subtract -ve offset from width' >
									<exp exp='.PosINT[2] = .PosINT[2] + .PosINT[0]' />
									<exp exp='.PosINT[0] = 0' />
								</command>
								<command class_type='sim' title='Built-in Function' macro='Console.WriteText' >
									<argument exp='Console' />
									<argument exp='&quot;Warning: &quot; + .Name + &quot; Has a negative X dimension\n&quot;' />
								</command>
							</flowline>
							<flowline >
								<command class_type='decision' title='Check for out of bounds X dimension' exp='.PosINT[0] &gt; .XPix' swap='0' >
									<flowline >
										<command class_type='sim' title='Built-in Function' macro='Console.WriteText' >
											<argument exp='Console' />
											<argument exp='&quot;Warning: &quot; + .Name + &quot; Out of bounds - skipped\n&quot;' />
										</command>
										<command class_type='calculation' title='Calculation' >
											<exp exp='.Return = 1' />
										</command>
									</flowline>
									<flowline />
								</command>
							</flowline>
						</command>
						<command class_type='decision' title='Check for -ve Y dimension' exp='.PosINT[1] &lt; 0' swap='0' >
							<flowline >
								<command class_type='calculation' title='Subtract -ve offset from height' >
									<exp exp='.PosINT[3] = .PosINT[3] + .PosINT[1]' />
									<exp exp='.PosINT[1] = 0' />
								</command>
								<command class_type='sim' title='Built-in Function' macro='Console.WriteText' >
									<argument exp='Console' />
									<argument exp='&quot;Warning: &quot; + .Name + &quot; Has a negative Y dimension\n&quot;' />
								</command>
							</flowline>
							<flowline >
								<command class_type='decision' title='Check for out of bounds Y dimension' exp='.PosINT[1] &gt; .YPix' swap='0' >
									<flowline >
										<command class_type='sim' title='Built-in Function' macro='Console.WriteText' >
											<argument exp='Console' />
											<argument exp='&quot;Warning: &quot; + .Name + &quot; Out of bounds - skipped\n&quot;' />
										</command>
										<command class_type='calculation' title='Calculation' >
											<exp exp='.Return = 1' />
										</command>
									</flowline>
									<flowline />
								</command>
							</flowline>
						</command>
						<command class_type='decision' title='Check for width too big' exp='(.PosINT[0] + .PosINT[2]) &gt; .XPix' swap='0' >
							<flowline >
								<command class_type='calculation' title='Subtract +ve offset from width' >
									<exp exp='.PosINT[2] = .XPix - .PosINT[0]' />
								</command>
								<command class_type='sim' title='Built-in Function' macro='Console.WriteText' >
									<argument exp='Console' />
									<argument exp='&quot;Warning: &quot; + .Name + &quot; Width goes off the display\n&quot;' />
								</command>
							</flowline>
							<flowline />
						</command>
						<command class_type='decision' title='Check for height too big' exp='(.PosINT[1] + .PosINT[3]) &gt; .YPix' swap='0' >
							<flowline >
								<command class_type='calculation' title='Subtract +ve offset from width' >
									<exp exp='.PosINT[3] = .YPix - .PosINT[1]' />
								</command>
								<command class_type='sim' title='Built-in Function' macro='Console.WriteText' >
									<argument exp='Console' />
									<argument exp='&quot;Warning: &quot; + .Name + &quot; Height goes off the display\n&quot;' />
								</command>
							</flowline>
							<flowline />
						</command>
					</flowline>
				</macro>
