					<flowline name='CheckCollision' description='Checks to see if two objects bounding boxes are colliding,
Useful when implementing 2D games.' statediag='0' >
						<return name='Return' type='b1' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='ID1' type='u16' description='ID of Object or Widget to check' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='ID2' type='u16' description='ID of Object or Widget to check' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='Test' type='u8' description='' isconst='0' isHidden='0' isinit='1' usrinit='0' setinit='0' />
						<command class_type='calculation' title='Calculation' >
							<exp exp='.Return = 0' />
						</command>
						<command class_type='calculation' title='Return to 0 indexed ID' >
							<exp exp='.ID1 = .ID1 - 1' />
							<exp exp='.ID2 = .ID2 - 1' />
						</command>
						<command class_type='decision' title='Decision' exp='X[.ID1] &gt; X[.ID2]' swap='0' >
							<flowline >
								<command class_type='decision' title='Decision' exp='X[.ID1] &lt;= (X[.ID2] + W[.ID2])' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='.Test = 1' />
										</command>
									</flowline>
									<flowline />
								</command>
							</flowline>
							<flowline >
								<command class_type='decision' title='Decision' exp='X[.ID2] &lt;= (X[.ID1] + W[.ID1])' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='.Test = 1' />
										</command>
									</flowline>
									<flowline />
								</command>
							</flowline>
						</command>
						<command class_type='decision' title='Decision' exp='.Test' swap='0' >
							<flowline >
								<command class_type='decision' title='Decision' exp='Y[.ID1] &gt; Y[.ID2]' swap='0' >
									<flowline >
										<command class_type='decision' title='Decision' exp='Y[.ID1] &lt;= (Y[.ID2] + H[.ID2])' swap='0' >
											<flowline >
												<command class_type='calculation' title='Calculation' >
													<exp exp='.Test = 2' />
												</command>
											</flowline>
											<flowline />
										</command>
									</flowline>
									<flowline >
										<command class_type='decision' title='Decision' exp='Y[.ID2] &lt;= (Y[.ID1] + H[.ID1])' swap='0' >
											<flowline >
												<command class_type='calculation' title='Calculation' >
													<exp exp='.Test = 2' />
												</command>
											</flowline>
											<flowline />
										</command>
									</flowline>
								</command>
								<command class_type='decision' title='Decision' exp='.Test = 2' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='.Return = 1' />
										</command>
									</flowline>
									<flowline />
								</command>
							</flowline>
							<flowline />
						</command>
					</flowline>
				</macro>
