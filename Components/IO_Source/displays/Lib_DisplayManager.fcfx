				<macro >
					<flowline name='ModifyColour' description='Changes the theme index of the selected Object or Widget.' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
						<param name='ID' type='u16' description='Unique ID of the object or Widget' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Index' type='u8' description='0=Foreground, 1=Background, 2=TextColour, 3=Highlight(Widgets Only)' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='R' type='u8' description='Colour Channels' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='G' type='u8' description='Colour Channels' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='B' type='u8' description='Colour Channels' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Redraw' type='b1' description='Redraws the component straight away in the new colours' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='MQTT_Str' type='T8' description='' isconst='0' isHidden='0' isinit='1' usrinit='&quot;&quot;' setinit='&quot;&quot;' >
							<array size='30' />
						</local>
						<command class_type='decision' title='Decision' exp='.ID &gt; 0' swap='0' >
							<flowline >
								<command class_type='calculation' title='Change to 0 based ID' >
									<exp exp='.ID = .ID - 1' />
								</command>
								<command class_type='decision' title='Check ID is valid' exp='.ID &lt; NumObjects' swap='0' >
									<flowline >
										<command class_type='decision' title='Decision' exp='.Index &lt; 3' swap='0' >
											<flowline >
												<command class_type='calculation' title='Update Theme' >
													<exp exp='ObjectColour[.ID][.Index][0] = .R' />
													<exp exp='ObjectColour[.ID][.Index][1] = .G' />
													<exp exp='ObjectColour[.ID][.Index][2] = .B' />
												</command>
											</flowline>
											<flowline />
										</command>
									</flowline>
									<flowline />
								</command>
								<command class_type='decision' title='Check ID is valid' exp='(.ID &gt;= MaxObjects) &amp;&amp; (.ID &lt; (MaxObjects + NumWidgets))' swap='0' >
									<flowline >
										<command class_type='decision' title='Decision' exp='.Index &lt; 4' swap='0' >
											<flowline >
												<command class_type='calculation' title='Update Theme' >
													<exp exp='WidgetColour[.ID - MaxObjects][.Index][0] = .R' />
													<exp exp='WidgetColour[.ID - MaxObjects][.Index][1] = .G' />
													<exp exp='WidgetColour[.ID - MaxObjects][.Index][2] = .B' />
												</command>
											</flowline>
											<flowline />
										</command>
									</flowline>
									<flowline />
								</command>
								<command class_type='decision' title='Decision' exp='.Redraw' swap='0' >
									<flowline >
										<command class_type='call' title='User Macro' macro='DrawItem' >
											<argument exp='.ID + 1' />
										</command>
