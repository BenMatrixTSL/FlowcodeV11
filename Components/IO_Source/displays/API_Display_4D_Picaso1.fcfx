											<argument exp='.Y + .CurY' />
										</command>
										<command class_type='call' title='Call Macro' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='DrawLine' >
											<argument exp='.X - (.CurX / .K)' />
											<argument exp='.Y - .CurY' />
