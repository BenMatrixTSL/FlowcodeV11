				<macro >
					<flowline name='BPlot' description='Sets a pixel with the current background colour at pixel location X, Y.' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
						<param name='X1' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Y1' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='address' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='mod_y' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='data' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='old_data' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='shift' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<command class_type='switch' title='Switch' textarea='36,25,36,25' exp='Orientation' >
							<case >
								<flowline />
							</case>
							<case exp='1' >
								<flowline >
									<command class_type='calculation' title='Calculation' >
										<exp exp='.address = pixel_width - (.Y1 + 1)' />
										<exp exp='.Y1 = .X1' />
										<exp exp='.X1 = .address' />
									</command>
								</flowline>
							</case>
							<case exp='2' >
								<flowline >
									<command class_type='calculation' title='Calculation' >
										<exp exp='.X1 = pixel_width - (.X1 + 1)' />
										<exp exp='.Y1 = pixel_height - (.Y1 + 1)' />
									</command>
								</flowline>
							</case>
							<case exp='3' >
								<flowline >
									<command class_type='calculation' title='Calculation' >
										<exp exp='.address = .Y1' />
										<exp exp='.Y1 = pixel_height - (.X1 + 1)' />
										<exp exp='.X1 = .address' />
									</command>
								</flowline>
							</case>
						</command>
						<command class_type='decision' title='Decision' exp='0' swap='0' >
							<flowline >
								<command class_type='calculation' title='Calculation' >
									<exp exp='CommsBuff[0] = .X1' />
									<exp exp='CommsBuff[1] = .Y1' />
								</command>
								<command class_type='decision' title='Decision' exp='background_col_var' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='commsBuff[1] = commsBuff[1] | 0x80' />
										</command>
									</flowline>
									<flowline />
								</command>
								<command class_type='call' title='DrawDotXRegAddr' macro='WriteSeriesToReg' >
									<argument exp='64' />
									<argument exp='commsBuff' />
									<argument exp='2' />
								</command>
							</flowline>
							<flowline >
								<command class_type='calculation' title='Calculation' >
									<exp exp='.mod_y = .Y1 &amp; 7' />
								</command>
								<command class_type='decision' title='Decision' exp='UseBuffer' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculate RAM Address' >
											<exp exp='.address = ((.Y1 &gt;&gt; 3) * pixel_width) + .X1' />
										</command>
										<command class_type='calculation' title='Calculation' >
											<exp exp='.data = dram[.address]' />
										</command>
										<command class_type='calculation' title='Calculation' >
											<exp exp='.old_data = .data' />
										</command>
									</flowline>
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='.data = background_col_var' />
											<exp exp='.old_data = ~ .data' />
										</command>
									</flowline>
								</command>
								<command class_type='decision' title='Decision' exp='background_col_var' swap='0' >
									<flowline >
										<command class_type='calculation' title='Set Pixel' >
											<exp exp='.data = .data | (0x01 &lt;&lt; .mod_y)' />
										</command>
									</flowline>
									<flowline >
										<command class_type='calculation' title='Clear Pixel' >
											<exp exp='.data = .data &amp; ~ (0x01 &lt;&lt; .mod_y)' />
										</command>
									</flowline>
								</command>
								<command class_type='call' title='Call Macro' macro='Goto_XY' >
									<argument exp='.X1' />
									<argument exp='.Y1' />
								</command>
								<command class_type='decision' title='Decision' exp='DisplayVersion' swap='0' >
									<flowline >
										<command class_type='call' title='Write Data to display - 8 pixels' macro='SendData' >
											<argument exp='.data' />
										</command>
									</flowline>
									<flowline >
										<command class_type='calculation' title='Get Y coord offset 0, 2, 4 or 6' >
											<exp exp='.shift = .Y1 &amp; 0x06' />
										</command>
										<command class_type='calculation' title='shift and mask off 2 LSB' >
											<exp exp='.mod_y = .data &gt;&gt; .shift' />
											<exp exp='.mod_y = .mod_y &amp; 0x03' />
										</command>
										<command class_type='calculation' title='Calculation' >
											<exp exp='.shift = 0' />
										</command>
										<command class_type='decision' title='Decision' exp='.mod_y &amp; 0x01' swap='0' >
											<flowline >
												<command class_type='calculation' title='Calculation' >
													<exp exp='.shift = .shift | 0x0F' />
												</command>
											</flowline>
											<flowline />
										</command>
										<command class_type='decision' title='Decision' exp='.mod_y &amp; 0x02' swap='0' >
											<flowline >
												<command class_type='calculation' title='Calculation' >
													<exp exp='.shift = .shift | 0xF0' />
												</command>
											</flowline>
											<flowline />
										</command>
										<command class_type='call' title='Write Data to display - 2 pixels' macro='SendData' >
											<argument exp='.shift' />
										</command>
									</flowline>
								</command>
								<command class_type='decision' title='Decision' exp='UseBuffer' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='dram[.address] = .data' />
										</command>
									</flowline>
									<flowline />
								</command>
							</flowline>
						</command>
					</flowline>
				</macro>
