											<exp exp='.address = ((.Y1 &gt;&gt; 3) * pixel_width) + .X1' />
										</command>
										<command class_type='calculation' title='Calculation' >
											<exp exp='.data = dram[.address]' />
										</command>
										<command class_type='calculation' title='Calculation' >
											<exp exp='.old_data = .data' />
										</command>
									</flowline>
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='.data = background_col_var' />
											<exp exp='.old_data = ~ .data' />
										</command>
									</flowline>
								</command>
								<command class_type='decision' title='Decision' exp='foreground_col_var' swap='0' >
									<flowline >
										<command class_type='calculation' title='Set Pixel' >
											<exp exp='.data = .data | (0x01 &lt;&lt; .mod_y)' />
										</command>
									</flowline>
									<flowline >
										<command class_type='calculation' title='Clear Pixel' >
											<exp exp='.data = .data &amp; ~ (0x01 &lt;&lt; .mod_y)' />
										</command>
									</flowline>
								</command>
								<command class_type='call' title='Call Macro' macro='Goto_XY' >
									<argument exp='.X1' />
									<argument exp='.Y1' />
								</command>
								<command class_type='decision' title='Decision' exp='DisplayVersion = 0' swap='0' >
									<flowline />
									<flowline />
								</command>
								<command class_type='decision' title='Decision' exp='DisplayVersion = 1' swap='0' >
									<flowline />
									<flowline />
								</command>
								<command class_type='decision' title='Decision' exp='DisplayVersion = 2' swap='0' >
									<flowline />
									<flowline />
								</command>
								<command class_type='decision' title='Decision' exp='DisplayVersion' swap='0' >
									<flowline >
										<command class_type='call' title='Write Data to display - 8 pixels' macro='SendData' >
											<argument exp='.data' />
										</command>
									</flowline>
									<flowline >
										<command class_type='calculation' title='Get Y coord offset 0, 2, 4 or 6' >
											<exp exp='.shift = .Y1 &amp; 0x06' />
										</command>
										<command class_type='calculation' title='shift and mask off 2 LSB' >
											<exp exp='.mod_y = .data &gt;&gt; .shift' />
											<exp exp='.mod_y = .mod_y &amp; 0x03' />
										</command>
										<command class_type='calculation' title='Calculation' >
											<exp exp='.shift = 0' />
										</command>
										<command class_type='decision' title='Decision' exp='.mod_y &amp; 0x01' swap='0' >
											<flowline >
												<command class_type='calculation' title='Calculation' >
													<exp exp='.shift = .shift | 0x0F' />
												</command>
											</flowline>
											<flowline />
										</command>
										<command class_type='decision' title='Decision' exp='.mod_y &amp; 0x02' swap='0' >
											<flowline >
												<command class_type='calculation' title='Calculation' >
													<exp exp='.shift = .shift | 0xF0' />
												</command>
											</flowline>
											<flowline />
										</command>
										<command class_type='call' title='Write Data to display - 2 pixels' macro='SendData' >
											<argument exp='.shift' />
										</command>
									</flowline>
								</command>
								<command class_type='decision' title='Decision' exp='UseBuffer' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='dram[.address] = .data' />
										</command>
									</flowline>
									<flowline />
								</command>
							</flowline>
						</command>
