				<macro >
					<flowline name='Goto_XY' description='' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
						<param name='X' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Y' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<command class_type='calculation' title='Calculation' >
							<exp exp='currentx = .X' />
							<exp exp='currenty = .Y' />
							<exp exp='page = .Y &gt;&gt; 3' />
						</command>
						<command class_type='decision' title='Decision' exp='DisplayVersion = 0' swap='0' >
							<flowline >
								<command class_type='call' title='Set Column' macro='SendCommand3' >
									<argument exp='0x15' />
									<argument exp='.x * 4' />
									<argument exp='0x37' />
								</command>
								<command class_type='call' title='Set Row' macro='SendCommand3' >
									<argument exp='0x75' />
									<argument exp='.Y' />
									<argument exp='0x5C' />
								</command>
								<command class_type='call' title='Set Address - offsetby 8' disable='1' macro='SendData' >
									<argument exp='8 + (.X * 4)' />
								</command>
								<command class_type='call' title='Set Address' disable='1' macro='SendData' >
									<argument exp='0x37' />
								</command>
								<command class_type='call' title='Set Column' disable='1' macro='SendCommand' >
									<argument exp='0x75' />
								</command>
								<command class_type='call' title='Set Address' disable='1' macro='SendData' >
									<argument exp='.Y' />
								</command>
								<command class_type='call' title='Set Address' disable='1' macro='SendData' >
									<argument exp='7 + .Y' />
								</command>
								<command class_type='call' title='Write Ram Command' disable='1' macro='SendCommand' >
									<argument exp='0x5C' />
								</command>
							</flowline>
							<flowline />
						</command>
						<command class_type='decision' title='Decision' exp='DisplayVersion = 1' swap='0' >
							<flowline >
								<command class_type='call' title='Set Row' macro='SendCommand' >
									<argument exp='0xB0 | page' />
								</command>
								<command class_type='call' title='Column' macro='SendCommand' >
									<argument exp='.X &amp; 0x0F' />
								</command>
								<command class_type='call' title='Column' macro='SendCommand' >
									<argument exp='0x10 | (.X &gt;&gt; 4)' />
								</command>
							</flowline>
							<flowline />
						</command>
						<command class_type='decision' title='Decision' exp='DisplayVersion = 2' swap='0' >
							<flowline >
								<command class_type='call' title='Set Row' macro='SendCommand' >
									<argument exp='0xB0 | page' />
								</command>
								<command class_type='call' title='Set Column Lower' macro='SendCommand' >
									<argument exp='.X &amp; 0x0F' />
								</command>
								<command class_type='call' title='Set Column Higher' macro='SendCommand' >
									<argument exp='0x10 + ((.X &gt;&gt; 4) &amp; 0x0F)' />
								</command>
							</flowline>
							<flowline />
						</command>
						<command class_type='decision' title='Decision' exp='DisplayVersion = 3' swap='0' >
