							<flowline >
								<command class_type='loop' title='Loop' type='0' exp='.y &lt;= 0' >
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='.r = round (SQRT (.Radius * .Radius - .y * .y))' />
											<exp exp='.xx1 = .x1 + .r' />
											<exp exp='.xx2 = .x1 - .r' />
											<exp exp='.xx1 = .xx1 + .width - .Radius' />
