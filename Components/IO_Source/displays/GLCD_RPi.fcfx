struct fb_fix_screeninfo finfo;

screen_fid = open(&quot;/dev/fb0&quot;, O_RDWR);
if (screen_fid &gt; 0) 
{
	if (ioctl(screen_fid, FBIOGET_FSCREENINFO, &amp;finfo) == 0)
	{
		if (ioctl(screen_fid, FBIOGET_VSCREENINFO, &amp;vinfo) == 0)
		{
			screen_siz = finfo.smem_len;
			screen_mvx = vinfo.xres;
			screen_mvy = vinfo.yres;
			screen_bpp = vinfo.bits_per_pixel;

			if (screen_dbf)
			{
				printf(&quot;width  = %d\n&quot;, screen_mvx);
				printf(&quot;height = %d\n&quot;, screen_mvy);
				printf(&quot;bits per pixel = %d\n&quot;, screen_bpp);
				printf(&quot;frame buffer size = %d (bytes)\n&quot;, screen_siz);
			}

			screen_ptr = (char*)mmap(0, screen_siz, PROT_READ | PROT_WRITE, MAP_SHARED, screen_fid, 0);

			if ((int)screen_ptr  == -1)
			{
				if (screen_dbf) printf(&quot;Failed to mmap\n&quot;);
				close(screen_fid);
				screen_fid = -1;
			}
			else
			{
				FCR_RETVAL = 1; // All OK
			}
		}
		else
		{
			if (screen_dbf) printf(&quot;Error reading variable information\n&quot;);
			close(screen_fid);
			screen_fid = -1;
		}
	}
	else
	{
		if (screen_dbf) printf(&quot;Error reading fixed information\n&quot;);
		close(screen_fid);
		screen_fid = -1;
	}
}
else
{
    if (screen_dbf) printf(&quot;Error: cannot open framebuffer\n&quot;);
}' language='C' />
