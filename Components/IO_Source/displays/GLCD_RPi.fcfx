				<macro >
					<flowline name='Plot' description='Sets a pixel with the current foreground colour at pixel location X, Y.' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
						<param name='X1' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Y1' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='temp' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='Colour' type='u32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<command class_type='switch' title='Switch' textarea='36,25,36,25' exp='Orientation' >
							<case >
								<flowline />
							</case>
							<case exp='1' >
								<flowline >
									<command class_type='calculation' title='Calculation' >
										<exp exp='.temp = pixel_width - (.Y1 + 1)' />
										<exp exp='.Y1 = .X1' />
										<exp exp='.X1 = .temp' />
									</command>
								</flowline>
							</case>
							<case exp='2' >
								<flowline >
									<command class_type='calculation' title='Calculation' >
										<exp exp='.X1 = pixel_width - (.X1 + 1)' />
										<exp exp='.Y1 = pixel_height - (.Y1 + 1)' />
									</command>
								</flowline>
							</case>
							<case exp='3' >
								<flowline >
									<command class_type='calculation' title='Calculation' >
										<exp exp='.temp = .Y1' />
										<exp exp='.Y1 = pixel_height - (.X1 + 1)' />
										<exp exp='.X1 = .temp' />
									</command>
								</flowline>
							</case>
						</command>
						<command class_type='decision' title='Decision' exp='(.X1 &lt; pixel_width) &amp;&amp; (.Y1 &lt; pixel_height)' swap='0' >
							<flowline >
								<command class_type='calculation' title='Calculation' >
									<exp exp='.Colour = foreground_col_var' />
								</command>
								<command class_type='native' title='C Code' ccode='{
	unsigned char *p;
	unsigned long offset;
	switch(screen_bpp)
	{
	  case 16:
		offset = ((screen_mvx * FCL_Y1) + FCL_X1)*2;
		if (offset &lt; (screen_siz - 1))
		{
			p = screen_ptr + offset;
			*p++ = FCL_COLOUR;
			FCL_COLOUR &gt;&gt;= 8;
			*p = FCL_COLOUR;
		}
		break;
	  case 32:
		offset = ((screen_mvx * FCL_Y1) + FCL_X1)*4;
		if (offset &lt; (screen_siz - 3))
		{
			p = screen_ptr + offset;
			*p++ = FCL_COLOUR;
			FCL_COLOUR &gt;&gt;= 8;
			*p++ = FCL_COLOUR;
			FCL_COLOUR &gt;&gt;= 8;
			*p++ = FCL_COLOUR;
			FCL_COLOUR &gt;&gt;= 8;
			*p = FCL_COLOUR;
		}
		break;
	}
}' language='C' />
							</flowline>
							<flowline />
						</command>
					</flowline>
				</macro>
