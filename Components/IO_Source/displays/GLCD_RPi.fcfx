							<exp exp='.y = .oy' />
						</command>
						<command class_type='loop' title='Loop' type='0' exp='.y &lt; (.oy + .height)' >
							<flowline >
								<command class_type='calculation' title='Calculation' >
									<exp exp='.x = .ox' />
								</command>
								<command class_type='loop' title='Loop' type='0' exp='.x &lt; (.ox + .width)' >
									<flowline >
										<command class_type='decision' title='Decision' exp='.fg' swap='0' >
											<flowline >
												<command class_type='call' title='Call Macro' macro='Plot' >
													<argument exp='.x' />
													<argument exp='.y' />
												</command>
											</flowline>
											<flowline >
												<command class_type='call' title='Call Macro' macro='BPlot' >
													<argument exp='.x' />
													<argument exp='.y' />
												</command>
											</flowline>
										</command>
										<command class_type='calculation' title='Calculation' >
											<exp exp='.x = .x + 1' />
										</command>
									</flowline>
								</command>
								<command class_type='calculation' title='Calculation' >
									<exp exp='.y = .y + 1' />
								</command>
							</flowline>
						</command>
