							<exp exp='.Red = foreground_colour &amp; 0xFF' />
							<exp exp='.Red = .Red &gt;&gt; (8 - blue_bit_depth)' />
							<exp exp='.Green = (foreground_colour &gt;&gt; 8) &amp; 0xFF' />
