								<command class_type='calculation' title='Start Locations' >
									<exp exp='.CurX = 0' />
									<exp exp='.CurY = .Radius' />
								</command>
								<command class_type='loop' title='Loop' type='0' exp='.CurX &lt;= .CurY' >
									<flowline >
										<command class_type='decision' title='Decision' exp='.CurY &gt; 0' swap='0' >
											<flowline >
												<command class_type='call' title='Call Macro' macro='DrawLine' >
													<argument exp='.X - .CurY' />
													<argument exp='.Y + .CurX' />
