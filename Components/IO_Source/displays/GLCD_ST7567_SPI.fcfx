										<command class_type='calculation' title='Calculation' >
											<exp exp='.sx = Round (FLOAT .j * cos (.Angle))' />
											<exp exp='.sy = Round (FLOAT .j * sin (.Angle))' />
										</command>
										<command class_type='calculation' title='Calculation' >
