				<macro >
					<flowline name='Plot' description='Sets a pixel with the current foreground colour at pixel location X, Y.' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
						<param name='X1' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Y1' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='index' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='mod_y' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='page' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='temp' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<command class_type='switch' title='Switch' textarea='36,25,36,25' exp='Orientation' >
							<case >
								<flowline />
							</case>
							<case exp='1' >
								<flowline >
									<command class_type='calculation' title='Calculation' >
										<exp exp='.temp = pixel_width - (.Y1 + 1)' />
										<exp exp='.Y1 = .X1' />
										<exp exp='.X1 = .temp' />
									</command>
								</flowline>
							</case>
							<case exp='2' >
								<flowline >
									<command class_type='calculation' title='Calculation' >
										<exp exp='.X1 = pixel_width - (.X1 + 1)' />
										<exp exp='.Y1 = pixel_height - (.Y1 + 1)' />
									</command>
								</flowline>
							</case>
							<case exp='3' >
								<flowline >
									<command class_type='calculation' title='Calculation' >
										<exp exp='.temp = .Y1' />
										<exp exp='.Y1 = pixel_height - (.X1 + 1)' />
										<exp exp='.X1 = .temp' />
									</command>
								</flowline>
							</case>
						</command>
						<command class_type='calculation' title='Calculation' >
							<exp exp='.page = .Y1 &gt;&gt; 3' />
						</command>
						<command class_type='decision' title='Decision' exp='.X1 &gt;= 128' swap='0' >
							<flowline >
								<command class_type='calculation' title='Calculation' >
									<exp exp='.X1 = .X1 - 128' />
								</command>
							</flowline>
							<flowline />
						</command>
						<command class_type='calculation' title='Calculation' >
							<exp exp='.index = (.page * 128) + .X1' />
						</command>
						<command class_type='calculation' title='Calculation' >
							<exp exp='.mod_y = .Y1 &amp; 0x07' />
						</command>
						<command class_type='decision' title='Decision' exp='foreground_col_var' swap='0' >
							<flowline >
								<command class_type='calculation' title='Calculation' >
									<exp exp='LCD_Data[.index] = LCD_Data[.index] | (1 &lt;&lt; .mod_y)' />
								</command>
							</flowline>
							<flowline >
								<command class_type='calculation' title='Calculation' >
									<exp exp='LCD_Data[.index] = LCD_Data[.index] &amp; ~ (1 &lt;&lt; .mod_y)' />
								</command>
							</flowline>
						</command>
						<command class_type='call' title='Write Page' macro='Write_Command' >
							<argument exp='0xB0 | .page' />
						</command>
						<command class_type='call' title='Call Macro' macro='Write_Command' >
							<argument exp='0x10 | (.X1 &gt;&gt; 4)' />
						</command>
						<command class_type='call' title='Call Macro' macro='Write_Command' >
							<argument exp='.X1 &amp; 0x0F' />
						</command>
						<command class_type='call' title='Call Macro' macro='Write_Data' >
							<argument exp='LCD_Data[.index]' />
						</command>
					</flowline>
				</macro>
