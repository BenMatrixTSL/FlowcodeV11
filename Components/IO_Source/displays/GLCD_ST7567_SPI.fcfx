											<exp exp='.sy = .sy + .Y' />
										</command>
										<command class_type='call' title='User Macro' macro='Plot' >
											<argument exp='.sx' />
											<argument exp='.sy' />
										</command>
									</flowline>
								</command>
							</flowline>
						</command>
					</flowline>
