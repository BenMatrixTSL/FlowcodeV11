							<exp exp='.b = (.b &amp; 0xF0) &gt;&gt; 4 | (.b &amp; 0x0F) &lt;&lt; 4' />
							<exp exp='.b = (.b &amp; 0xCC) &gt;&gt; 2 | (.b &amp; 0x33) &lt;&lt; 2' />
							<exp exp='.b = (.b &amp; 0xAA) &gt;&gt; 1 | (.b &amp; 0x55) &lt;&lt; 1' />
