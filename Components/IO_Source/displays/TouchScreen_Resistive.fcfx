				<macro >
					<flowline name='Calibrate' description='Performs a calibration routine which draws a circle at the top left of the screen 
followed by the bottom right of the screen, from these coordinates the component
can work out the corresponding X,Y pixel coordinate.' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
						<param name='Margin' type='u16' description='Margin from the edge of the display, Range 0-63' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Radius' type='u16' description='Radius of the calibration touch marker, should be &lt;= margin' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='idx' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='Temp' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<command class_type='calculation' title='Calculation' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' >
							<exp exp='.Margin = .Margin &amp; 0x003F' />
						</command>
						<command class_type='decision' title='Decision' cmdcolor='16098537' cmdcolor_sec='9519237' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' exp='.Radius &gt; .Margin' swap='0' >
							<flowline >
								<command class_type='calculation' title='Calculation' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' >
									<exp exp='.Radius = .Margin' />
								</command>
							</flowline>
							<flowline />
						</command>
						<command class_type='loop' title='Loop while LCD touched' cmdcolor='16098537' cmdcolor_sec='9519237' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' type='0' exp='IsTouched () = 1' >
							<flowline />
						</command>
						<command class_type='calculation' title='Touch Mapping Off' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' >
							<exp exp='LCD_TouchMapping = 0' />
						</command>
						<command class_type='call' title='User Macro' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' component='Glcd_Col_Dummy1' macro='ClearDisplay' />
						<command class_type='loop' title='Loop' cmdcolor='16098537' cmdcolor_sec='9519237' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' type='2' forvar='.idx' exp='2' >
							<flowline >
								<command class_type='delay' title='Delay' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' exp='1' type='2' />
								<command class_type='switch' title='Switch' textarea='36,25,36,25' cmdcolor='16098537' cmdcolor_sec='9519237' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' exp='.idx' >
									<case >
										<flowline >
											<command class_type='call' title='User Macro' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' component='Glcd_Col_Dummy1' macro='DrawEllipse' >
												<argument exp='.Margin' />
												<argument exp='.Margin' />
												<argument exp='.Radius' />
												<argument exp='.Radius' />
												<argument exp='0' />
												<argument exp='1' />
											</command>
										</flowline>
									</case>
									<case exp='1' >
										<flowline >
											<command class_type='decision' title='Decision' cmdcolor='16098537' cmdcolor_sec='9519237' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' exp='Orientation &amp; 0x01' swap='0' >
												<flowline >
													<command class_type='call' title='User Macro' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' component='Glcd_Col_Dummy1' macro='DrawEllipse' >
														<argument exp='YPixel - (.Margin + 1)' />
														<argument exp='XPixel - (.Margin + 1)' />
														<argument exp='.Radius' />
														<argument exp='.Radius' />
														<argument exp='0' />
														<argument exp='1' />
													</command>
												</flowline>
												<flowline >
													<command class_type='call' title='User Macro' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' component='Glcd_Col_Dummy1' macro='DrawEllipse' >
														<argument exp='XPixel - (.Margin + 1)' />
														<argument exp='YPixel - (.Margin + 1)' />
														<argument exp='.Radius' />
														<argument exp='.Radius' />
														<argument exp='0' />
														<argument exp='1' />
													</command>
												</flowline>
											</command>
										</flowline>
									</case>
								</command>
								<command class_type='loop' title='Loop while LCD not touched' cmdcolor='16098537' cmdcolor_sec='9519237' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' type='0' exp='IsTouched () = 0' >
									<flowline />
								</command>
								<command class_type='switch' title='Switch' textarea='36,25,36,25' cmdcolor='16098537' cmdcolor_sec='9519237' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' exp='.idx' >
									<case >
										<flowline >
											<command class_type='calculation' title='Min X/Y' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' >
												<exp exp='LCD_Touch_Calib[0] = LCD_Touch[0]' />
												<exp exp='LCD_Touch_Calib[2] = LCD_Touch[1]' />
											</command>
										</flowline>
									</case>
									<case exp='1' >
										<flowline >
											<command class_type='calculation' title='Max X/Y' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' >
												<exp exp='LCD_Touch_Calib[1] = LCD_Touch[0]' />
												<exp exp='LCD_Touch_Calib[3] = LCD_Touch[1]' />
											</command>
										</flowline>
									</case>
								</command>
								<command class_type='call' title='User Macro' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' component='Glcd_Col_Dummy1' macro='ClearDisplay' />
								<command class_type='loop' title='Loop while LCD touched' cmdcolor='16098537' cmdcolor_sec='9519237' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' type='0' exp='IsTouched () = 1' >
									<flowline />
								</command>
							</flowline>
						</command>
						<command class_type='decision' title='Decision' cmdcolor='16098537' cmdcolor_sec='9519237' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' exp='LCD_Touch_Calib[1] &lt; LCD_Touch_Calib[0]' swap='0' >
							<flowline >
								<command class_type='calculation' title='Calculation' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' >
									<exp exp='.Temp = LCD_Touch_Calib[0]' />
									<exp exp='LCD_Touch_Calib[0] = LCD_Touch_Calib[1]' />
									<exp exp='LCD_Touch_Calib[1] = .Temp' />
								</command>
								<command class_type='calculation' title='Flag X is flipped' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' >
									<exp exp='.Margin = .Margin | 0x40' />
								</command>
							</flowline>
							<flowline />
						</command>
						<command class_type='decision' title='Decision' cmdcolor='16098537' cmdcolor_sec='9519237' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' exp='LCD_Touch_Calib[3] &lt; LCD_Touch_Calib[2]' swap='0' >
							<flowline >
								<command class_type='calculation' title='Calculation' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' >
									<exp exp='.Temp = LCD_Touch_Calib[2]' />
									<exp exp='LCD_Touch_Calib[2] = LCD_Touch_Calib[3]' />
									<exp exp='LCD_Touch_Calib[3] = .Temp' />
								</command>
								<command class_type='calculation' title='Flag Y is flipped' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' >
									<exp exp='.Margin = .Margin | 0x80' />
								</command>
							</flowline>
							<flowline />
						</command>
						<command class_type='calculation' title='Touch Mapping On' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' >
							<exp exp='LCD_TouchMapping = 1' />
						</command>
					</flowline>
				</macro>
