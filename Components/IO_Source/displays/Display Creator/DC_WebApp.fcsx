				<macro >
					<flowline name='SetTextLabel' description='Draws a single line of text onto an object or Widget location.
Useful for labelling Widgets such as buttons or having name, value statistics.
Drawn using the object themes.' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
						<param name='ID' type='u16' description='Unique identifier of object or Widget.' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Text' type='T8' description='Text string to set as the label' isconst='0' isHidden='0' isinit='0' usrinit='&quot;&quot;' setinit='' >
							<array size='2000' />
						</param>
						<param name='Font' type='u8' description='GLCD Font Index to use' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='HAlignment' type='u8' description='0=Left, 1=Center, 2=Right' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='VAlignment' type='u8' description='0=Top, 1=Middle, 2=Bottom' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Transparent' type='b1' description='0=Background Colour Drawn, 1=Don&apos;t Draw Background Colour' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='ValidID' type='b1' description='' isconst='0' isHidden='0' isinit='1' usrinit='0' setinit='0' />
						<local name='PixelHeight' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='Y' type='s16' description='' isconst='0' isHidden='0' isinit='1' usrinit='0' setinit='0' />
						<local name='Idx' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='Len' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='X' type='s16' description='' isconst='0' isHidden='0' isinit='1' usrinit='0' setinit='0' />
						<local name='TextPixelWidth' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='char' type='s32' description='' isconst='0' isHidden='0' isinit='1' usrinit='0' setinit='0' />
						<local name='IsWidget' type='b1' description='' isconst='0' isHidden='0' isinit='1' usrinit='0' setinit='0' />
						<command class_type='decision' title='Check ID is valid' exp='(.ID &gt; 0) &amp;&amp; (.ID &lt;= NumObjects)' swap='0' >
							<flowline >
								<command class_type='calculation' title='Calculation' >
