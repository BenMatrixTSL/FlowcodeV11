									<exp exp='WidgetColour[NumWidgets][2][1] = .Colours[7]' />
									<exp exp='WidgetColour[NumWidgets][2][2] = .Colours[8]' />
									<exp exp='WidgetColour[NumWidgets][3][0] = .Colours[9]' />
									<exp exp='WidgetColour[NumWidgets][3][1] = .Colours[10]' />
									<exp exp='WidgetColour[NumWidgets][3][2] = .Colours[11]' />
									<exp exp='DrawHighlight[NumWidgets] = .DrawHighlight' />
									<exp exp='Value[NumWidgets] = 0.0' />
									<exp exp='OldValue[NumWidgets] = 2.0' />
								</command>
								<command class_type='calculation' title='Increment Number Of Widgets' >
									<exp exp='NumWidgets = NumWidgets + 1' />
								</command>
								<command class_type='calculation' title='Calculation' >
									<exp exp='.Return = MaxObjects + NumWidgets' />
								</command>
								<command class_type='decision' title='Decision' exp='.DrawWidget' swap='0' >
