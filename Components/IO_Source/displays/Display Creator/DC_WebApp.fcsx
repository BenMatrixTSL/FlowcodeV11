						<param name='Font' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='CharacterIndex' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='width' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='FirstChar' type='s32' description='' isconst='0' isHidden='0' isinit='1' usrinit='0' setinit='0' />
						<command class_type='calculation' title='Calculation' >
							<exp exp='.Return = 0' />
						</command>
						<command class_type='native' title='JavaScript' ccode='FCL_FIRSTCHAR = font_firstchar[FCL_FONT];' language='C' />
						<command class_type='decision' title='Decision' exp='(.font = 0) || (.font = 3)' swap='0' >
							<flowline >
								<command class_type='calculation' title='Calculation' >
									<exp exp='.CharacterIndex = .CharacterIndex - .FirstChar' />
								</command>
								<command class_type='decision' title='Decision' exp='.font = 3' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='.return = .CharacterIndex * 17' />
										</command>
									</flowline>
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='.return = .CharacterIndex * 10' />
										</command>
									</flowline>
								</command>
							</flowline>
							<flowline >
								<command class_type='loop' title='Loop' type='0' exp='.CharacterIndex &gt; .FirstChar' >
									<flowline >
										<command class_type='calculation' title='Pre-Decrement' >
											<exp exp='.CharacterIndex = .CharacterIndex - 1' />
										</command>
										<command class_type='call' title='Component Macro' macro='ReadFontWidth' >
											<return exp='.width' />
											<argument exp='.font' />
											<argument exp='.CharacterIndex' />
										</command>
										<command class_type='calculation' title='Calculation' >
											<exp exp='.Return = .Return + .width' />
										</command>
									</flowline>
								</command>
							</flowline>
						</command>
						<command class_type='native' title='JavaScript' ccode='FCR_RETVAL = FCR_RETVAL * font_bytespercolumn[FCL_FONT];' language='C' />
					</flowline>
				</macro>
