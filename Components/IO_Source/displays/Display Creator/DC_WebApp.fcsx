										<command class_type='native' title='Check for valid Base64 encoded data - parse if ok' ccode='if (base64regex.test(FCL_MESSAGE))
    FCV_MQTTCOUNT = base64ToArrayBuffer(FCL_MESSAGE);
else FCV_MQTTCOUNT = 0;' language='C' />
										<command class_type='decision' title='Decision' exp='mqttcount' swap='0' >
											<flowline >
												<command class_type='calculation' title='Calculation' >
													<exp exp='.msg = &quot;&quot;' />
												</command>
												<command class_type='loop' title='Loop' type='2' forvar='.Index' exp='mqttcount' >
													<flowline >
														<command class_type='calculation' title='Calculation' >
															<exp exp='.msg = .msg + ToString$(mqttdata[.index]) + &quot;, &quot;' />
