												</command>
											</flowline>
											<flowline >
												<command class_type='decision' title='Decision' exp='LineWIdth[.ID] &gt; 1' swap='0' >
													<flowline >
														<command class_type='calculation' title='Calculation' >
															<exp exp='.idx = 0 - (LineWidth[.ID] &gt;&gt; 1)' />
														</command>
														<command class_type='loop' title='Loop' type='2' exp='LineWidth[.ID] - 1' >
															<flowline >
																<command class_type='call' title='Component Macro' macro='CanvasDrawRectangle' >
																	<argument exp='X[.ID] + .idx' />
																	<argument exp='Y[.ID] + .idx' />
																	<argument exp='X[.ID] + W[.ID] - .idx' />
																	<argument exp='Y[.ID] + H[.ID] - .idx' />
																	<argument exp='1' />
																	<argument exp='0' />
																</command>
																<command class_type='calculation' title='Calculation' >
																	<exp exp='.Idx = .Idx + 1' />
																</command>
															</flowline>
														</command>
													</flowline>
													<flowline />
