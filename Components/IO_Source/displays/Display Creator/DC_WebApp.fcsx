													<argument exp='.Index' />
												</command>
											</flowline>
											<flowline />
										</command>
										<command class_type='call' title='Component Macro' component='JSONDecoder1' macro='FindName' >
											<return exp='.Index' />
											<argument exp='&quot;BU&quot;' />
											<argument exp='0' />
										</command>
										<command class_type='decision' title='Decision' exp='.Index &gt;= 0' swap='0' >
											<flowline >
