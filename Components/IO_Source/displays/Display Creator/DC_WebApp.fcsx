									<exp exp='Type[MaxObjects + NumWidgets] = .WidgetType' />
									<exp exp='X[MaxObjects + NumWidgets] = .X' />
									<exp exp='Y[MaxObjects + NumWidgets] = .Y' />
									<exp exp='W[MaxObjects + NumWidgets] = .Width' />
									<exp exp='H[MaxObjects + NumWidgets] = .Height' />
									<exp exp='LineWidth[MaxObjects + NumWidgets] = .LineWidth' />
									<exp exp='WidgetColour[NumWidgets][0][0] = .Colours[0]' />
									<exp exp='WidgetColour[NumWidgets][0][1] = .Colours[1]' />
									<exp exp='WidgetColour[NumWidgets][0][2] = .Colours[2]' />
