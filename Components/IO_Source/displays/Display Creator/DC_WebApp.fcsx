												<command class_type='call' title='User Macro' macro='CanvasDrawLine' >
													<argument exp='.xx1' />
													<argument exp='.Y1 + .y + .Radius' />
													<argument exp='.oxx1' />
													<argument exp='.Y1 + .y + .Radius' />
