				<macro >
					<flowline name='CanvasPrint' description='' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
						<param name='Str' type='T8' description='' isconst='0' isHidden='0' isinit='0' usrinit='&quot;&quot;' setinit='' >
							<array size='2000' />
						</param>
						<param name='X1' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Y1' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Font' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Transparent' type='b1' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='BytesPerColumn' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='CharacterColumns' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='CharacterHeight' type='s32' description='' isconst='0' isHidden='0' isinit='1' usrinit='0' setinit='0' />
						<local name='ColumnIndex' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='ColumnSubIndex' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='PixelData' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='SpaceColumns' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='idx' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='len_str' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='xcount' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='ycount' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='ypix' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='character' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<command class_type='calculation' title='Calculation' >
							<exp exp='.xcount = 0' />
							<exp exp='.idx = 0' />
						</command>
						<command class_type='calculation' title='Calculation' >
							<exp exp='.len_str = Length$ (.Str)' />
						</command>
						<command class_type='native' title='JavaScript' disable='1' ccode='let result = Array.from(FCL_STR, char =&gt; char.charCodeAt(0));' language='C' />
						<command class_type='call' title='Call Macro' macro='ReadFontStat' >
							<return exp='.SpaceColumns' />
							<argument exp='.Font' />
							<argument exp='0' />
						</command>
						<command class_type='call' title='Call Macro' macro='ReadFontStat' >
							<return exp='.CharacterHeight' />
							<argument exp='.Font' />
							<argument exp='1' />
						</command>
						<command class_type='decision' title='Decision' exp='ScaleY &gt; 1' swap='0' >
							<flowline >
								<command class_type='calculation' title='Calculation' >
									<exp exp='.CharacterHeight = .CharacterHeight / ScaleY' />
								</command>
							</flowline>
							<flowline />
						</command>
						<command class_type='calculation' title='Calculate character height' >
							<exp exp='.BytesPerColumn = ((.CharacterHeight - 1) &gt;&gt; 3) + 1' />
						</command>
						<command class_type='loop' title='Loop through characters in string' type='0' exp='.idx &lt; .len_str' >
							<flowline >
								<command class_type='native' title='JavaScript' disable='1' ccode='FCL_CHARACTER = result[FCL_IDX];' language='C' />
								<command class_type='calculation' title='Calculation' >
									<exp exp='.character = Char$(.Str,.idx)' />
								</command>
								<command class_type='call' title='Call Macro' macro='ReadFontWidth' >
									<return exp='.CharacterColumns' />
									<argument exp='.Font' />
									<argument exp='.character' />
								</command>
								<command class_type='decision' title='Decision' exp='ScaleX &gt; 1' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='.CharacterColumns = .CharacterColumns / ScaleX' />
										</command>
									</flowline>
									<flowline />
								</command>
								<command class_type='calculation' title='Calculation' >
									<exp exp='.ColumnSubIndex = 0' />
								</command>
								<command class_type='loop' title='Loop' type='0' exp='.ColumnSubIndex &lt; .BytesPerColumn' >
									<flowline >
										<command class_type='decision' title='Decision' exp='.ColumnSubIndex' swap='0' >
											<flowline >
												<command class_type='calculation' title='Calculation' >
													<exp exp='.xcount = .xcount - ((.CharacterColumns + .SpaceColumns) * ScaleX)' />
												</command>
											</flowline>
											<flowline />
										</command>
										<command class_type='calculation' title='Calculation' >
											<exp exp='.ColumnIndex = 0' />
										</command>
										<command class_type='loop' title='Loop' type='0' exp='.ColumnIndex &lt; (.CharacterColumns + .SpaceColumns)' >
											<flowline >
												<command class_type='loop' title='Loop' type='2' exp='ScaleX' >
													<flowline >
														<command class_type='decision' title='Decision' exp='.ColumnIndex &lt; .CharacterColumns' swap='0' >
															<flowline >
																<command class_type='call' title='Call Macro' macro='ReadFontByte' >
																	<return exp='.PixelData' />
																	<argument exp='.Font' />
																	<argument exp='.character' />
																	<argument exp='.ColumnIndex + (.CharacterColumns * .ColumnSubIndex)' />
																</command>
															</flowline>
															<flowline >
																<command class_type='calculation' title='Character Spacing' >
																	<exp exp='.PixelData = 0' />
																</command>
															</flowline>
														</command>
														<command class_type='calculation' title='Calculation' >
															<exp exp='.ycount = (.ColumnSubIndex * 8) * ScaleY' />
														</command>
														<command class_type='decision' title='Decision' exp='((.ColumnSubIndex + 1) * 8) &lt; .CharacterHeight' swap='0' >
															<flowline >
																<command class_type='calculation' title='Calculation' >
																	<exp exp='.ypix = 0' />
																</command>
															</flowline>
															<flowline >
																<command class_type='calculation' title='Calculation' >
																	<exp exp='.ypix = ((.ColumnSubIndex + 1) * 8) - .CharacterHeight' />
																</command>
															</flowline>
														</command>
														<command class_type='loop' title='Loop' type='0' exp='.ypix &lt; 8' >
															<flowline >
																<command class_type='loop' title='Loop' type='2' exp='ScaleY' >
																	<flowline >
																		<command class_type='decision' title='Decision' exp='.PixelData &amp; (0x01 &lt;&lt; .ypix)' swap='0' >
																			<flowline >
																				<command class_type='call' title='Call Macro' macro='CanvasPlot' >
																					<argument exp='.X1 + .xcount' />
																					<argument exp='.Y1 + .ycount' />
																				</command>
																			</flowline>
																			<flowline >
																				<command class_type='decision' title='Decision' exp='.Transparent = 0' swap='0' >
																					<flowline >
																						<command class_type='call' title='Call Macro' macro='CanvasCanvasBPlot' >
																							<argument exp='.X1 + .xcount' />
																							<argument exp='.Y1 + .ycount' />
																						</command>
																					</flowline>
																					<flowline />
																				</command>
																			</flowline>
																		</command>
																		<command class_type='calculation' title='Calculation' >
																			<exp exp='.ycount = .ycount + 1' />
																		</command>
																	</flowline>
																</command>
																<command class_type='calculation' title='Calculation' >
																	<exp exp='.ypix = .ypix + 1' />
																	<exp exp='' />
																</command>
															</flowline>
														</command>
														<command class_type='calculation' title='Calculation' >
															<exp exp='.xcount = .xcount + 1' />
														</command>
													</flowline>
												</command>
												<command class_type='calculation' title='Calculation' >
													<exp exp='.ColumnIndex = .ColumnIndex + 1' />
												</command>
											</flowline>
										</command>
										<command class_type='calculation' title='Calculation' >
											<exp exp='.ColumnSubIndex = .ColumnSubIndex + 1' />
										</command>
									</flowline>
								</command>
								<command class_type='calculation' title='Calculation' >
									<exp exp='.idx = .idx + 1' />
								</command>
							</flowline>
						</command>
					</flowline>
				</macro>
				<macro >
					<flowline name='ModifyScale' description='Changes the width and height of the selected Object or Widget.' statediag='0' >
