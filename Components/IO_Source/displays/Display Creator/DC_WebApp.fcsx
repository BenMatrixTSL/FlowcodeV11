									<argument exp='ObjectColour[.ID][1][0]' />
									<argument exp='ObjectColour[.ID][1][1]' />
									<argument exp='ObjectColour[.ID][1][2]' />
								</command>
								<command class_type='decision' title='Decision' exp='Type[.ID] = Object_Hidden' swap='0' >
									<flowline />
									<flowline />
								</command>
								<command class_type='decision' title='Decision' exp='Type[.ID] = Object_Rectangle' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='.Idx = 0' />
										</command>
										<command class_type='decision' title='Decision' exp='LineWidth[.ID] == 0' swap='0' >
											<flowline >
												<command class_type='call' title='Component Macro' macro='SetForegroundColour' >
													<argument exp='ObjectColour[.ID][1][0]' />
													<argument exp='ObjectColour[.ID][1][1]' />
