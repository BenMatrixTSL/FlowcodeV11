													<exp exp='ObjectColour[.ID][.Index][0] = .R' />
													<exp exp='ObjectColour[.ID][.Index][1] = .G' />
													<exp exp='ObjectColour[.ID][.Index][2] = .B' />
												</command>
											</flowline>
											<flowline />
										</command>
									</flowline>
