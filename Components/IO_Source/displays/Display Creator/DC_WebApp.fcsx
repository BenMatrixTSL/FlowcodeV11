						<command class_type='decision' title='Decision' exp='Length$(.Message)' swap='0' >
							<flowline >
								<command class_type='decision' title='Decision' exp='.Topic = Topic_Setup' swap='0' >
									<flowline >
										<command class_type='call' title='Component Macro' component='JSONDecoder1' macro='ParseJSON' >
											<argument exp='.Message' />
										</command>
										<command class_type='call' title='Component Macro' component='JSONDecoder1' macro='FindName' >
