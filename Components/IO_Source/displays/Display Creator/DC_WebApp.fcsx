				<macro >
					<flowline name='CanvasDrawRectangle' description='' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
						<param name='X1' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Y1' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='X2' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Y2' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Transparent' type='b1' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Solid' type='b1' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='Xmax' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='Xmin' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='Ymax' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='Ymin' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<command class_type='call' title='Call Macro' macro='CanvasDrawLine' >
							<argument exp='.X1' />
							<argument exp='.Y1' />
							<argument exp='.X2' />
							<argument exp='.Y1' />
						</command>
						<command class_type='call' title='Call Macro' macro='CanvasDrawLine' >
							<argument exp='.X2' />
							<argument exp='.Y1' />
							<argument exp='.X2' />
							<argument exp='.Y2' />
						</command>
						<command class_type='call' title='Call Macro' macro='CanvasDrawLine' >
							<argument exp='.X1' />
							<argument exp='.Y2' />
							<argument exp='.X2' />
							<argument exp='.Y2' />
						</command>
						<command class_type='call' title='Call Macro' macro='CanvasDrawLine' >
							<argument exp='.X1' />
							<argument exp='.Y1' />
							<argument exp='.X1' />
							<argument exp='.Y2' />
						</command>
						<command class_type='decision' title='Decision' exp='.Transparent = 0' swap='0' >
							<flowline >
								<command class_type='decision' title='Decision' exp='.X1 &gt; .X2' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='.Xmax = .X1' />
											<exp exp='.Xmin = .X2 + 1' />
										</command>
									</flowline>
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='.Xmax = .X2' />
											<exp exp='.Xmin = .X1 + 1' />
										</command>
									</flowline>
								</command>
								<command class_type='decision' title='Decision' exp='.Y1 &gt; .Y2' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='.Ymax = .Y1' />
											<exp exp='.Ymin = .Y2 + 1' />
										</command>
									</flowline>
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='.Ymax = .Y2' />
											<exp exp='.Ymin = .Y1 + 1' />
										</command>
									</flowline>
								</command>
								<command class_type='calculation' title='Calculation' >
									<exp exp='.Y1 = .Ymin' />
								</command>
								<command class_type='loop' title='Loop' type='0' exp='.Y1 &lt; .YMax' >
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='.X1 = .Xmin' />
										</command>
										<command class_type='loop' title='Loop' type='0' exp='.X1 &lt; .XMax' >
											<flowline >
												<command class_type='decision' title='Decision' exp='.Solid' swap='0' >
													<flowline >
														<command class_type='call' title='Call Macro' macro='CanvasPlot' >
															<argument exp='.X1' />
															<argument exp='.Y1' />
														</command>
													</flowline>
													<flowline >
														<command class_type='call' title='Call Macro' macro='CanvasCanvasBPlot' >
															<argument exp='.X1' />
															<argument exp='.Y1' />
														</command>
													</flowline>
												</command>
												<command class_type='calculation' title='Calculation' >
													<exp exp='.X1 = .X1 + 1' />
												</command>
											</flowline>
										</command>
										<command class_type='calculation' title='Calculation' >
											<exp exp='.Y1 = .Y1 + 1' />
										</command>
									</flowline>
								</command>
							</flowline>
							<flowline />
						</command>
					</flowline>
				</macro>
				<macro >
					<flowline name='CanvasDrawLine' description='' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
						<param name='x1' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='y1' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='x2' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='y2' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='C1' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='D1' type='s32' description='' isconst='0' isHidden='0' isinit='1' usrinit='0' setinit='0' />
						<local name='M1' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='PixelX' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='PixelY' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='Xinc' type='s32' description='' isconst='0' isHidden='0' isinit='1' usrinit='1' setinit='1' />
						<local name='Yinc' type='s32' description='' isconst='0' isHidden='0' isinit='1' usrinit='1' setinit='1' />
						<command class_type='calculation' title='Calculation' >
							<exp exp='.X1 = Floor(.X1)' />
							<exp exp='.X2 = Floor(.X2)' />
							<exp exp='.Y1 = Floor(.Y1)' />
							<exp exp='.Y2 = Floor(.Y2)' />
						</command>
						<command class_type='calculation' title='Calculation' >
							<exp exp='.PixelX = .X2 - .X1' />
							<exp exp='.PixelY = .Y2 - .Y1' />
						</command>
						<command class_type='decision' title='If negative X direction' exp='.PixelX &lt; 0' swap='0' >
							<flowline >
								<command class_type='calculation' title='Calculation' >
									<exp exp='.Xinc = -1' />
									<exp exp='.PixelX = .PixelX * -1' />
								</command>
							</flowline>
							<flowline />
						</command>
						<command class_type='decision' title='If negative Y direction' exp='.PixelY &lt; 0' swap='0' >
							<flowline >
								<command class_type='calculation' title='Calculation' >
									<exp exp='.Yinc = -1' />
									<exp exp='.PixelY = .PixelY * -1' />
								</command>
							</flowline>
							<flowline />
						</command>
						<command class_type='decision' title='Decision' exp='.PixelY &lt;= .PixelX' swap='0' >
							<flowline >
								<command class_type='calculation' title='Calculation' >
									<exp exp='.C1 = 2 * .PixelX' />
									<exp exp='.M1 = 2 * .PixelY' />
								</command>
								<command class_type='loop' title='Loop' type='0' exp='.X1 != .X2' >
									<flowline >
										<command class_type='call' title='Call Macro' macro='CanvasPlot' >
											<argument exp='.X1' />
											<argument exp='.Y1' />
										</command>
										<command class_type='calculation' title='Calculation' >
											<exp exp='.X1 = .X1 + .Xinc' />
										</command>
										<command class_type='calculation' title='Calculation' >
											<exp exp='.D1 = .D1 + .M1' />
										</command>
										<command class_type='decision' title='Decision' exp='.D1 &gt; .PixelX' swap='0' >
											<flowline >
												<command class_type='calculation' title='Calculation' >
													<exp exp='.Y1 = .Y1 + .Yinc' />
												</command>
												<command class_type='calculation' title='Calculation' >
													<exp exp='.D1 = .D1 - .C1' />
												</command>
											</flowline>
											<flowline />
										</command>
									</flowline>
								</command>
							</flowline>
							<flowline >
								<command class_type='calculation' title='Calculation' >
									<exp exp='.C1 = 2 * .PixelY' />
									<exp exp='.M1 = 2 * .PixelX' />
								</command>
								<command class_type='loop' title='Loop' type='0' exp='.Y1 != .Y2' >
									<flowline >
										<command class_type='call' title='Call Macro' macro='CanvasPlot' >
											<argument exp='.X1' />
											<argument exp='.Y1' />
										</command>
										<command class_type='calculation' title='Calculation' >
											<exp exp='.Y1 = .Y1 + .Yinc' />
										</command>
										<command class_type='calculation' title='Calculation' >
											<exp exp='.D1 = .D1 + .M1' />
										</command>
										<command class_type='decision' title='Decision' exp='.D1 &gt; .PixelY' swap='0' >
											<flowline >
												<command class_type='calculation' title='Calculation' >
													<exp exp='.X1 = .X1 + .Xinc' />
												</command>
												<command class_type='calculation' title='Calculation' >
													<exp exp='.D1 = .D1 - .C1' />
												</command>
											</flowline>
											<flowline />
										</command>
									</flowline>
								</command>
							</flowline>
						</command>
						<command class_type='call' title='Call Macro' macro='CanvasPlot' >
							<argument exp='.X1' />
							<argument exp='.Y1' />
						</command>
					</flowline>
				</macro>
				<macro >
					<flowline name='ModifyPosition' description='Moves an existing object or Widget to absolute coordinates on the screen.' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
						<param name='ID' type='u16' description='Unique ID of the object or Widget' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='X' type='s16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Y' type='s16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Relative' type='b1' description='0=Absolute Positioning, 1=Relative Positioning' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<command class_type='decision' title='Decision' exp='.ID &gt; 0' swap='0' >
							<flowline >
								<command class_type='calculation' title='Change to 0 based ID' >
									<exp exp='.ID = .ID - 1' />
								</command>
								<command class_type='decision' title='Check ID is valid' exp='.ID &lt; NumObjects' swap='0' >
									<flowline >
										<command class_type='decision' title='Decision' exp='.Relative' swap='0' >
											<flowline >
												<command class_type='calculation' title='Update Coordinates' >
													<exp exp='X[.ID] = X[.ID] + .X' />
													<exp exp='Y[.ID] = Y[.ID] + .Y' />
												</command>
											</flowline>
											<flowline >
												<command class_type='calculation' title='Update Coordinates' >
													<exp exp='X[.ID] = .X' />
													<exp exp='Y[.ID] = .Y' />
												</command>
											</flowline>
										</command>
									</flowline>
									<flowline />
								</command>
								<command class_type='decision' title='Check ID is valid' exp='(.ID &gt;= MaxObjects) &amp;&amp; (.ID &lt; (MaxObjects + NumWidgets))' swap='0' >
									<flowline >
										<command class_type='decision' title='Decision' exp='.Relative' swap='0' >
											<flowline >
												<command class_type='calculation' title='Update Coordinates' >
													<exp exp='X[.ID] = X[.ID] + .X' />
													<exp exp='Y[.ID] = Y[.ID] + .Y' />
												</command>
											</flowline>
											<flowline >
												<command class_type='calculation' title='Update Coordinates' >
													<exp exp='X[.ID] = .X' />
													<exp exp='Y[.ID] = .Y' />
												</command>
											</flowline>
										</command>
									</flowline>
									<flowline />
								</command>
							</flowline>
							<flowline />
						</command>
					</flowline>
				</macro>
