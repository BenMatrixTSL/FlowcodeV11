				<macro >
					<flowline name='CreateWidgetArray' description='Creates a 2d array of buttons that can be used to create things like keypads.
Returns the ID of the first button, the following buttons have incremental IDs going column by column and then row by row.' statediag='0' >
						<return name='Return' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='WidgetType' type='u8' description='0=WidgetHidden, 1=WidgetRect, 2=WidgetRoundedRect, 3=WidgetEllipse, 4=VSlider, 5=HSlider, 6=VSliderRound, 7=HSliderRound' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='NumColumns' type='u8' description='Number of columns of buttons in the array' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='NumRows' type='u8' description='Number of rows of buttons in the array' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='InitialPositionX' type='u16' description='X location of the initial button' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='InitialPositionY' type='u16' description='Y location of the initial button' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='WidgetWidth' type='u16' description='Widget width in pixels' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='WidgetHeight' type='u16' description='Widget height in pixels' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='SpacingX' type='u16' description='Horizontal spacing between the widgets' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='SpacingY' type='u16' description='Vertical spacing between the widgets' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='LineWidth' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Colours' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' >
							<array size='12' />
						</param>
						<param name='Font' type='u8' description='Font used for the widgets text' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='WidgetText' type='T8*' description='A comma seperated text field with the text for each widget e.g. &quot;1,2,3,..&quot;' isconst='0' isHidden='0' isinit='0' usrinit='&quot;&quot;' setinit='' >
							<array size='1000' />
						</param>
						<param name='DrawHighlight' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='Row' type='u8' description='' isconst='0' isHidden='0' isinit='1' usrinit='0' setinit='0' />
						<local name='Col' type='u8' description='' isconst='0' isHidden='0' isinit='1' usrinit='0' setinit='0' />
						<local name='WY' type='u16' description='' isconst='0' isHidden='0' isinit='1' usrinit='0' setinit='0' />
						<local name='WX' type='u16' description='' isconst='0' isHidden='0' isinit='1' usrinit='0' setinit='0' />
						<local name='Count' type='u8' description='' isconst='0' isHidden='0' isinit='1' usrinit='0' setinit='0' />
						<local name='ID' type='u16' description='' isconst='0' isHidden='0' isinit='1' usrinit='0' setinit='0' />
						<local name='TmpStr' type='T8' description='' isconst='0' isHidden='0' isinit='0' usrinit='&quot;&quot;' setinit='' >
							<array size='10' />
						</local>
						<local name='Len' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<command class_type='calculation' title='Calculation' >
							<exp exp='.Return = 0' />
						</command>
						<command class_type='loop' title='Loop' type='2' exp='.NumRows' >
							<flowline >
								<command class_type='calculation' title='Calculation' >
									<exp exp='.Col = 0' />
								</command>
								<command class_type='loop' title='Loop' type='2' exp='.NumColumns' >
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='.WX = .InitialPositionX + .Col * (.WidgetWidth + .SpacingY)' />
											<exp exp='.WY = .InitialPositionY + .Row * (.WidgetHeight + .SpacingX)' />
										</command>
										<command class_type='call' title='User Macro' macro='CreateWidget' >
											<return exp='.ID' />
											<argument exp='.WidgetType' />
											<argument exp='.WX' />
											<argument exp='.WY' />
											<argument exp='.WidgetWidth' />
											<argument exp='.WidgetHeight' />
											<argument exp='.LineWidth' />
											<argument exp='.Colours' />
											<argument exp='.DrawHighlight' />
											<argument exp='1' />
										</command>
										<command class_type='decision' title='Decision' exp='.Count' swap='0' >
											<flowline />
											<flowline >
												<command class_type='calculation' title='Return first widget ID' >
													<exp exp='.Return = .ID' />
												</command>
											</flowline>
										</command>
										<command class_type='calculation' title='Calculation' >
											<exp exp='.TmpStr = SplitString$(.WidgetText,&quot;,&quot;,.Count)' />
											<exp exp='.Len = Length$(.TmpStr)' />
										</command>
										<command class_type='decision' title='Text label to print?' exp='.Len' swap='0' >
											<flowline >
												<command class_type='call' title='User Macro' macro='SetTextLabel' >
													<argument exp='.ID' />
													<argument exp='.TmpStr' />
													<argument exp='.Font' />
													<argument exp='1' />
													<argument exp='1' />
													<argument exp='1' />
												</command>
											</flowline>
											<flowline />
										</command>
										<command class_type='calculation' title='Calculation' >
											<exp exp='.Count = .Count + 1' />
											<exp exp='.Col = .Col + 1' />
										</command>
									</flowline>
								</command>
								<command class_type='calculation' title='Calculation' >
									<exp exp='.Row = .Row + 1' />
								</command>
							</flowline>
						</command>
