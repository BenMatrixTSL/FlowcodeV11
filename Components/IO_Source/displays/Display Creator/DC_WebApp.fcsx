				<macro >
					<flowline name='ModifyColour' description='Overrides the colour of an object theme, used by the draw object macro. ' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
						<param name='ID' type='s32' description='Unique ID of the object or Widget' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Index' type='s32' description='0=Foreground, 1=Background, 2=TextColour, 3=Highlight(Widgets Only)' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='R' type='s32' description='Foreground colour channel value' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='G' type='s32' description='Foreground colour channel value' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='B' type='s32' description='Foreground colour channel value' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Redraw' type='b1' description='Redraws the component straight away in the new colours' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<command class_type='decision' title='Decision' exp='.ID &gt; 0' swap='0' >
							<flowline >
								<command class_type='calculation' title='Change to 0 based ID' >
									<exp exp='.ID = .ID - 1' />
								</command>
								<command class_type='decision' title='Check ID is valid' exp='.ID &lt; NumObjects' swap='0' >
									<flowline >
