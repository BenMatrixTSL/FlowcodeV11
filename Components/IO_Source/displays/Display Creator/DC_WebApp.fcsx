														<command class_type='call' title='Call Macro' macro='CanvasPlot' >
															<argument exp='.X + (.CurX / .K)' />
															<argument exp='.Y + .CurY' />
														</command>
														<command class_type='call' title='Call Macro' macro='CanvasPlot' >
															<argument exp='.X + (.CurX / .K)' />
															<argument exp='.Y - .CurY' />
														</command>
														<command class_type='call' title='Call Macro' macro='CanvasPlot' >
															<argument exp='.X - (.CurX / .K)' />
															<argument exp='.Y - .CurY' />
														</command>
														<command class_type='calculation' title='Calculation' >
															<exp exp='.e2 = .err' />
														</command>
														<command class_type='decision' title='Decision' exp='.e2 &lt;= .CurX' swap='0' >
															<flowline >
																<command class_type='calculation' title='Calculation' >
																	<exp exp='.CurX = .CurX + 1' />
																	<exp exp='.err = .err + ((.CurX * 2) + 1)' />
																	<exp exp='' />
																</command>
																<command class_type='decision' title='Decision' exp='(0 - .CurY == .CurX) &amp;&amp; (.e2 &lt;= .CurY)' swap='0' >
																	<flowline >
																		<command class_type='calculation' title='Calculation' >
																			<exp exp='.e2 = 0' />
																		</command>
																	</flowline>
																	<flowline />
																</command>
															</flowline>
															<flowline />
														</command>
														<command class_type='decision' title='Decision' exp='.e2 &gt; .CurY' swap='0' >
															<flowline >
																<command class_type='calculation' title='Calculation' >
																	<exp exp='.CurY = .CurY + 1' />
																	<exp exp='.err = .err + ((.CurY * 2) + 1)' />
																	<exp exp='' />
																</command>
															</flowline>
															<flowline />
														</command>
													</flowline>
												</command>
											</flowline>
											<flowline />
										</command>
									</flowline>
								</command>
								<command class_type='decision' title='Decision' exp='.Rep = 1' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='.Rep = .Rep + 1' />
											<exp exp='.Solid = 0' />
											<exp exp='.Transparent = 1' />
										</command>
										<command class_type='calculation' title='Calculation' >
											<exp exp='fgColour = .FGCol' />
										</command>
										<command class_type='calculation' title='Calculation' >
											<exp exp='.Done = .Done + 1' />
										</command>
									</flowline>
									<flowline />
								</command>
								<command class_type='calculation' title='Calculation' >
									<exp exp='.Done = .Done - 1' />
								</command>
							</flowline>
						</command>
					</flowline>
				</macro>
				<macro >
					<flowline name='ModifyBitmap' description='Allows the bitmap image to be swapped out' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
						<param name='ID' type='u16' description='Object or Widget ID to update' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Filename' type='T8' description='Bitmap filename in 8.3 file format' isconst='0' isHidden='0' isinit='0' usrinit='&quot;&quot;' setinit='' >
							<array size='20' />
						</param>
						<param name='Scaling' type='b1' description='0=Native, 1=Stretch' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Transparancy' type='b1' description='0=Disabled, 1=Enabled' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Redraw' type='b1' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<command class_type='calculation' title='Calculation' >
							<exp exp='BitmapFilename = .Filename' />
						</command>
						<command class_type='decision' title='Check ID is valid' exp='(.ID &gt; 0) &amp;&amp; (.ID &lt;= NumObjects)' swap='0' >
							<flowline >
								<command class_type='calculation' title='Return to 0 indexed ID' >
									<exp exp='.ID = .ID - 1' />
								</command>
								<command class_type='calculation' title='Calculation' >
									<exp exp='ObjectColour[.ID][0][0] = .Scaling' />
									<exp exp='ObjectColour[.ID][0][1] = .Transparancy' />
								</command>
								<command class_type='decision' title='Decision' exp='.Redraw' swap='0' >
									<flowline >
										<command class_type='call' title='User Macro' macro='DrawItem' >
											<argument exp='.ID + 1' />
										</command>
									</flowline>
									<flowline />
								</command>
							</flowline>
							<flowline />
						</command>
						<command class_type='decision' title='Check ID is valid' exp='(.ID &gt; MaxObjects) &amp;&amp; (.ID &lt;= (MaxObjects + NumWidgets))' swap='0' >
							<flowline >
								<command class_type='calculation' title='Return to 0 indexed ID' >
									<exp exp='.ID = .ID - 1' />
								</command>
								<command class_type='calculation' title='Calculation' >
									<exp exp='WidgetColour[.ID - MaxObjects][0][0] = .Scaling' />
									<exp exp='WidgetColour[.ID - MaxObjects][0][1] = .Transparancy' />
								</command>
								<command class_type='decision' title='Decision' exp='.Redraw' swap='0' >
									<flowline >
										<command class_type='call' title='User Macro' macro='DrawItem' >
											<argument exp='.ID + 1' />
										</command>
									</flowline>
									<flowline />
								</command>
							</flowline>
							<flowline />
						</command>
					</flowline>
				</macro>
