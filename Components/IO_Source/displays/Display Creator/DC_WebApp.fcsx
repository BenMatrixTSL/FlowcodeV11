				<macro >
					<flowline name='DrawItem' description='Draws a single object or Widget onto the display without clearing the display.
Doesn&apos;t draw text, this is done using the SetText macros.' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
						<param name='ID' type='u16' description='Unique ID of the Object or Widget' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='PixelWidth' type='u16' description='' isconst='0' isHidden='0' isinit='1' usrinit='0' setinit='0' />
						<local name='PixelHeight' type='u16' description='' isconst='0' isHidden='0' isinit='1' usrinit='0' setinit='0' />
						<local name='Difference' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='Idx' type='s32' description='' isconst='0' isHidden='0' isinit='1' usrinit='0' setinit='0' />
						<command class_type='decision' title='Check ID is valid' exp='(.ID &gt; 0) &amp;&amp; (.ID &lt;= NumObjects)' swap='0' >
							<flowline >
								<command class_type='calculation' title='Return to 0 indexed ID' >
									<exp exp='.ID = .ID - 1' />
