				<macro >
					<flowline name='ReadFontByte' description='' statediag='0' >
						<return name='Return' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Font' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Character' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='ByteIndex' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='position' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='firstchar' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='charcount' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<command class_type='native' title='JavaScript' ccode='FCL_FIRSTCHAR = font_firstchar[FCL_FONT];
FCL_CHARCOUNT = font_charcount[FCL_FONT];' language='C' />
						<command class_type='decision' title='Decision' exp='(.Character &lt; .FirstChar) || (.Character &gt; (.FirstChar + .CharCount))' swap='0' >
							<flowline >
								<command class_type='decision' title='Space?' exp='.Character = 32' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='.Return = 0' />
										</command>
									</flowline>
									<flowline >
										<command class_type='comment' title='' comment='Out of range' textarea='12,0,12,0' />
										<command class_type='calculation' title='Calculation' >
											<exp exp='.Return = 0xAA' />
										</command>
									</flowline>
								</command>
							</flowline>
							<flowline >
								<command class_type='native' title='JavaScript' disable='1' ccode='FCL_CHARACTER = FCL_CHARACTER - font_firstchar[FCL_FONT];' language='C' />
								<command class_type='decision' title='Optimise by only looking up the character start once per char' exp='.Character = oldchar' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='.Position = oldposition' />
										</command>
									</flowline>
									<flowline >
										<command class_type='call' title='User Macro' macro='SumWidths' >
											<return exp='.Position' />
											<argument exp='.Font' />
											<argument exp='.Character' />
										</command>
										<command class_type='calculation' title='Store sum values' >
											<exp exp='oldposition = .Position' />
											<exp exp='oldchar = .Character' />
										</command>
									</flowline>
								</command>
								<command class_type='calculation' title='Calculation' >
									<exp exp='.Position = .Position + .ByteIndex' />
								</command>
								<command class_type='switch' title='Switch' textarea='36,25,36,25' exp='.Font' >
									<case >
										<flowline >
											<command class_type='native' title='JavaScript' ccode='FCR_RETVAL = font0_data[FCL_POSITION];' language='C' />
										</flowline>
									</case>
									<case exp='1' >
										<flowline >
											<command class_type='native' title='JavaScript' ccode='FCR_RETVAL = font1_data[FCL_POSITION];' language='C' />
										</flowline>
									</case>
									<case exp='2' >
										<flowline >
											<command class_type='native' title='JavaScript' ccode='FCR_RETVAL = font2_data[FCL_POSITION];' language='C' />
										</flowline>
									</case>
									<case exp='3' >
										<flowline >
											<command class_type='native' title='JavaScript' ccode='FCR_RETVAL = font3_data[FCL_POSITION];' language='C' />
										</flowline>
									</case>
									<case exp='4' >
										<flowline >
											<command class_type='native' title='JavaScript' ccode='FCR_RETVAL = font4_data[FCL_POSITION];' language='C' />
										</flowline>
									</case>
									<case exp='5' >
										<flowline >
											<command class_type='native' title='JavaScript' ccode='FCR_RETVAL = font5_data[FCL_POSITION];' language='C' />
										</flowline>
									</case>
									<case exp='6' >
										<flowline >
											<command class_type='native' title='JavaScript' ccode='FCR_RETVAL = font6_data[FCL_POSITION];' language='C' />
										</flowline>
									</case>
									<case exp='7' >
										<flowline >
											<command class_type='native' title='JavaScript' ccode='FCR_RETVAL = font7_data[FCL_POSITION];' language='C' />
										</flowline>
									</case>
								</command>
							</flowline>
						</command>
					</flowline>
				</macro>
