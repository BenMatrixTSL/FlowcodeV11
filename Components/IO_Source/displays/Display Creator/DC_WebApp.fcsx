				<macro >
					<flowline name='CreateWidget' description='Define an interactive Widget or slider.
Returns the Widget ID.' statediag='0' >
						<return name='Return' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='WidgetType' type='u8' description='0=WidgetHidden, 1=WidgetRect, 2=WidgetRoundedRect, 3=WidgetEllipse, 4=VSlider, 5=HSlider, 6=VSliderRound, 7=HSliderRound' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
