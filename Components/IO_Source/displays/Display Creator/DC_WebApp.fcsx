						<param name='RedrawSlider' type='b1' description='0=Do not redraw, 1=Redraw' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<command class_type='decision' title='Decision' exp='.WidgetID &gt; 0' swap='0' >
