										<command class_type='call' title='Component Macro' macro='SetBackgroundColour' >
											<argument exp='ObjectColour[.ID][1][0]' />
											<argument exp='ObjectColour[.ID][1][0]' />
											<argument exp='ObjectColour[.ID][1][0]' />
										</command>
									</flowline>
								</command>
								<command class_type='decision' title='Decision' exp='.HAlignment' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='.Len = Length$(.Text)' />
											<exp exp='.TextPixelWidth = 0' />
										</command>
										<command class_type='loop' title='Loop' type='2' forvar='.Idx' exp='.Len' >
											<flowline >
												<command class_type='calculation' title='Calculation' >
													<exp exp='.Char = Char$(.Text,.Idx)' />
												</command>
												<command class_type='call' title='Get font character pixel width' macro='ReadFontWidth' >
													<return exp='.PixelHeight' />
													<argument exp='.Font' />
													<argument exp='.char' />
												</command>
												<command class_type='calculation' title='Calculation' >
													<exp exp='.TextPixelWidth = .TextPixelWidth + .PixelHeight' />
												</command>
												<command class_type='decision' title='Decision' exp='(.Idx + 1) &lt; .Len' swap='0' >
													<flowline >
														<command class_type='call' title='Get font character spacing pixel width' macro='ReadFontStat' >
															<return exp='.PixelHeight' />
															<argument exp='.Font' />
															<argument exp='0' />
														</command>
														<command class_type='calculation' title='Calculation' >
															<exp exp='.TextPixelWidth = .TextPixelWidth + .PixelHeight' />
														</command>
													</flowline>
													<flowline />
												</command>
											</flowline>
										</command>
										<command class_type='switch' title='Switch' textarea='36,25,36,25' exp='.HAlignment' >
											<case >
												<flowline />
											</case>
											<case exp='1' >
												<flowline >
													<command class_type='calculation' title='Calculation' >
														<exp exp='.X = (W[.ID] - .TextPixelWidth) &gt;&gt; 1' />
													</command>
												</flowline>
											</case>
											<case exp='2' >
												<flowline >
													<command class_type='calculation' title='Calculation' >
														<exp exp='.X = W[.ID] - (.TextPixelWidth + TextAlignOffset)' />
													</command>
												</flowline>
											</case>
										</command>
									</flowline>
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='.X = TextAlignOffset' />
										</command>
									</flowline>
								</command>
								<command class_type='call' title='Get font pixel height' macro='ReadFontStat' >
									<return exp='.PixelHeight' />
									<argument exp='.Font' />
									<argument exp='1' />
								</command>
								<command class_type='switch' title='Switch' textarea='36,25,36,25' exp='.VAlignment' >
									<case >
										<flowline >
											<command class_type='calculation' title='Calculation' >
												<exp exp='.Y = TextAlignOffset' />
											</command>
										</flowline>
									</case>
									<case exp='1' >
										<flowline >
											<command class_type='calculation' title='Calculation' >
												<exp exp='.Y = (H[.ID] - .PixelHeight) &gt;&gt; 1' />
											</command>
										</flowline>
									</case>
									<case exp='2' >
										<flowline >
											<command class_type='calculation' title='Calculation' >
												<exp exp='.Y = H[.ID] - (.PixelHeight + TextAlignOffset)' />
											</command>
										</flowline>
									</case>
								</command>
								<command class_type='call' title='Get font pixel height' macro='CanvasPrint' >
									<argument exp='.Text' />
									<argument exp='X[.ID] + .X' />
									<argument exp='Y[.ID] + .Y' />
									<argument exp='.Font' />
									<argument exp='.Transparent' />
								</command>
							</flowline>
							<flowline />
						</command>
					</flowline>
				</macro>
