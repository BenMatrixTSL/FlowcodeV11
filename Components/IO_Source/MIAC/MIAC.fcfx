					<flowline name='LCDRawSend' description='' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
						<param name='In' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Mask' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<command class_type='native' title='C Code' textarea='36,0,36,959' ccode='
	unsigned char pt;
	unsigned char outVal;
	outVal = LCD_PORT;
	clear_bit(outVal, LCD_BIT0);
	clear_bit(outVal, LCD_BIT1);
	clear_bit(outVal, LCD_BIT2);
	clear_bit(outVal, LCD_BIT3);
	clear_bit(outVal, LCD_RS);
	clear_bit(outVal, LCD_E);
	pt = ((FCL_IN &gt;&gt; 4) &amp; 0x0f);
	if (pt &amp; 0x01)
	    set_bit(outVal, LCD_BIT0);
	if (pt &amp; 0x02)
	    set_bit(outVal, LCD_BIT1);
	if (pt &amp; 0x04)
	    set_bit(outVal, LCD_BIT2);
	if (pt &amp; 0x08)
	    set_bit(outVal, LCD_BIT3);
	if (FCL_MASK)
	    set_bit(outVal, LCD_RS);
	LCD_PORT = outVal;
	LCD_DELAY;
	set_bit (LCD_PORT, LCD_E);
	LCD_DELAY;
	clear_bit (LCD_PORT, LCD_E);
	pt = (FCL_IN &amp; 0x0f);
	LCD_DELAY;
	outVal = LCD_PORT;
	clear_bit(outVal, LCD_BIT0);
	clear_bit(outVal, LCD_BIT1);
	clear_bit(outVal, LCD_BIT2);
	clear_bit(outVal, LCD_BIT3);
	clear_bit(outVal, LCD_RS);
	clear_bit(outVal, LCD_E);
	if (pt &amp; 0x01)
	    set_bit(outVal, LCD_BIT0);
	if (pt &amp; 0x02)
	    set_bit(outVal, LCD_BIT1);
	if (pt &amp; 0x04)
	    set_bit(outVal, LCD_BIT2);
	if (pt &amp; 0x08)
	    set_bit(outVal, LCD_BIT3);
	if (FCL_MASK)
	    set_bit(outVal, LCD_RS);
	LCD_PORT = outVal;
	LCD_DELAY;
	set_bit (LCD_PORT, LCD_E);
	LCD_DELAY;
	clear_bit (LCD_PORT, LCD_E);
	LCD_DELAY;
' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' language='C' />
					</flowline>
