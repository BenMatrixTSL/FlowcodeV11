#define LCD_PORT    portd
#define LCD_TRIS    trisd
#define LCD_BIT0    5
#define LCD_BIT1    4
#define LCD_BIT2    3
#define LCD_BIT3    0
#define LCD_RS      1
#define LCD_E       6

#define LCD_DELAY  FCI_DELAYBYTE_US(100)

void LCDRawSend(char nIn, char nMask);
#define MX_ADC_SAMP_TIME 40
MX_UINT8 InputANALOG_8Bit(MX_UINT8 InputID);

void LCDRawSend(char nIn, char nMask)
{
	unsigned char pt;
	unsigned char outVal;
	outVal = LCD_PORT;
	clear_bit(outVal, LCD_BIT0);
	clear_bit(outVal, LCD_BIT1);
	clear_bit(outVal, LCD_BIT2);
	clear_bit(outVal, LCD_BIT3);
	clear_bit(outVal, LCD_RS);
	clear_bit(outVal, LCD_E);

	pt = ((nIn &gt;&gt; 4) &amp; 0x0f);
	if (pt &amp; 0x01)
	    set_bit(outVal, LCD_BIT0);
	if (pt &amp; 0x02)
	    set_bit(outVal, LCD_BIT1);
	if (pt &amp; 0x04)
	    set_bit(outVal, LCD_BIT2);
	if (pt &amp; 0x08)
	    set_bit(outVal, LCD_BIT3);

	if (nMask)
	    set_bit(outVal, LCD_RS);

	LCD_PORT = outVal;

	LCD_DELAY;
	set_bit (LCD_PORT, LCD_E);

	LCD_DELAY;
	clear_bit (LCD_PORT, LCD_E);

	pt = (nIn &amp; 0x0f);
	LCD_DELAY;
	outVal = LCD_PORT;

	clear_bit(outVal, LCD_BIT0);
	clear_bit(outVal, LCD_BIT1);
	clear_bit(outVal, LCD_BIT2);
	clear_bit(outVal, LCD_BIT3);
	clear_bit(outVal, LCD_RS);

	clear_bit(outVal, LCD_E);

	if (pt &amp; 0x01)
	    set_bit(outVal, LCD_BIT0);
	if (pt &amp; 0x02)
	    set_bit(outVal, LCD_BIT1);
	if (pt &amp; 0x04)
	    set_bit(outVal, LCD_BIT2);
	if (pt &amp; 0x08)
	    set_bit(outVal, LCD_BIT3);

	if (nMask)
	    set_bit(outVal, LCD_RS);

	LCD_PORT = outVal;

	LCD_DELAY;
	set_bit (LCD_PORT, LCD_E);

	LCD_DELAY;
	clear_bit (LCD_PORT, LCD_E);

	LCD_DELAY;
}

MX_UINT8 InputANALOG_8Bit(MX_UINT8 InputID)
{
	//set up ADC conversion
	char cnt, chan_id;

	if(InputID &amp;&amp; (InputID &lt; 9))
	{
		chan_id = InputID;
		if (InputID &gt;= 5) chan_id++;

		ADCON2 = 0x03;
		ADCON1 = 15 - chan_id;

		//turn ADC on
		ADCON0 = 0x01 | ((chan_id - 1) &lt;&lt; 2);

		//wait the acquisition time
		cnt = 0;
		while (cnt &lt; MX_ADC_SAMP_TIME) cnt++;

		//begin conversion and wait until it has finished
		ADCON0 = ADCON0 | 0x02;
		while (ADCON0 &amp; 0x02);

		ADCON1 = 0x0f;
		ADCON0 = 0x00;
	}

	return(ADRESH);
}
