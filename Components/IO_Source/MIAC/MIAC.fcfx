	delay_ms(2);
	LCDRawSend(0x02, 0);
	delay_ms(2);
' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' language='C' />
