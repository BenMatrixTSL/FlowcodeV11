				<macro >
					<flowline name='DisableServo_Sim' description='Disables a servo channel effectivly disabling the motor.' >
						<return name='Return' type='v0' description='' isconst='0' isinit='0' usrinit='' setinit='' />
						<param name='Channel' type='u8' description='The channel to set the position for.  First channel is zero.' isconst='0' isinit='0' usrinit='0' setinit='' />
						<local name='Dummy' type='u8' description='' isconst='0' isinit='0' usrinit='0' setinit='' />
						<command class_type='decision' title='Decision' cmdcolor='3568629' cmdcolor_sec='3568629' exp='.Channel &lt; Channel_Count' swap='0' >
							<flowline >
