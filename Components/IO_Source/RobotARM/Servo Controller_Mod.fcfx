				<macro >
					<flowline name='MoveToPosition' description='Moves a single step towards the new position, the rate of movement is determined by how often the move to macro is called. Returns 1 if the position has been reached.' >
						<return name='Return' type='u8' description='1 = reached target, 0 = target not reached.' isconst='0' isinit='0' usrinit='0' setinit='' />
						<param name='Channel' type='u8' description='The channel to set the position for.  First channel is zero.' isconst='0' isinit='0' usrinit='0' setinit='' />
						<param name='Position' type='u8' description='Target position to move towards.' isconst='0' isinit='0' usrinit='0' setinit='' />
						<command class_type='calculation' title='Arrived - Unless we are told otherwise' cmdcolor='7895044' cmdcolor_sec='7895044' >
							<exp exp='.Return = 1' />
						</command>
						<command class_type='decision' title='Decision' cmdcolor='3568629' cmdcolor_sec='3568629' exp='.Channel &lt; Channel_Count' swap='0' >
							<flowline >
								<command class_type='decision' title='Decision' cmdcolor='3568629' cmdcolor_sec='3568629' exp='.Position != Servo_Position[.Channel]' swap='0' >
									<flowline >
										<command class_type='calculation' title='Not there yet' cmdcolor='7895044' cmdcolor_sec='7895044' >
											<exp exp='.Return = 0' />
											<exp exp='' />
										</command>
										<command class_type='decision' title='Decision' cmdcolor='3568629' cmdcolor_sec='3568629' exp='.Position &gt; Servo_Position[.Channel]' swap='0' >
											<flowline >
												<command class_type='calculation' title='Increment Position' cmdcolor='7895044' cmdcolor_sec='7895044' >
													<exp exp='Servo_Position[.Channel] = Servo_Position[.Channel] + 1' />
												</command>
											</flowline>
											<flowline >
												<command class_type='calculation' title='Decrement Position' cmdcolor='7895044' cmdcolor_sec='7895044' >
													<exp exp='Servo_Position[.Channel] = Servo_Position[.Channel] - 1' />
												</command>
											</flowline>
										</command>
										<command class_type='calculation' title='Copy to required variable so we dont break the auto move stuff' cmdcolor='7895044' cmdcolor_sec='7895044' >
											<exp exp='Servo_Required[.Channel] = Servo_Position[.Channel]' />
										</command>
									</flowline>
									<flowline />
								</command>
							</flowline>
							<flowline />
						</command>
					</flowline>
				</macro>
				<macro >
					<flowline name='SetTrim' description='Allows the level of trim for each servo channel to be adjusted progmatically.' >
						<return name='Return' type='v0' description='' isconst='0' isinit='0' usrinit='' setinit='' />
						<param name='Channel' type='u8' description='The channel to set the position for.  First channel is zero.' isconst='0' isinit='0' usrinit='0' setinit='' />
						<param name='Trim' type='u8' description='0=0us, 1=+25us, 2=+50us, 3=+75us, 4=+100us, 5=-25us, 6=-50us, 7=-75us, 8=-100us' isconst='0' isinit='0' usrinit='0' setinit='' />
						<local name='tcalc' type='s16' description='' isconst='0' isinit='0' usrinit='0' setinit='' />
						<command class_type='decision' title='Decision' cmdcolor='3568629' cmdcolor_sec='3568629' exp='.Channel &lt; Channel_Count' swap='0' >
							<flowline >
								<command class_type='decision' title='Ensure Trim is in range' cmdcolor='3568629' cmdcolor_sec='3568629' exp='.Trim &lt; 100' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' cmdcolor='7895044' cmdcolor_sec='7895044' >
											<exp exp='.tcalc = .Trim - 50' />
										</command>
										<command class_type='decision' title='Decision' cmdcolor='3568629' cmdcolor_sec='3568629' exp='.tcalc &lt; 0' swap='0' >
											<flowline >
												<command class_type='calculation' title='Calculation' cmdcolor='7895044' cmdcolor_sec='7895044' >
													<exp exp='Servo_Trim[.Channel] = 0 - ((.tcalc) * Servo_Clock_Count_25)' />
												</command>
											</flowline>
											<flowline >
												<command class_type='calculation' title='Calculation' cmdcolor='7895044' cmdcolor_sec='7895044' >
													<exp exp='Servo_Trim[.Channel] = .tcalc * Servo_Clock_Count_25' />
												</command>
											</flowline>
										</command>
									</flowline>
									<flowline />
								</command>
							</flowline>
							<flowline />
						</command>
					</flowline>
				</macro>
