step iteration.' textarea='12,0,12,0' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' />
								<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='0' cmdgradient='4' cmdopacity='0.64' exp='(.dif0 &gt;= .dif1) &amp;&amp; (.dif0 &gt;= .dif2) &amp;&amp; (.dif0 &gt;= .dif3) &amp;&amp; (.dif0 &gt;= .dif4)' swap='0' >
