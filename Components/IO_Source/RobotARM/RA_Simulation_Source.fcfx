						<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='0' cmdgradient='4' cmdopacity='0.64' exp='(.dif0 = 0) &amp;&amp; (.dif1 = 0) &amp;&amp; (.dif2 = 0) &amp;&amp; (.dif3 = 0) &amp;&amp; (.dif4 = 0)' swap='0' >
