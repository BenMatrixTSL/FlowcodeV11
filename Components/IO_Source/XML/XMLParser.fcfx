				<macro >
					<flowline name='ParseJSONDataString' description='Parses JSON formatted data in a piecemeal format as and when file data is available. 
Creates objects to store the contents of the JSON ' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
						<param name='Data' type='T8*' description='JSON Data string, can be a chunk of a file at a time' isconst='0' isHidden='0' isinit='0' usrinit='&quot;&quot;' setinit='' >
							<array size='20' />
						</param>
