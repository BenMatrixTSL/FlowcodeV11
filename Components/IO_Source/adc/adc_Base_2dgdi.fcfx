						<command class_type='comment' title='' comment='Set bit depth in a property (so constant)' textarea='500,7,500,7' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' />
