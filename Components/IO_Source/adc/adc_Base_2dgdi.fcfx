						</command>
						<command class_type='loop' title='Loop' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' type='0' exp='.adcspd != 0' >
							<flowline >
								<command class_type='comment' title='' comment='Add the value as a 3-digit string to the text' textarea='500,7,500,7' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' />
								<command class_type='sim' title='Panel' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='Tree.GetSubValue' >
									<return exp='.adcval' />
									<argument exp='.adcspd' />
									<argument exp='&quot;value&quot;' />
								</command>
								<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='.optpart = ToString$ (.adcval)' />
									<exp exp='.digits = Length$ (.optpart)' />
								</command>
								<command class_type='loop' title='Loop' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' type='0' exp='.digits &lt; 3' >
									<flowline >
										<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
											<exp exp='.optpart = &quot;0&quot; + .optpart' />
											<exp exp='.digits = Length$ (.optpart)' />
										</command>
									</flowline>
								</command>
								<command class_type='calculation' title='Append value' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='.adcspdlist = .adcspdlist + .optpart' />
								</command>
								<command class_type='comment' title='' comment='Add the value description and a newline' textarea='500,7,500,7' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' />
								<command class_type='sim' title='Panel' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='Tree.GetSubValue' >
									<return exp='.optpart' />
									<argument exp='.adcspd' />
									<argument exp='&quot;name&quot;' />
								</command>
								<command class_type='calculation' title='Append value' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='.adcspdlist = .adcspdlist + .optpart + &quot;\n&quot;' />
								</command>
								<command class_type='comment' title='' comment='Go to next' textarea='500,7,500,7' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' />
								<command class_type='sim' title='Panel' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='Tree.StepNext' >
									<argument exp='.adcspd' />
								</command>
							</flowline>
						</command>
						<command class_type='sim' title='Commit the property' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='Component.Property.SetFilter' >
							<argument exp='this' />
							<argument exp='&quot;convspd&quot;' />
							<argument exp='.adcspdlist' />
						</command>
