						<command class_type='decision' title='If sample bits not dividing by zero' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='.adcval &gt; 0' swap='0' >
