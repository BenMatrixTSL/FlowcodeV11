								</command>
							</flowline>
							<flowline />
						</command>
						<command class_type='sim' title='Commit the property' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='Component.Property.SetValue' >
