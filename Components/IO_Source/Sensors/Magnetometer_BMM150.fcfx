											<exp exp='.x2 = (.x1 / .x0) - 0x4000' />
											<exp exp='.Return = .x2' />
											<exp exp='.x3 = .Return * .Return' />
											<exp exp='.x4 = trimXY[1] * (.x3 / 128)' />
											<exp exp='.x5 = trimXY[0] * 128' />
											<exp exp='.x6 = .Return * .x5' />
											<exp exp='.x7 = ((.x4 + .x6) / 512) + 0x100000' />
											<exp exp='.x8 = trimX[1] + 0xA0' />
											<exp exp='.x9 = (.x7 * .x8) / 4096' />
											<exp exp='.x10 = .data * .x9' />
											<exp exp='.Return = .x10 / 8192' />
											<exp exp='.Return = (.Return + (trimX[0] * 8)) / 16' />
