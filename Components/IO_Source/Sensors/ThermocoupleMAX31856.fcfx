							<exp exp='.combine = ((.data[0] &amp; 0x7F) &gt;&gt; 5)' />
							<exp exp='TypeConversions1::SetByte(2,.combine)' />
							<exp exp='.combine = (.data[0] &lt;&lt; 3) | (.data[1] &gt;&gt; 5)' />
