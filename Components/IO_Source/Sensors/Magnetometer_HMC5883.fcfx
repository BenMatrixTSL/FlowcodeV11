									<return exp='.Reading' />
									<argument exp='1' />
								</command>
								<command class_type='calculation' title='Calculation' >
									<exp exp='.Reading = .Reading / (2 * compass_range)' />
									<exp exp='.Reading = .Reading * 4095' />
								</command>
								<command class_type='decision' title='Decision' exp='.Reading &gt; 2047' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='.Reading = 2047' />
										</command>
									</flowline>
									<flowline >
										<command class_type='decision' title='Decision' exp='.Reading &lt; -2048' swap='0' >
											<flowline >
												<command class_type='calculation' title='Calculation' >
