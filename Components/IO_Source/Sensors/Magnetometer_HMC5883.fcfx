				<macro >
					<flowline name='Read_Bearing' description='Reads the current sensor compass bearing based on degrees CW from magnetic north.
Returns 0-359 where 0 = North, 90 = East, 180 = South, 270 = West
Requires the ArcTan floating point function to be available to work correctly.
Supported on AVR, 16-bit ' statediag='0' >
						<return name='Return' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='NumSamples' type='u8' description='The number of readings to base the bearing on' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='fcalc' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<local name='fx' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='fy' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<command class_type='calculation' title='Calculation' >
							<exp exp='.Return = 0' />
						</command>
						<command class_type='call' title='Call Macro' macro='UpdateCompassData' />
						<command class_type='decision' title='Decision' exp='Y_Var = 0' swap='0' >
							<flowline >
								<command class_type='decision' title='Decision' exp='X_Var &lt; 0' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='.Return = 180' />
										</command>
									</flowline>
									<flowline >
										<command class_type='decision' title='Decision' exp='X_Var &gt; 0' swap='0' >
											<flowline >
												<command class_type='calculation' title='Calculation' >
													<exp exp='.Return = 0' />
												</command>
											</flowline>
											<flowline />
										</command>
									</flowline>
								</command>
							</flowline>
							<flowline >
								<command class_type='calculation' title='Calculation' >
									<exp exp='.fx = FLOAT X_Var' />
									<exp exp='.fy = FLOAT Y_Var' />
									<exp exp='.fcalc = .fx / .fy' />
									<exp exp='.fx = atan (.fcalc)' />
									<exp exp='.fcalc = .fx * PHI' />
								</command>
								<command class_type='decision' title='Decision' exp='Y_Var &gt; 0' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='.fcalc = 90.0 - .fcalc' />
										</command>
									</flowline>
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='.fcalc = 270.0 - .fcalc' />
										</command>
									</flowline>
								</command>
								<command class_type='calculation' title='Calculation' >
									<exp exp='.Return = float2int (.fcalc)' />
								</command>
							</flowline>
						</command>
					</flowline>
				</macro>
