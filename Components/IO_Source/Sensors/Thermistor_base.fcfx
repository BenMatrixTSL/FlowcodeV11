				<macro >
					<flowline name='SetTemperature' description='Simulation macro that sets the internal ADC value to the appropriate value for a given temperature.  This allows all internal systems of the sensor to be simulated.' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
						<param name='Temperature' type='f32' description='The temperature in C - restricted to -25 to +125 range' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<local name='i' type='u8' description='LUT Index' isconst='0' isHidden='0' isinit='1' usrinit='0' setinit='0' />
						<local name='lo' type='f32' description='Values from the LUT' isconst='0' isHidden='0' isinit='1' usrinit='0.0' setinit='0.0' />
						<local name='hi' type='f32' description='Values from the LUT' isconst='0' isHidden='0' isinit='1' usrinit='0.0' setinit='0.0' />
						<command class_type='comment' title='' comment='Ensure temperature is within the valid range' textarea='12,0,12,0' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' />
						<command class_type='decision' title='Decision' cmdcolor='16098537' cmdcolor_sec='9519237' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' exp='.Temperature &lt; -25.0' swap='0' >
							<flowline >
								<command class_type='calculation' title='Calculation' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' >
									<exp exp='.Temperature = -25' />
								</command>
							</flowline>
							<flowline >
								<command class_type='decision' title='Decision' cmdcolor='16098537' cmdcolor_sec='9519237' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' exp='.Temperature &gt; 125.0' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' >
											<exp exp='.Temperature = 125.0' />
										</command>
									</flowline>
									<flowline />
								</command>
							</flowline>
						</command>
						<command class_type='comment' title='' comment='Obtain the ADC value from the LUT' textarea='112,20,112,20' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' />
						<command class_type='calculation' title='Calculation' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' >
							<exp exp='.i = (floor (.Temperature / 25.0)) + 1' />
							<exp exp='.lo = SamplePoints[.i]' />
							<exp exp='.hi = SamplePoints[.i + 1]' />
							<exp exp='.Temperature = ((.Temperature / 25.0) - .i + 1) * (.hi - .lo) + .lo' />
							<exp exp='.Temperature = .Temperature / ((1 &lt;&lt; ADC_BITS) - 1)' />
						</command>
						<command class_type='sim' title='Built-in Function' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' macro='Component.Property.GetValue' >
							<return exp='.i' />
							<argument exp='this' />
							<argument exp='&quot;adc_base1::channel&quot;' />
						</command>
						<command class_type='sim' title='Simulation' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' macro='Connection.SetAn' >
							<argument exp='.i' />
							<argument exp='.Temperature' />
							<argument exp='false' />
						</command>
					</flowline>
				</macro>
				<macro >
					<flowline name='Main' description='' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
