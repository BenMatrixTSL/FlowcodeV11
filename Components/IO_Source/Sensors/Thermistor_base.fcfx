									<exp exp='.Expansion = .Expansion + &quot;,\n&quot;' />
									<exp exp='.Expansion = .Expansion + STRING ((P50 * (1 &lt;&lt; ADC_BITS)) / (RESISTOR + P50))' />
									<exp exp='.Expansion = .Expansion + &quot;,\n&quot;' />
									<exp exp='.Expansion = .Expansion + STRING ((P75 * (1 &lt;&lt; ADC_BITS)) / (RESISTOR + P75))' />
									<exp exp='.Expansion = .Expansion + &quot;,\n&quot;' />
									<exp exp='.Expansion = .Expansion + STRING ((P100 * (1 &lt;&lt; ADC_BITS)) / (RESISTOR + P100))' />
									<exp exp='.Expansion = .Expansion + &quot;,\n&quot;' />
									<exp exp='.Expansion = .Expansion + STRING ((P125 * (1 &lt;&lt; ADC_BITS)) / (RESISTOR + P125))' />
									<exp exp='.Expansion = .Expansion + &quot;\n};\n&quot;' />
								</command>
							</flowline>
							<flowline />
						</command>
						<command class_type='calculation' title='Calculation' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' >
							<exp exp='.Return = 1' />
						</command>
					</flowline>
				</macro>
				<macro >
					<flowline name='GetRealTemperature' description='Reads the ADC input and Returns the calculated Temperature as Float value in degrees C' statediag='0' >
						<return name='Return' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<local name='adc' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='IntTemp' type='s16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<command class_type='decision' title='Decision' cmdcolor='16098537' cmdcolor_sec='9519237' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' exp='ADCConfig' swap='0' >
							<flowline >
								<command class_type='calculation' title='Calculation' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' >
									<exp exp='.adc = ExternalADC' />
