				<macro >
					<flowline name='Read_Bearing' description='Reads the current sensor compass bearing based on degrees CW from magnetic north.
Returns 0-359 where 0 = North, 90 = East, 180 = South, 270 = West
Requires the ArcTan floating point function to be available to work correctly.
Supported on AVR, 16-bit ' statediag='0' >
						<return name='Return' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='fcalc' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<local name='AngleRads' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<local name='OK' type='u8' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='Difference' type='s16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='Mid' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<local name='X' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<local name='Y' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<command class_type='calculation' title='Calculation' >
							<exp exp='.Return = 0' />
						</command>
						<command class_type='decision' title='Decision' exp='LastSampleType == 2' swap='0' simple='1' op1='LastSampleType' op2='2' oper='0' logical='0' op1_b='0' op2_b='0' oper_b='0' >
							<flowline >
								<command class_type='calculation' title='Calculation' >
									<exp exp='.OK = 1' />
								</command>
							</flowline>
							<flowline >
								<command class_type='call' title='Call Macro' macro='UpdateCompassData' >
									<return exp='.OK' />
								</command>
							</flowline>
						</command>
						<command class_type='decision' title='Decision' exp='.OK &gt; 0' swap='0' simple='1' op1='.OK' op2='0' oper='2' logical='0' op1_b='0' op2_b='0' oper_b='0' >
							<flowline >
								<command class_type='calculation' title='Calculate compensated X' >
									<exp exp='.Difference = MinMax[1][0] - MinMax[0][0]' />
									<exp exp='.Mid = (.Difference / 2) + MinMax[0][0]' />
									<exp exp='.X = X_Var - .Mid' />
								</command>
								<command class_type='calculation' title='Calculate compensated Y' >
									<exp exp='.Difference = MinMax[1][1] - MinMax[0][1]' />
									<exp exp='.Mid = (.Difference / 2) + MinMax[0][1]' />
									<exp exp='.Y = Y_Var - .Mid' />
								</command>
								<command class_type='calculation' title='Calculation' >
									<exp exp='.AngleRads = atan2(.Y,.X)' />
									<exp exp='.fcalc = .AngleRads * PHI' />
								</command>
								<command class_type='decision' title='Ensure returned range is shifted from -180-180 to 0-360' exp='.fcalc &lt; 0' swap='0' simple='1' op1='.fcalc' op2='0' oper='4' logical='0' op1_b='0' op2_b='0' oper_b='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='.fcalc = .fcalc + 360.0' />
										</command>
									</flowline>
									<flowline />
								</command>
								<command class_type='calculation' title='Calculation' >
									<exp exp='Bearing = float2int(.fcalc)' />
								</command>
							</flowline>
							<flowline />
						</command>
						<command class_type='calculation' title='Calculation' >
							<exp exp='.Return = Bearing' />
						</command>
					</flowline>
				</macro>
				<macro >
