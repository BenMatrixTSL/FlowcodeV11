								</command>
								<command class_type='calculation' title='Calculation' >
									<exp exp='.Return = 1.0 / (.Return / FLOAT BetaValue + 1.0 / (25.0 + 273.15)) - 273.15' />
								</command>
							</flowline>
							<flowline >
								<command class_type='comment' title='' comment='Pullup' textarea='12,0,12,0' />
								<command class_type='calculation' title='Calculation' >
									<exp exp='.Return = FLOAT FixedResistorValue / (FLOAT .MaxADC / FLOAT .ReadADC - 1.0)' />
								</command>
								<command class_type='calculation' title='Calculation' >
									<exp exp='.Return = log(.Return / FLOAT ThermistorValue25C)' />
								</command>
