				<macro >
					<flowline name='ReadTemperatureFloat' description='Reads the temperature as a floating point value in °C.
Resultion sets the return bit value and conversion time.
Calls the ReadTemperatureRaw function and then converts to a meaningful value.' statediag='0' >
						<return name='Return' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<local name='UT' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='X2' type='s32' description='' isconst='0' isHidden='0' isinit='1' usrinit='1' setinit='1' />
						<local name='B5' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='T' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='X1' type='s32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<command class_type='call' title='User Macro' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='I2C_Write_Register' >
							<argument exp='0xF4' />
							<argument exp='0x2E' />
						</command>
						<command class_type='delay' title='Delay' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='5' type='1' />
						<command class_type='call' title='User Macro' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='I2C_Read_MSB_LSB_Int' >
							<return exp='.UT' />
							<argument exp='0xF6' />
						</command>
						<command class_type='calculation' title='Test vaules should result to 15.0 float. ' disable='1' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
							<exp exp='.UT = 27898' />
							<exp exp='AC1 = 408' />
							<exp exp='AC2 = -72' />
							<exp exp='AC3 = -14383' />
							<exp exp='AC4 = 32741' />
							<exp exp='AC5 = 32757' />
							<exp exp='AC6 = 23153' />
							<exp exp='B1 = 6190' />
							<exp exp='B2 = 4' />
							<exp exp='MB = -32768' />
							<exp exp='MC = -8711' />
							<exp exp='MD = 2868' />
						</command>
						<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='.UT' swap='0' >
							<flowline >
								<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='.X1 = ((.UT - AC6) * AC5) &gt;&gt; 15' />
									<exp exp='.T = MC' />
									<exp exp='.X2 = .T &lt;&lt; 11' />
									<exp exp='.T = MD' />
									<exp exp='.X2 = .X2 / (.X1 + .T)' />
									<exp exp='B5 = .X1 + .X2' />
									<exp exp='.T = (B5 + 8) &gt;&gt; 4' />
									<exp exp='.Return = FLOAT.T / 10.0' />
								</command>
							</flowline>
							<flowline />
						</command>
					</flowline>
				</macro>
