							<exp exp='.X2 = (B1 * ((.B6 * .B6) &gt;&gt; 12)) &gt;&gt; 16' />
							<exp exp='.X3 = ((.X1 + .X2) + (1 &lt;&lt; 1)) &gt;&gt; 2' />
