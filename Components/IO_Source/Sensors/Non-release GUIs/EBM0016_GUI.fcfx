				<macro >
					<flowline name='SetHumidity' description='Sends the given Humidity to the display, and calculates the internal values used by the simulated read macros.' >
						<return name='Return' type='v0' description='' isconst='0' isinit='0' usrinit='' setinit='' />
						<param name='Humidity' type='f32' description='Relative Humidity from 0  to 100 %' isconst='0' isinit='0' usrinit='0.0' setinit='' />
						<local name='thermo_on_length' type='f32' description='Length of the thermometer graphic to light up' isconst='0' isinit='1' usrinit='20.0' setinit='20.0' />
						<local name='thermo_off_length' type='f32' description='Length of the off section of the thermometer' isconst='0' isinit='1' usrinit='20.0' setinit='20.0' />
						<local name='thermo_on_mid' type='f32' description='Mid Y-position of the on section.' isconst='0' isinit='1' usrinit='10.0' setinit='10.0' />
						<local name='thermo_off_mid' type='f32' description='Mid point of the off section of the thermometer.' isconst='0' isinit='1' usrinit='30.0' setinit='30.0' />
						<local name='temp_str' type='T8' description='Temporary string for display values' isconst='0' isinit='1' usrinit='&quot;&quot;' setinit='&quot;&quot;' >
							<array size='20' />
						</local>
						<local name='register_val' type='s16' description='Register 0 value of the sensor chip' isconst='0' isinit='1' usrinit='0' setinit='0' />
						<local name='ratio' type='f32' description='0...1 value used for scaling the thermometer display' isconst='0' isinit='1' usrinit='0.0' setinit='0.0' />
						<command class_type='comment' title='' comment='Set thermometer graphic' textarea='53,13,53,13' cmdcolor='10944511' cmdcolor_sec='11465458' />
						<command class_type='decision' title='Decision' cmdcolor='12615680' cmdcolor_sec='15376912' exp='.Humidity &gt; 100' swap='0' >
							<flowline >
								<command class_type='calculation' title='Calculation' cmdcolor='16760919' cmdcolor_sec='16760832' >
									<exp exp='.Humidity = 100' />
								</command>
							</flowline>
							<flowline >
								<command class_type='decision' title='Decision' cmdcolor='12615680' cmdcolor_sec='15376912' exp='.Humidity &lt; 0' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' cmdcolor='16760919' cmdcolor_sec='16760832' >
											<exp exp='.Humidity = 0' />
										</command>
									</flowline>
									<flowline />
								</command>
							</flowline>
						</command>
						<command class_type='calculation' title='Calculation' cmdcolor='16760919' cmdcolor_sec='16760832' >
							<exp exp='.ratio = Humidity * 0.01' />
							<exp exp='.thermo_on_length = .ratio * 36.0 + 4.0' />
							<exp exp='.thermo_off_length = 40.0 - .thermo_on_length' />
							<exp exp='.thermo_on_mid = .thermo_on_length * 0.5 - 20' />
							<exp exp='.thermo_off_mid = 20 - .thermo_off_length * 0.5' />
							<exp exp='' />
						</command>
						<command class_type='sim' title='Simulation' cmdcolor='1879295' cmdcolor_sec='5751039' macro='Panel.Position.ScaleTo' >
							<argument exp='thermo_on1' />
							<argument exp='5' />
							<argument exp='5' />
							<argument exp='.thermo_on_length' />
						</command>
						<command class_type='sim' title='Simulation' cmdcolor='1879295' cmdcolor_sec='5751039' macro='Panel.Position.ScaleTo' >
							<argument exp='thermo_off1' />
							<argument exp='5' />
							<argument exp='5' />
							<argument exp='.thermo_off_length' />
						</command>
						<command class_type='sim' title='Simulation' cmdcolor='1879295' cmdcolor_sec='5751039' macro='Panel.Position.MoveTo' >
							<argument exp='thermo_on1' />
							<argument exp='0' />
							<argument exp='.thermo_on_mid' />
							<argument exp='0' />
						</command>
						<command class_type='sim' title='Simulation' cmdcolor='1879295' cmdcolor_sec='5751039' macro='Panel.Position.MoveTo' >
							<argument exp='thermo_off1' />
							<argument exp='0' />
							<argument exp='.thermo_off_mid' />
							<argument exp='0' />
						</command>
						<command class_type='calculation' title='Calculation' textarea='43,10,43,10' cmdcolor='16760919' cmdcolor_sec='16760832' >
							<exp exp='.register_val = (.Humidity + 6.0) * 65536.0 / 125.0' />
							<exp exp='RH_MSB = .register_val &gt;&gt; 8' />
							<exp exp='RH_LSB = .register_val &amp; 252' />
							<exp exp='.Humidity = (RH_MSB &lt;&lt; 8) OR RH_LSB' />
							<exp exp='.Humidity = .Humidity * 125.0 / 65536.0 - 6.0' />
						</command>
						<command class_type='sim' title='Simulation' cmdcolor='1879295' cmdcolor_sec='5751039' macro='Component.Property.SetValue' >
							<argument exp='this' />
							<argument exp='&quot;humidity&quot;' />
							<argument exp='.Humidity' />
						</command>
						<command class_type='calculation' title='Calculation' cmdcolor='16760919' cmdcolor_sec='16760832' >
							<exp exp='.temp_str = &quot;0000&quot; + STRING (SIGNED (fabs (humidity * 10) % 10))' />
							<exp exp='.temp_str = Right$ (.temp_str,1)' />
							<exp exp='.temp_str = STRING (SIGNED (humidity)) + &quot;.&quot; + .temp_str + &quot; %RH&quot;' />
							<exp exp='' />
						</command>
						<command class_type='sim' title='Simulation' cmdcolor='1879295' cmdcolor_sec='5751039' macro='Component.Property.SetValue' >
							<argument exp='temp_text1' />
							<argument exp='&quot;text&quot;' />
							<argument exp='.temp_str' />
						</command>
						<command class_type='decision' title='Decision' cmdcolor='12615680' cmdcolor_sec='15376912' exp='number_base &gt; 0' swap='0' >
							<flowline >
								<command class_type='calculation' title='Calculation' cmdcolor='16760919' cmdcolor_sec='16760832' >
									<exp exp='.temp_str = NumberToHex$ (RH_MSB) + &quot;, &quot; + NumberToHex$ (RH_LSB)' />
								</command>
							</flowline>
							<flowline >
								<command class_type='calculation' title='Calculation' cmdcolor='16760919' cmdcolor_sec='16760832' >
									<exp exp='.temp_str = STRING (RH_MSB) + &quot;, &quot; + STRING (RH_LSB)' />
								</command>
							</flowline>
						</command>
						<command class_type='sim' title='Simulation' cmdcolor='1879295' cmdcolor_sec='5751039' macro='Component.Property.SetValue' >
							<argument exp='I2C_text1' />
							<argument exp='&quot;text&quot;' />
