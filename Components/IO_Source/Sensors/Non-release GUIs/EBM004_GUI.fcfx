						<local name='thermo_on_length' type='f32' description='Length of the thermometer graphic to light up' isconst='0' isinit='1' usrinit='20.0' setinit='20.0' />
						<local name='thermo_off_length' type='f32' description='Length of the off section of the thermometer' isconst='0' isinit='1' usrinit='20.0' setinit='20.0' />
						<local name='thermo_on_mid' type='f32' description='Mid Y-position of the on section.' isconst='0' isinit='1' usrinit='10.0' setinit='10.0' />
						<local name='thermo_off_mid' type='f32' description='Mid point of the off section of the thermometer.' isconst='0' isinit='1' usrinit='30.0' setinit='30.0' />
						<local name='temp_str' type='T8' description='Temporary string for display values' isconst='0' isinit='1' usrinit='&quot;&quot;' setinit='&quot;&quot;' >
							<array size='20' />
						</local>
						<local name='register_val' type='s16' description='Register 0 value of the sensor chip' isconst='0' isinit='1' usrinit='0' setinit='0' />
						<local name='ratio' type='f32' description='0...1 value used for scaling the thermometer display' isconst='0' isinit='1' usrinit='0.0' setinit='0.0' />
						<command class_type='comment' title='' comment='Set thermometer graphic' textarea='53,13,53,13' cmdcolor='16760919' cmdcolor_sec='16777147' />
						<command class_type='decision' title='Decision' cmdcolor='32896' cmdcolor_sec='6612196' exp='.Temperature &gt; max_temp' swap='0' >
							<flowline >
								<command class_type='calculation' title='Calculation' cmdcolor='8415488' cmdcolor_sec='14994788' >
									<exp exp='.Temperature = max_temp' />
								</command>
							</flowline>
							<flowline >
								<command class_type='decision' title='Decision' cmdcolor='32896' cmdcolor_sec='6612196' exp='.Temperature &lt; min_temp' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' cmdcolor='8415488' cmdcolor_sec='14994788' >
											<exp exp='.Temperature = min_temp' />
										</command>
									</flowline>
									<flowline />
								</command>
							</flowline>
						</command>
						<command class_type='calculation' title='Calculation' cmdcolor='8415488' cmdcolor_sec='14994788' >
							<exp exp='.ratio = (.Temperature - min_temp) / (max_temp - min_temp)' />
							<exp exp='.thermo_on_length = .ratio * 36.0 + 4.0' />
							<exp exp='.thermo_off_length = 40.0 - .thermo_on_length' />
							<exp exp='.thermo_on_mid = .thermo_on_length * 0.5 - 20' />
							<exp exp='.thermo_off_mid = 20 - .thermo_off_length * 0.5' />
							<exp exp='' />
						</command>
						<command class_type='sim' title='Simulation' cmdcolor='16744448' cmdcolor_sec='16760919' macro='Panel.Position.ScaleTo' >
							<argument exp='thermo_on' />
							<argument exp='5' />
							<argument exp='5' />
							<argument exp='.thermo_on_length' />
						</command>
						<command class_type='sim' title='Simulation' cmdcolor='16744448' cmdcolor_sec='16760919' macro='Panel.Position.ScaleTo' >
							<argument exp='thermo_off' />
							<argument exp='5' />
							<argument exp='5' />
							<argument exp='.thermo_off_length' />
						</command>
						<command class_type='sim' title='Simulation' cmdcolor='16744448' cmdcolor_sec='16760919' macro='Panel.Position.MoveTo' >
							<argument exp='thermo_on' />
							<argument exp='0' />
							<argument exp='.thermo_on_mid' />
							<argument exp='0' />
						</command>
						<command class_type='sim' title='Simulation' cmdcolor='16744448' cmdcolor_sec='16760919' macro='Panel.Position.MoveTo' >
							<argument exp='thermo_off' />
							<argument exp='0' />
							<argument exp='.thermo_off_mid' />
							<argument exp='0' />
						</command>
						<command class_type='calculation' title='Calculation' textarea='43,10,43,10' cmdcolor='8415488' cmdcolor_sec='14994788' >
							<exp exp='.register_val = .temperature * 256' />
							<exp exp='I2C_MSB = .register_val &gt;&gt; 8' />
							<exp exp='I2C_LSB = .register_val &amp; 224' />
							<exp exp='.temperature = ((SIGNED I2C_MSB &lt;&lt; 8) OR I2C_LSB) / 256.0' />
						</command>
						<command class_type='sim' title='Simulation' cmdcolor='16744448' cmdcolor_sec='16760919' macro='Component.Property.SetValue' >
							<argument exp='this' />
							<argument exp='&quot;temperature&quot;' />
							<argument exp='.temperature' />
						</command>
						<command class_type='calculation' title='Calculation' cmdcolor='8415488' cmdcolor_sec='14994788' >
							<exp exp='.temp_str = &quot;0000&quot; + STRING (SIGNED (fabs (temperature * 1000) % 1000))' />
							<exp exp='.temp_str = Right$ (.temp_str,3)' />
							<exp exp='.temp_str = STRING (SIGNED (temperature)) + &quot;.&quot; + .temp_str + &quot; C&quot;' />
							<exp exp='' />
						</command>
						<command class_type='sim' title='Simulation' cmdcolor='16744448' cmdcolor_sec='16760919' macro='Component.Property.SetValue' >
							<argument exp='temp_text' />
							<argument exp='&quot;text&quot;' />
							<argument exp='.temp_str' />
						</command>
						<command class_type='decision' title='Decision' cmdcolor='32896' cmdcolor_sec='6612196' exp='number_base &gt; 0' swap='0' >
							<flowline >
								<command class_type='calculation' title='Calculation' cmdcolor='8415488' cmdcolor_sec='14994788' >
									<exp exp='.temp_str = NumberToHex$ (I2C_MSB) + &quot;, &quot; + NumberToHex$ (I2C_LSB)' />
								</command>
							</flowline>
							<flowline >
								<command class_type='calculation' title='Calculation' cmdcolor='8415488' cmdcolor_sec='14994788' >
									<exp exp='.temp_str = STRING (I2C_MSB) + &quot;, &quot; + STRING (I2C_LSB)' />
								</command>
							</flowline>
						</command>
						<command class_type='sim' title='Simulation' cmdcolor='16744448' cmdcolor_sec='16760919' macro='Component.Property.SetValue' >
							<argument exp='I2C_text' />
							<argument exp='&quot;text&quot;' />
							<argument exp='.temp_str' />
						</command>
					</flowline>
				</macro>
