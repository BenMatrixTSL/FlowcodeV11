				<macro >
					<flowline name='ReadGasResistance' description='Reads the resistance of the gas in ohms from the last heating cycle.' statediag='0' >
						<return name='Return' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<local name='gas_adc' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
