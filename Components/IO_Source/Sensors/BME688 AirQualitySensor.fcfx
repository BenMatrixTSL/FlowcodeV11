				<macro >
					<flowline name='ReadRegister16' description='Reads a dual byte value from a register pair LSB / MSB' statediag='0' >
						<return name='Return' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
