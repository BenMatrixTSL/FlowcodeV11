								</command>
							</flowline>
							<flowline />
						</command>
						<command class_type='comment' title='' comment='Collect 20-bit temperature measurement' textarea='12,0,12,0' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' />
						<command class_type='call' title='Read MSB' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='ReadRegister' >
							<return exp='.Byte' />
							<argument exp='0x22 + (.Index * 0x11)' />
						</command>
						<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
							<exp exp='.temp_adc = .Byte' />
						</command>
						<command class_type='call' title='Read LSB' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='ReadRegister' >
							<return exp='.Byte' />
							<argument exp='0x23 + (.Index * 0x11)' />
						</command>
						<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
							<exp exp='.temp_adc = .temp_adc &lt;&lt; 8' />
							<exp exp='.temp_adc = .temp_adc | .Byte' />
						</command>
						<command class_type='call' title='Read XLSB' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='ReadRegister' >
							<return exp='.Byte' />
							<argument exp='0x24 + (.Index * 0x11)' />
						</command>
						<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
							<exp exp='.temp_adc = .temp_adc &lt;&lt; 4' />
							<exp exp='.temp_adc = .temp_adc | (.Byte &gt;&gt; 4)' />
						</command>
						<command class_type='comment' title='' comment='Perform calcuation based on calibrated coefficients' textarea='12,0,12,0' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' />
						<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
							<exp exp='.var1 = ((FLOAT .temp_adc / 16384.0) - (FLOAT par_t1 / 1024.0)) * FLOAT par_t2' />
						</command>
						<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
							<exp exp='.var2 = (((FLOAT .temp_adc / 131072.0) - (FLOAT par_t1 / 8192.0)) * ((FLOAT .temp_adc / 131072.0) - (FLOAT par_t1 / 8192.0))) * (FLOAT par_t3 * 16.0)' />
						</command>
						<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
							<exp exp='t_fine = .var1 + .var2' />
						</command>
						<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
							<exp exp='temp_comp = t_fine / 5120.0' />
							<exp exp='.Return = temp_comp' />
						</command>
