									<exp exp='.char = (Serial[5 - .idx] &amp; 0x0F)' />
								</command>
								<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='0' cmdgradient='4' cmdopacity='0.64' exp='.char &gt; 9' swap='0' >
									<flowline >
