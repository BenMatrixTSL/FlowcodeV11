//BME280 Datasheet Page 23-24

#define BME280_S32_t MX_SINT32
#define BME280_U32_t MX_UINT32

BME280_S32_t v_x1_u32r;
BME280_U32_t h;

v_x1_u32r = (FCL_TFINE - ((BME280_S32_t)76800));
v_x1_u32r = (((((FCL_ADC_H &lt;&lt; 14) - (((BME280_S32_t)FCL_H4) &lt;&lt; 20) -
