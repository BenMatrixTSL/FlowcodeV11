                    ((BME280_S32_t)2097152)) * ((BME280_S32_t)FCL_H2) + 8192) &gt;&gt; 14));

v_x1_u32r = (v_x1_u32r - (((((v_x1_u32r &gt;&gt; 15) * (v_x1_u32r &gt;&gt; 15)) &gt;&gt; 7) *
                               ((BME280_S32_t)FCL_H1)) &gt;&gt; 4));
v_x1_u32r = (v_x1_u32r &lt; 0) ? 0 : v_x1_u32r;
v_x1_u32r = (v_x1_u32r &gt; 419430400) ? 419430400 : v_x1_u32r;
h = (BME280_U32_t)(v_x1_u32r&gt;&gt;12);
//return h;
FCR_RETVAL = h/1024.0;' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' language='C' />
