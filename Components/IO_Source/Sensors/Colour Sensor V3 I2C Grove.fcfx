						<local name='Z' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<local name='xc' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<local name='yc' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<local name='n' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<local name='cct' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<command class_type='calculation' title='Map RGB values to their XYZ Counterparts' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' >
							<exp exp='.X = (-0.14282 * FLOAT R) + (1.54924 * FLOAT G) + (-0.95641 * FLOAT B)' />
							<exp exp='.Y = (-0.32466 * FLOAT R) + (1.57837 * FLOAT G) + (-0.73191 * FLOAT B)' />
							<exp exp='.Z = (-0.68202 * FLOAT R) + (0.77073 * FLOAT G) + (0.56332 * FLOAT B)' />
						</command>
						<command class_type='calculation' title='Calculate Chromicity co-ordinates' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' >
							<exp exp='.xc = (.X) / (.X + .Y + .Z)' />
							<exp exp='.yc = (.Y) / (.X + .Y + .Z)' />
						</command>
						<command class_type='calculation' title='Use McCamy&apos;s Formula to determine CCT' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' >
							<exp exp='.n = (.xc - 0.3320) / (0.1858 - .yc)' />
						</command>
						<command class_type='calculation' title='Calculate the final CCT' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' >
							<exp exp='.cct = (449.0 * pow (.n,3)) + (3525.0 * pow (.n,2)) + (6823.3 * .n) + 5520.33' />
							<exp exp='' />
						</command>
						<command class_type='calculation' title='Calculation' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' >
							<exp exp='.Return = .cct' />
						</command>
					</flowline>
