						<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='IntOpt = 0 || SimulateComms' swap='0' >
							<flowline >
								<command class_type='call' title='Read Status Register' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='Read_Register' >
									<return exp='.temp_reg' />
									<argument exp='0x00' />
								</command>
								<command class_type='decision' title='Check ZYX Data Ready Bit' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='.temp_reg &amp; 0x08' swap='0' >
									<flowline >
										<command class_type='call' title='Read XYZ registers' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='Read_N_Registers' >
											<argument exp='1' />
											<argument exp='6' />
											<argument exp='.data' />
										</command>
										<command class_type='calculation' title='Combine MSB and LSB' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
											<exp exp='X_Var = (((.data[0] &lt;&lt; 8) | .data[1]) &gt;&gt; 4) - 2048' />
											<exp exp='Y_Var = (((.data[2] &lt;&lt; 8) | .data[3]) &gt;&gt; 4) - 2048' />
											<exp exp='Z_Var = (((.data[4] &lt;&lt; 8) | .data[5]) &gt;&gt; 4) - 2048' />
										</command>
