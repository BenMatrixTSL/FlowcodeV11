										<command class_type='call' title='Read XYZ registers' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='Read_N_Registers' >
											<argument exp='1' />
											<argument exp='6' />
											<argument exp='.data' />
										</command>
										<command class_type='calculation' title='Combine MSB and LSB' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
											<exp exp='X_Var = (((.data[0] &lt;&lt; 8) | .data[1]) &gt;&gt; 4) - 2048' />
											<exp exp='Y_Var = (((.data[2] &lt;&lt; 8) | .data[3]) &gt;&gt; 4) - 2048' />
											<exp exp='Z_Var = (((.data[4] &lt;&lt; 8) | .data[5]) &gt;&gt; 4) - 2048' />
										</command>
										<command class_type='calculation' title='New data has been collected' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
											<exp exp='.Return = 1' />
										</command>
									</flowline>
									<flowline />
								</command>
							</flowline>
						</command>
					</flowline>
				</macro>
