												<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
													<exp exp='GripperPosition[1] = GripperPosition[1] + 5' />
												</command>
											</flowline>
											<flowline >
												<command class_type='sim' title='Simulation' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='Panel.Position.MoveAlong' >
													<argument exp='LeftGripper' />
													<argument exp='Gripper' />
													<argument exp='0' />
													<argument exp='-0.1 * .Difference' />
													<argument exp='0' />
												</command>
												<command class_type='sim' title='Simulation' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='Panel.Position.MoveAlong' >
													<argument exp='RightGripper' />
													<argument exp='Gripper' />
													<argument exp='0' />
													<argument exp='0.1 * .Difference' />
													<argument exp='0' />
												</command>
												<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
													<exp exp='GripperPosition[1] = GripperPosition[1] + .Difference' />
												</command>
											</flowline>
										</command>
									</flowline>
								</command>
							</flowline>
							<flowline />
						</command>
						<command class_type='comment' title='' comment='Perform Stepper Motor Simulated Movement' textarea='12,0,12,0' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' />
						<command class_type='switch' title='Collect Biggest Remaining Movement' textarea='36,25,36,25' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='LinMaster' >
							<case >
								<flowline >
									<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
										<exp exp='.Difference = MotorPositionA[1] - MotorPositionA[0]' />
									</command>
								</flowline>
							</case>
							<case exp='1' >
								<flowline >
									<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
										<exp exp='.Difference = MotorPositionB[1] - MotorPositionB[0]' />
									</command>
								</flowline>
							</case>
							<case exp='2' >
								<flowline >
									<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
										<exp exp='.Difference = MotorPositionC[1] - MotorPositionC[0]' />
									</command>
								</flowline>
							</case>
						</command>
						<command class_type='decision' title='Ensure Movement count is positive' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='.Difference &lt; 0' swap='0' >
							<flowline >
								<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='.Difference = - .Difference' />
								</command>
							</flowline>
							<flowline />
						</command>
						<command class_type='decision' title='Work out simulated step size, 50 steps or less' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='.Difference &gt;= 50' swap='0' >
							<flowline >
								<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='.StepCount = 50' />
								</command>
							</flowline>
							<flowline >
								<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='.StepCount = .Difference' />
								</command>
							</flowline>
						</command>
						<command class_type='calculation' title='Work out accumulated step angle ' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
							<exp exp='LinAccum[0] = LinStep[0] * FLOAT .StepCount' />
							<exp exp='LinAccum[1] = LinStep[1] * FLOAT .StepCount' />
							<exp exp='LinAccum[2] = LinStep[2] * FLOAT .StepCount' />
						</command>
						<command class_type='calculation' title='Convert steps to whole numbers and remove from accumulator' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
							<exp exp='.LinAdvance[0] = LinAccum[0]' />
							<exp exp='LinAccum[0] = LinAccum[0] - FLOAT .LinAdvance[0]' />
							<exp exp='.LinAdvance[1] = LinAccum[1]' />
							<exp exp='LinAccum[1] = LinAccum[1] - FLOAT .LinAdvance[1]' />
							<exp exp='.LinAdvance[2] = LinAccum[2]' />
							<exp exp='LinAccum[2] = LinAccum[2] - FLOAT .LinAdvance[2]' />
						</command>
						<command class_type='decision' title='Motor A Movement Required' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='MotorPositionA[0] = MotorPositionA[1]' swap='1' >
							<flowline >
								<command class_type='decision' title='Move Direction?' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='MotorPositionA[0] &lt; MotorPositionA[1]' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
											<exp exp='MotorPositionA[1] = MotorPositionA[1] - .LinAdvance[0]' />
										</command>
									</flowline>
									<flowline >
										<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
											<exp exp='MotorPositionA[1] = MotorPositionA[1] + .LinAdvance[0]' />
										</command>
									</flowline>
								</command>
							</flowline>
							<flowline />
						</command>
						<command class_type='decision' title='Motor B Movement Required' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='MotorPositionB[0] = MotorPositionB[1]' swap='1' >
							<flowline >
								<command class_type='decision' title='Move Direction?' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='MotorPositionB[0] &lt; MotorPositionB[1]' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
											<exp exp='MotorPositionB[1] = MotorPositionB[1] - .LinAdvance[1]' />
										</command>
									</flowline>
									<flowline >
										<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
											<exp exp='MotorPositionB[1] = MotorPositionB[1] + .LinAdvance[1]' />
										</command>
									</flowline>
								</command>
							</flowline>
							<flowline />
						</command>
						<command class_type='decision' title='Motor C Movement Required' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='MotorPositionC[0] = MotorPositionC[1]' swap='1' >
							<flowline >
								<command class_type='decision' title='Move Direction?' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='MotorPositionC[0] &lt; MotorPositionC[1]' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
											<exp exp='MotorPositionC[1] = MotorPositionC[1] - .LinAdvance[2]' />
										</command>
									</flowline>
									<flowline >
										<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
											<exp exp='MotorPositionC[1] = MotorPositionC[1] + .LinAdvance[2]' />
										</command>
									</flowline>
								</command>
							</flowline>
							<flowline />
						</command>
						<command class_type='calculation' title='Step Angle1.8 / 8 / 5.1' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
							<exp exp='.StepAngle = 0.044117' />
						</command>
						<command class_type='calculation' title='Calculate Joint Angles in Degrees' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
							<exp exp='.Angle[0] = .StepAngle * FLOAT MotorPositionA[1]' />
							<exp exp='.Angle[1] = .StepAngle * FLOAT MotorPositionB[1]' />
							<exp exp='.Angle[2] = .StepAngle * FLOAT MotorPositionC[1]' />
						</command>
						<command class_type='sim' title='Simulation' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='Panel.Position.RotateTo' >
							<argument exp='Gripper' />
							<argument exp='Gripper' />
							<argument exp='Gripper' />
							<argument exp='0' />
							<argument exp='0' />
							<argument exp='.Angle[0]' />
						</command>
						<command class_type='calculation' title='Apply Angular Difference' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
							<exp exp='.Angle[2] = (.Angle[1] - .Angle[2]) + 40.0' />
						</command>
						<command class_type='calculation' title='Apply Static Offset' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
							<exp exp='.Angle[1] = 132.0 - .Angle[1]' />
						</command>
						<command class_type='calculation' title='Calculate Joint Angles in Radians' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
							<exp exp='.Angle[0] = .Angle[0] * 0.0174533' />
							<exp exp='.Angle[1] = .Angle[1] * 0.0174533' />
							<exp exp='.Angle[2] = .Angle[2] * 0.0174533' />
						</command>
						<command class_type='calculation' title='Calculate Forward Kinematics' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
							<exp exp='.XYZ[2] = 71.0 + (SIN (.Angle[1]) * 176.0) - (COS (.Angle[2] - (1.58 - .Angle[1])) * 189.0) - 37.0' />
							<exp exp='.K1 = (SIN (.Angle[2] - (1.58 - .Angle[1])) * 189.0) + (COS (.Angle[1]) * 176.0) + 41.0 + 40.0' />
							<exp exp='.K1 = .K1 + 40.0' />
							<exp exp='.XYZ[0] = COS (.Angle[0]) * .K1' />
							<exp exp='.XYZ[1] = SIN (.Angle[0]) * .K1' />
						</command>
						<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
							<exp exp='XYZ[0] = .XYZ[0]' />
							<exp exp='XYZ[1] = .XYZ[1]' />
							<exp exp='XYZ[2] = .XYZ[2]' />
						</command>
						<command class_type='sim' title='Simulation' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='Panel.Position.MoveTo' >
							<argument exp='Gripper' />
							<argument exp='.XYZ[0]' />
							<argument exp='.XYZ[1]' />
							<argument exp='.XYZ[2]' />
						</command>
						<command class_type='sim' title='Simulation' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='System.TimerStart' >
							<argument exp='0' />
							<argument exp='50' />
						</command>
					</flowline>
				</macro>
