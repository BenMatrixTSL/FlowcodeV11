				<macro >
					<flowline name='GetAngles' description='Collects the current joint angles of the motors.' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
						<param name='Angles' type='f32' description='Float Array To Hold the step position return values' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' >
							<array size='3' />
						</param>
						<local name='A' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='B' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='C' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='StepAngle' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<command class_type='sim' title='Simulation' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='Console.WriteText' >
							<argument exp='ConsoleHandle' />
							<argument exp='&quot;\nGetPosition() : &quot;' />
						</command>
						<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='SimMode &amp; 0x01' swap='0' >
							<flowline >
								<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='Out[0] = &apos;:&apos;' />
									<exp exp='Out[1] = &apos;D&apos;' />
									<exp exp='Out[2] = &apos;\n&apos;' />
								</command>
								<command class_type='call' title='Call Macro' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='SendAndReceiveData' >
									<return exp='RetCount' />
									<argument exp='3' />
									<argument exp='6' />
									<argument exp='TOUT_COMM' />
								</command>
								<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='RetCount = 6' swap='0' >
									<flowline >
										<command class_type='calculation' title='Populate Return Data into UINT variables' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
											<exp exp='.A = In[0] | (In[1] &lt;&lt; 8)' />
											<exp exp='.B = In[2] | (In[3] &lt;&lt; 8)' />
											<exp exp='.C = In[4] | (In[5] &lt;&lt; 8)' />
										</command>
										<command class_type='calculation' title='Scale to degrees' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
											<exp exp='.Angles[0] = .A' />
											<exp exp='.Angles[0] = ((.Angles[0] / MICROSTEPCOUNT) * STEPANGLE) / GearRatio' />
											<exp exp='.Angles[1] = .B' />
											<exp exp='.Angles[1] = ((.Angles[1] / MICROSTEPCOUNT) * STEPANGLE) / GearRatio' />
											<exp exp='.Angles[2] = .C' />
											<exp exp='.Angles[2] = ((.Angles[2] / MICROSTEPCOUNT) * STEPANGLE) / GearRatio' />
										</command>
										<command class_type='sim' title='Simulation' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='Console.WriteText' >
											<argument exp='ConsoleHandle' />
											<argument exp='STRING .Angles[0] + &quot;, &quot; + STRING .Angles[1] + &quot;, &quot; + STRING .Angles[2]' />
										</command>
									</flowline>
									<flowline >
										<command class_type='sim' title='Simulation' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='Console.WriteText' >
											<argument exp='ConsoleHandle' />
											<argument exp='&quot;TIMEOUT&quot;' />
										</command>
									</flowline>
								</command>
							</flowline>
							<flowline >
								<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='SimMode &amp; 0x02' swap='0' >
									<flowline >
										<command class_type='calculation' title='Step Angle1.8 / 8 / 5.1' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
											<exp exp='.StepAngle = 0.044117' />
										</command>
										<command class_type='calculation' title='Calculate Joint Angles in Degrees' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
											<exp exp='.Angles[0] = .StepAngle * FLOAT MotorPositionA[0]' />
											<exp exp='.Angles[1] = .StepAngle * FLOAT MotorPositionB[0]' />
											<exp exp='.Angles[2] = .StepAngle * FLOAT MotorPositionC[0]' />
										</command>
										<command class_type='sim' title='Simulation' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='Console.WriteText' >
											<argument exp='ConsoleHandle' />
											<argument exp='STRING .Angles[0] + &quot;, &quot; + STRING .Angles[1] + &quot;, &quot; + STRING .Angles[2]' />
										</command>
									</flowline>
									<flowline />
								</command>
							</flowline>
						</command>
					</flowline>
				</macro>
