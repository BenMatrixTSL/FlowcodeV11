				<macro >
					<flowline name='SetAngle' description='Sets the angle of a single motor' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
						<param name='Motor' type='u8' description='0=A, 1=B, 2=C' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Angle' type='f32' description='Range 0.0 to 359.0' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<local name='StepPosition' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<command class_type='sim' title='Simulation' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='Console.WriteText' >
							<argument exp='ConsoleHandle' />
							<argument exp='&quot;\nSetAngle(&quot; + STRING .Motor + &quot;, &quot; + STRING .Angle + &quot;)&quot;' />
						</command>
						<command class_type='calculation' title='Convert Angle to Step Count' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
							<exp exp='.Angle = .Angle / STEPANGLE' />
						</command>
						<command class_type='calculation' title='Convert Step Count To MicroSteps and scale by gear ratio' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
							<exp exp='.Angle = .Angle * MICROSTEPCOUNT * GearRatio' />
						</command>
						<command class_type='calculation' title='Convert to integer' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
							<exp exp='.StepPosition = .Angle' />
						</command>
						<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='SimMode &amp; 0x01' swap='0' >
							<flowline >
								<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='Out[0] = &apos;:&apos;' />
									<exp exp='Out[1] = &apos;I&apos;' />
									<exp exp='Out[2] = .Motor' />
									<exp exp='Out[3] = .StepPosition' />
									<exp exp='Out[4] = .StepPosition &gt;&gt; 8' />
									<exp exp='Out[5] = &apos;\n&apos;' />
								</command>
								<command class_type='call' title='Call Macro' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='SendAndReceiveData' >
									<return exp='RetCount' />
									<argument exp='6' />
									<argument exp='0' />
									<argument exp='TOUT_COMM' />
								</command>
							</flowline>
							<flowline />
						</command>
						<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='SimMode &amp; 0x02' swap='0' >
							<flowline >
								<command class_type='switch' title='Switch' textarea='36,25,36,25' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='.Motor' >
									<case >
										<flowline >
											<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
												<exp exp='MotorPositionA[0] = .StepPosition' />
											</command>
