				<macro >
					<flowline name='SetXYZ' description='Moves the machine so that the end tool is in the specified XYZ position.
Returns 1 if the coordinates are within range of the Arm else returns 0.' statediag='0' >
						<return name='Return' type='b1' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='X' type='s16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Y' type='s16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Z' type='s16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='L1' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<local name='L7' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<local name='a' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<local name='b' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<local name='c' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<local name='Angle' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' >
							<array size='3' />
						</local>
						<local name='xyz' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' >
							<array size='3' />
						</local>
						<command class_type='sim' title='Simulation' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='Console.WriteText' >
							<argument exp='ConsoleHandle' />
							<argument exp='&quot;\nSetXYZ(&quot; + STRING .X + &quot;, &quot; + STRING .Y + &quot;, &quot; + STRING .Z + &quot;)&quot;' />
						</command>
						<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='SimMode &amp; 0x01' swap='0' >
							<flowline >
								<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='Out[0] = &apos;:&apos;' />
									<exp exp='Out[1] = &apos;M&apos;' />
									<exp exp='Out[2] = .X &amp; 0xFF' />
									<exp exp='.X = .X &gt;&gt; 8' />
									<exp exp='Out[3] = .X &amp; 0xFF' />
									<exp exp='Out[4] = .Y &amp; 0xFF' />
									<exp exp='.Y = .Y &gt;&gt; 8' />
									<exp exp='Out[5] = .Y &amp; 0xFF' />
									<exp exp='Out[6] = .Z &amp; 0xFF' />
									<exp exp='.Z = .Z &gt;&gt; 8' />
									<exp exp='Out[7] = .Z &amp; 0xFF' />
									<exp exp='Out[8] = &apos;\n&apos;' />
								</command>
								<command class_type='call' title='Call Macro' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='SendAndReceiveData' >
									<return exp='RetCount' />
									<argument exp='9' />
									<argument exp='0' />
									<argument exp='TOUT_COMM' />
								</command>
							</flowline>
							<flowline />
						</command>
						<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='SimMode &amp; 2' swap='0' >
							<flowline >
								<command class_type='calculation' title='Convert Step Position into Angle in Degrees' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='.angle[0] = FLOAT MotorPositionA[1] * 0.044118' />
									<exp exp='.angle[1] = FLOAT MotorPositionB[1] * 0.044118' />
									<exp exp='.angle[2] = FLOAT MotorPositionC[1] * 0.044118' />
								</command>
								<command class_type='calculation' title='Now convert degrees into radians' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='.angle[0] = .angle[0] * 0.0174533' />
									<exp exp='.angle[1] = .angle[1] * 0.0174533' />
									<exp exp='.angle[2] = .angle[2] * 0.0174533' />
								</command>
								<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='.XYZ[0] = .X' />
									<exp exp='.XYZ[1] = .Y' />
									<exp exp='.XYZ[2] = .Z' />
								</command>
								<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='.Angle[0] = ATAN2 (.XYZ[1], .XYZ[0])' />
								</command>
								<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='.XYZ[0] = .XYZ[0] - COS (.Angle[0]) * 40.0' />
									<exp exp='.XYZ[1] = .XYZ[1] - SIN (.Angle[0]) * 40.0' />
									<exp exp='.XYZ[2] = .XYZ[2] - -77.0 - 71.0' />
								</command>
								<command class_type='calculation' title='Offset in X direction' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='.XYZ[0] = .XYZ[0] - (cos (.angle[0]) * 40.0)' />
									<exp exp='.XYZ[1] = .XYZ[1] - (sin (.angle[0]) * 40.0)' />
								</command>
								<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='.L1 = SQRT ((.xyz[0] * .xyz[0]) + (.xyz[1] * .xyz[1])) - 41.0' />
									<exp exp='.L7 = SQRT ((.L1 * .L1) + (.xyz[2] * .xyz[2]))' />
									<exp exp='.a = .xyz[2] / .L7' />
									<exp exp='.b = (.L7 * .L7 + 177.0 * 177.0 - 180.0 * 180.0) / (2 * .L7 * 177.0)' />
									<exp exp='.c = (177.0 * 177.0 + 180.0 * 180.0 - .L7 * .L7) / (2 * 177.0 * 180.0)' />
									<exp exp='.Angle[1] = (ATAN2 (.a,SQRT (1.0 - .a * .a)) + ATAN2 (SQRT (1.0 - .b * .b), .b))' />
									<exp exp='.Angle[2] = ATAN2 (SQRT (1.0 - .c * .c), .c)' />
								</command>
								<command class_type='calculation' title='Convert Angles from Radians into Degrees' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='.angle[0] = .angle[0] * 57.2958' />
									<exp exp='.angle[1] = .angle[1] * 57.2958' />
									<exp exp='.angle[2] = .angle[2] * 57.2958' />
								</command>
								<command class_type='calculation' title='Apply Static Offset' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='.angle[1] = 132.0 - .angle[1]' />
								</command>
								<command class_type='calculation' title='Apply Angular Difference' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='.angle[2] = .angle[1] - (.angle[2] - 40.0)' />
								</command>
								<command class_type='calculation' title='Convert Angles back into Step Counts' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='MotorPositionA[0] = .angle[0] * 22.666667' />
									<exp exp='MotorPositionB[0] = .angle[1] * 22.666667' />
									<exp exp='MotorPositionC[0] = .angle[2] * 22.666667' />
								</command>
								<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='MotorPositionA[0] &gt; 5800' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
											<exp exp='MotorPositionA[0] = 0' />
										</command>
									</flowline>
									<flowline />
								</command>
								<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='MotorPositionB[0] &gt; 2200' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
											<exp exp='MotorPositionB[0] = 0' />
										</command>
									</flowline>
									<flowline />
								</command>
								<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='MotorPositionC[0] &gt; 1900' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
											<exp exp='MotorPositionC[0] = 0' />
										</command>
									</flowline>
									<flowline />
								</command>
								<command class_type='call' title='Call Macro' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='SimLinInterpolate' />
							</flowline>
							<flowline />
						</command>
					</flowline>
				</macro>
				<macro >
					<flowline name='SetBluetoothPin' description='Configures the Bluetooth pin and stores into the none volatie memory onboard the robot arm.
Call the SetBluetoothMode macro after this to action the change.' statediag='0' >
