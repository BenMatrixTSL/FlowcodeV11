				<macro >
					<flowline name='StoreAngles' description='Stores the specified joint angles into an internal none volatile memory location.' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
						<param name='PositionIndex' type='u8' description='Range: 0 to 40' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='AngleA' type='f32' description='Range 0.0 to 359.0' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<param name='AngleB' type='f32' description='Range 0.0 to 359.0' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<param name='AngleC' type='f32' description='Range 0.0 to 359.0' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<local name='StepPositionA' type='u32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='StepPositionB' type='u32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='StepPositionC' type='u32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<command class_type='sim' title='Simulation' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='Console.WriteText' >
							<argument exp='ConsoleHandle' />
							<argument exp='&quot;\nStoreAngles(&quot; + STRING .PositionIndex + &quot;, &quot; + STRING .AngleA + &quot;, &quot; + STRING .AngleB + &quot;, &quot; + STRING .AngleC + &quot;)&quot;' />
						</command>
						<command class_type='calculation' title='Convert Angle to Step Count' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
							<exp exp='.AngleA = .AngleA / STEPANGLE' />
							<exp exp='.AngleB = .AngleB / STEPANGLE' />
							<exp exp='.AngleC = .AngleC / STEPANGLE' />
						</command>
						<command class_type='calculation' title='Convert Step Count To MicroSteps' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
							<exp exp='.StepPositionA = .AngleA * MICROSTEPCOUNT' />
							<exp exp='.StepPositionB = .AngleA * MICROSTEPCOUNT' />
							<exp exp='.StepPositionC = .AngleA * MICROSTEPCOUNT' />
						</command>
						<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='SimMode &amp; 0x01' swap='0' >
							<flowline >
								<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='Out[0] = &apos;:&apos;' />
									<exp exp='Out[1] = &apos;G&apos;' />
									<exp exp='Out[2] = .PositionIndex' />
									<exp exp='Out[3] = .StepPositionA &amp; 0xFF' />
									<exp exp='.StepPositionA = .StepPositionA &gt;&gt; 8' />
									<exp exp='Out[4] = .StepPositionA &amp; 0xFF' />
									<exp exp='.StepPositionA = .StepPositionA &gt;&gt; 8' />
									<exp exp='Out[5] = .StepPositionA &amp; 0xFF' />
									<exp exp='.StepPositionA = .StepPositionA &gt;&gt; 8' />
									<exp exp='Out[6] = .StepPositionA &amp; 0xFF' />
									<exp exp='Out[7] = .StepPositionB &amp; 0xFF' />
									<exp exp='.StepPositionB = .StepPositionB &gt;&gt; 8' />
									<exp exp='Out[8] = .StepPositionB &amp; 0xFF' />
									<exp exp='.StepPositionB = .StepPositionB &gt;&gt; 8' />
									<exp exp='Out[9] = .StepPositionB &amp; 0xFF' />
									<exp exp='.StepPositionB = .StepPositionB &gt;&gt; 8' />
									<exp exp='Out[10] = .StepPositionB &amp; 0xFF' />
									<exp exp='Out[11] = .StepPositionC &amp; 0xFF' />
									<exp exp='.StepPositionC = .StepPositionC &gt;&gt; 8' />
									<exp exp='Out[12] = .StepPositionC &amp; 0xFF' />
									<exp exp='.StepPositionC = .StepPositionC &gt;&gt; 8' />
									<exp exp='Out[13] = .StepPositionC &amp; 0xFF' />
									<exp exp='.StepPositionC = .StepPositionC &gt;&gt; 8' />
									<exp exp='Out[14] = .StepPositionC &amp; 0xFF' />
									<exp exp='Out[15] = &apos;\n&apos;' />
								</command>
								<command class_type='call' title='Call Macro' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='SendAndReceiveData' >
									<return exp='RetCount' />
									<argument exp='16' />
									<argument exp='0' />
									<argument exp='TOUT_COMM' />
								</command>
							</flowline>
							<flowline />
						</command>
						<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='SimMode &amp; 0x02' swap='0' >
							<flowline >
								<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='.PositionIndex &lt; 41' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
											<exp exp='SavePositions[.PositionIndex][0] = .StepPositionA' />
											<exp exp='SavePositions[.PositionIndex][1] = .StepPositionB' />
											<exp exp='SavePositions[.PositionIndex][2] = .StepPositionC' />
										</command>
									</flowline>
									<flowline />
								</command>
							</flowline>
							<flowline />
						</command>
					</flowline>
				</macro>
