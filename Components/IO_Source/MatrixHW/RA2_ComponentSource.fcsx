				<macro >
					<flowline name='SetCalibrationLengths' description='Sets the lengths of parameters A and D used as part of the kinematics calculations
to get and set the angles using XYZ coordinates' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
						<param name='LengthA' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<param name='LengthD' type='f32' description='' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<local name='A' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='D' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<command class_type='sim' title='Simulation' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='Console.WriteText' >
							<argument exp='ConsoleHandle' />
							<argument exp='&quot;\nSetCalibrationLengths(&quot; + STRING .LengthA + &quot;, &quot; + STRING .LengthD + &quot;)&quot;' />
						</command>
						<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='SimMode &amp; 0x01' swap='0' >
							<flowline >
								<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='.LengthA = .LengthA * 100.0' />
									<exp exp='.LengthD = .LengthD * 100.0' />
								</command>
								<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='.A = .LengthA' />
									<exp exp='.D = .LengthD' />
								</command>
								<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='Out[0] = &apos;:&apos;' />
									<exp exp='Out[1] = &apos;d&apos;' />
									<exp exp='Out[2] = .A &amp; 0xFF' />
									<exp exp='.A = .A &gt;&gt; 8' />
									<exp exp='Out[3] = .A &amp; 0xFF' />
									<exp exp='Out[4] = .D &amp; 0xFF' />
									<exp exp='.D = .D &gt;&gt; 8' />
									<exp exp='Out[5] = .D &amp; 0xFF' />
									<exp exp='Out[6] = &apos;\n&apos;' />
								</command>
								<command class_type='call' title='Call Macro' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='SendAndReceiveData' >
									<return exp='RetCount' />
									<argument exp='7' />
									<argument exp='0' />
									<argument exp='TOUT_COMM' />
								</command>
								<command class_type='call' title='Call Macro' disable='1' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='SendAndReceiveData' >
