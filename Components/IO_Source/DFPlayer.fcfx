						<command class_type='calculation' title='Invert Checksum' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
							<exp exp='.Checksum = 0 - .Checksum' />
						</command>
						<command class_type='calculation' title='Assign checksum to data packet' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
							<exp exp='CommandData[7] = .Checksum &gt;&gt; 8' />
							<exp exp='CommandData[8] = .Checksum' />
