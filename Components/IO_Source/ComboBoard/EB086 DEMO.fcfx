								<command class_type='call' title='Call Component Macro' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' component='led_7seg_quad' macro='ShowDigit' >
									<argument exp='.index' />
									<argument exp='.number' />
									<argument exp='.point' />
								</command>
								<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='.index = .index + 1' />
								</command>
							</flowline>
						</command>
					</flowline>
