									<flowline >
										<command class_type='decision' title='Check if bit is set' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='0' cmdgradient='4' cmdopacity='0.64' exp='.bit' swap='0' >
											<flowline >
