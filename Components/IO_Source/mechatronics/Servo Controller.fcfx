																<argument exp='1' />
																<argument exp='Servo_Position[.Channel]' />
															</command>
														</flowline>
													</case>
													<case exp='2' >
														<flowline >
															<command class_type='calculation' title='Calculation' >
																<exp exp='.Handle = sim2' />
															</command>
															<command class_type='sim' title='Call Component Macro' component='SensorLibraryComponent1' macro='SetValue' >
																<argument exp='2' />
																<argument exp='Servo_Position[.Channel]' />
															</command>
														</flowline>
													</case>
													<case exp='3' >
														<flowline >
															<command class_type='calculation' title='Calculation' >
																<exp exp='.Handle = sim3' />
															</command>
															<command class_type='sim' title='Call Component Macro' component='SensorLibraryComponent1' macro='SetValue' >
																<argument exp='3' />
																<argument exp='Servo_Position[.Channel]' />
															</command>
														</flowline>
													</case>
													<case exp='4' >
														<flowline >
															<command class_type='calculation' title='Calculation' >
																<exp exp='.Handle = sim4' />
															</command>
															<command class_type='sim' title='Call Component Macro' component='SensorLibraryComponent1' macro='SetValue' >
																<argument exp='4' />
																<argument exp='Servo_Position[.Channel]' />
															</command>
														</flowline>
