				<macro >
					<flowline name='MoveToPosition' description='Moves a single step towards the new position, the rate of movement is determined by how often the move to macro is called. 
Returns 1 if the position has been reached.' statediag='0' >
						<return name='Return' type='u8' description='1 = reached target, 0 = target not reached.' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Channel' type='u8' description='The channel to assign the new position. Range: 0-7' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Position' type='u8' description='Target position to move towards. Range: 0-255' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='Pos' type='u16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<command class_type='calculation' title='Arrived - Unless we are told otherwise' >
							<exp exp='.Return = 1' />
						</command>
						<command class_type='calculation' title='Calculation' >
							<exp exp='.Pos = .Position * Servo_Period_Multiplier' />
						</command>
						<command class_type='decision' title='Decision' exp='.Channel &lt; Channel_Count' swap='0' >
							<flowline >
								<command class_type='decision' title='Decision' exp='.Pos != Servo_Position[.Channel]' swap='0' >
									<flowline >
										<command class_type='calculation' title='Not there yet' >
											<exp exp='.Return = 0' />
											<exp exp='' />
										</command>
										<command class_type='decision' title='Decision' exp='.Pos &gt; Servo_Position[.Channel]' swap='0' >
											<flowline >
												<command class_type='decision' title='Decision' exp='.Pos &gt; (Servo_Position[.Channel] + Servo_Period_Multiplier)' swap='0' >
													<flowline >
														<command class_type='calculation' title='Increment Position' >
															<exp exp='Servo_Position[.Channel] = Servo_Position[.Channel] + Servo_Period_Multiplier' />
														</command>
													</flowline>
													<flowline >
														<command class_type='calculation' title='Jump directly to Position' >
															<exp exp='Servo_Position[.Channel] = .Pos' />
														</command>
													</flowline>
												</command>
											</flowline>
											<flowline >
												<command class_type='decision' title='Decision' exp='.Pos &lt; (Servo_Position[.Channel] - Servo_Period_Multiplier)' swap='0' >
													<flowline >
														<command class_type='calculation' title='Decrement Position' >
															<exp exp='Servo_Position[.Channel] = Servo_Position[.Channel] - Servo_Period_Multiplier' />
														</command>
													</flowline>
													<flowline >
														<command class_type='calculation' title='Jump directly to Position' >
															<exp exp='Servo_Position[.Channel] = .Pos' />
														</command>
													</flowline>
												</command>
											</flowline>
										</command>
										<command class_type='calculation' title='Copy to required variable so we dont break the auto move stuff' >
											<exp exp='Servo_Required[.Channel] = Servo_Position[.Channel]' />
										</command>
										<command class_type='native' title='C Code' ccode='#if defined MX_CAL_ESP' language='C' />
										<command class_type='call' title='User Macro' macro='SetPosition' >
											<argument exp='.Channel' />
											<argument exp='Servo_Required[.Channel]' />
										</command>
										<command class_type='native' title='C Code' ccode='#endif' language='C' />
									</flowline>
									<flowline />
								</command>
							</flowline>
							<flowline />
						</command>
					</flowline>
				</macro>
				<macro >
					<flowline name='SetTrim' description='Allows the level of trim for each servo channel to be adjusted progmatically.' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
						<param name='Channel' type='u8' description='The channel to set the position for.  First channel is zero.' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<param name='Trim' type='u8' description='Legacy: 0=0us, 1=+25us, 2=+50us, 3=+75us, 4=+100us, 5=-25us, 6=-50us, 7=-75us, 8=-100us or Smooth: 0-200' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<local name='tcalc' type='s16' description='' isconst='0' isHidden='0' isinit='0' usrinit='0' setinit='' />
						<command class_type='decision' title='Decision' exp='TrimMode' swap='0' >
							<flowline >
								<command class_type='decision' title='Decision' exp='.Channel &lt; Channel_Count' swap='0' >
									<flowline >
										<command class_type='decision' title='Ensure Trim is in range' exp='.Trim &lt; 200' swap='0' >
											<flowline >
												<command class_type='calculation' title='-100 to 100' >
													<exp exp='.tcalc = .Trim - 100' />
												</command>
												<command class_type='calculation' title='Work Out Trim Value' >
													<exp exp='Servo_Trim[.Channel] = .tcalc * (Servo_Clock_Count_25 / 50)' />
												</command>
											</flowline>
											<flowline />
										</command>
									</flowline>
									<flowline />
								</command>
							</flowline>
							<flowline >
								<command class_type='decision' title='Decision' exp='.Channel &lt; Channel_Count' swap='0' >
									<flowline >
										<command class_type='decision' title='Ensure Trim is in range' exp='.Trim &lt; 9' swap='0' >
											<flowline >
												<command class_type='decision' title='Decision' exp='.Trim &gt; 4' swap='0' >
													<flowline >
														<command class_type='calculation' title='Calculation' >
															<exp exp='Servo_Trim[.Channel] = 0 - ((.Trim - 4) * Servo_Clock_Count_25)' />
														</command>
													</flowline>
													<flowline >
														<command class_type='calculation' title='Calculation' >
															<exp exp='Servo_Trim[.Channel] = .Trim * Servo_Clock_Count_25' />
														</command>
													</flowline>
												</command>
											</flowline>
											<flowline />
										</command>
									</flowline>
									<flowline />
								</command>
							</flowline>
						</command>
