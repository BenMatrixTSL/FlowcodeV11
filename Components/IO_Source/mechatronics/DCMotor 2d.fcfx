						<command class_type='decision' title='Decision' exp='Enabled' swap='0' >
							<flowline >
								<command class_type='sim' title='Built-in Function' macro='Component.GetParent' >
									<return exp='.Handle' />
									<argument exp='this' />
								</command>
								<command class_type='decision' title='Decision' exp='.Handle' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='.Div = 25.0' />
										</command>
									</flowline>
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='.Div = 1.0' />
										</command>
									</flowline>
								</command>
								<command class_type='switch' title='Switch' textarea='36,25,36,25' exp='SimMode' >
									<case >
										<flowline >
											<command class_type='calculation' title='Coast' >
												<exp exp='SimSpeed = (SimSpeed * 0.5)' />
											</command>
										</flowline>
									</case>
									<case exp='1' >
										<flowline >
											<command class_type='calculation' title='Forwards' >
												<exp exp='SimSpeed = (SimSpeed * 0.9) + (SimStep * 0.1)' />
											</command>
										</flowline>
									</case>
									<case exp='2' >
										<flowline >
											<command class_type='calculation' title='Reverse' >
												<exp exp='SimSpeed = (SimSpeed * 0.9) + (SimStep * 0.1)' />
											</command>
										</flowline>
									</case>
									<case exp='3' >
										<flowline >
											<command class_type='calculation' title='Coast' >
												<exp exp='SimSpeed = (SimSpeed * 0.95)' />
											</command>
										</flowline>
									</case>
								</command>
								<command class_type='calculation' title='Calculation' >
									<exp exp='SimAngle = SimAngle + (SimSpeed * speed)' />
								</command>
								<command class_type='decision' title='Decision' exp='SimAngle &lt; 0.0' swap='0' >
									<flowline >
										<command class_type='calculation' title='Calculation' >
											<exp exp='SimAngle = SimAngle + 360.0' />
										</command>
									</flowline>
									<flowline />
								</command>
								<command class_type='decision' title='Decision' exp='SimAngle &gt;= 360.0' swap='0' >
									<flowline >
