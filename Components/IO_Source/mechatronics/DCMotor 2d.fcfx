										</command>
									</flowline>
									<flowline />
								</command>
								<command class_type='calculation' title='Convert Degrees to rads' >
									<exp exp='.Rad = SimAngle * 0.0174533' />
								</command>
								<command class_type='calculation' title='Convert Degrees to rads' >
