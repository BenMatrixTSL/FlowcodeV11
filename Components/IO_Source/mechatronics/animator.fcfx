				<macro >
					<flowline name='SetSpeed' description='Set the linear and rotational velocity of the object.  Motion continues without
any further intervention, subject to the friction, gravity and boundary settings.' statediag='0' >
						<return name='Return' type='v0' description='' isconst='0' isHidden='0' isinit='0' usrinit='' setinit='' />
						<param name='AlongX' type='f32' description='Speed along the X-axis.  World units per second.' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<param name='AlongY' type='f32' description='Speed along the Y-axis.  World units per second.' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<param name='AlongZ' type='f32' description='Speed along the Z-axis.  World units per second.' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<param name='AroundX' type='f32' description='Rotational speed around the X-axis.  Degrees per second.' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<param name='AroundY' type='f32' description='Rotational speed around the Y-axis.  Degrees per second.' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<param name='AroundZ' type='f32' description='Rotational speed around the Z-axis.  Degrees per second.' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<param name='Acceleration' type='f32' description='Time in milliseconds to accelerate to the new speed.settings.' isconst='0' isHidden='0' isinit='0' usrinit='0.0' setinit='' />
						<local name='scale' type='f32' description='' isconst='0' isHidden='0' isinit='1' usrinit='0.0' setinit='0.0' />
						<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='.Acceleration = 0' swap='0' >
							<flowline >
								<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='.scale = FLOAT interval / 1000.0' />
									<exp exp='accell = 1.0' />
									<exp exp='accel_slider = 1.0' />
									<exp exp='' />
									<exp exp='move_x = .AlongX * .scale' />
									<exp exp='move_y = .AlongY * .scale' />
									<exp exp='move_z = .AlongZ * .scale' />
									<exp exp='rotate_x = .AroundX * .scale' />
									<exp exp='rotate_y = .AroundY * .scale' />
									<exp exp='rotate_z = .AroundZ * .scale' />
									<exp exp='' />
									<exp exp='dx = move_x' />
									<exp exp='dy = move_y' />
									<exp exp='dz = move_z' />
									<exp exp='rx = rotate_x' />
									<exp exp='ry = rotate_y' />
									<exp exp='rz = rotate_z' />
									<exp exp='' />
									<exp exp='' />
									<exp exp='total_speed = sqrt ((move_x * move_x) + (move_y * move_y) + (move_z + move_z))' />
								</command>
							</flowline>
							<flowline >
								<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='old_move_x = dx' />
									<exp exp='old_move_y = dy' />
									<exp exp='old_move_z = dz' />
									<exp exp='old_rotate_x = rx' />
									<exp exp='old_rotate_y = ry' />
									<exp exp='old_rotate_z = rz' />
									<exp exp='' />
									<exp exp='.scale = FLOAT interval / 1000.0' />
									<exp exp='accell = 1000 * .scale / .Acceleration' />
									<exp exp='accel_slider = 0' />
									<exp exp='' />
									<exp exp='move_x = .AlongX * .scale' />
									<exp exp='move_y = .AlongY * .scale' />
									<exp exp='move_z = .AlongZ * .scale' />
									<exp exp='rotate_x = .AroundX * .scale' />
									<exp exp='rotate_y = .AroundY * .scale' />
									<exp exp='rotate_z = .AroundZ * .scale' />
									<exp exp='total_speed = sqrt ((move_x * move_x) + (move_y * move_y) + (move_z + move_z))' />
								</command>
							</flowline>
						</command>
						<command class_type='decision' title='Decision' cmdcolor='4767473' cmdcolor_sec='11337727' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' exp='total_speed &gt; 0 || rotate_x != 0 || rotate_y != 0 || rotate_z != 0' swap='0' >
							<flowline >
								<command class_type='calculation' title='Calculation' cmdcolor='4206456' cmdcolor_sec='10785756' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' >
									<exp exp='moving = true' />
								</command>
								<command class_type='sim' title='Simulation' cmdcolor='4487093' cmdcolor_sec='11066367' cmdcolor_txt='16777215' cmdgradient='4' cmdopacity='0.64' macro='System.TimerStart' >
									<argument exp='0' />
