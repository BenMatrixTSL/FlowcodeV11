	<supplement use='1' head='#if defined(MX_CAL_PIC32)

register unsigned int startCnt1;
register unsigned int waitCnt1;

#define sbNsDelay( nsDelay )                    \
{                                               \
   register unsigned int startCnt = _CP0_GET_COUNT(); \
   register unsigned int waitCnt = nsDelay / ( 1000000000 / (MX_CLK_SPEED / 2) );  \
   while( _CP0_GET_COUNT() - startCnt &lt; waitCnt ) ; \
}

#else

  #error &quot;Core Timer Component only works with the PIC32 target hardware&quot;
  #define sbNsDelay( nsDelay )  {}

#endif

