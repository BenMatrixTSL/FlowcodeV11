								<command class_type='decision' title='Decision' cmdcolor='16098537' cmdcolor_sec='9519237' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' exp='(.data[.idx] = &apos;}&apos;) &amp;&amp; (.ifcount &gt; 0)' swap='0' >
									<flowline >
										<command class_type='decision' title='Decision' cmdcolor='16098537' cmdcolor_sec='9519237' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' exp='.data[.idx - 2] != &apos;/&apos;' swap='0' >
											<flowline >
												<command class_type='decision' title='Decision' cmdcolor='16098537' cmdcolor_sec='9519237' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' exp='.data[.idx + 2] = &apos;e&apos;' swap='0' >
													<flowline >
														<command class_type='calculation' title='Calculation' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' >
															<exp exp='.Data[.idx] = &apos;#&apos;' />
															<exp exp='.Data[.idx + 1] = &apos;e&apos;' />
															<exp exp='.Data[.idx + 2] = &apos;l&apos;' />
															<exp exp='.Data[.idx + 3] = &apos;s&apos;' />
															<exp exp='.Data[.idx + 4] = &apos;e&apos;' />
															<exp exp='.Data[.idx + 5] = &apos; &apos;' />
															<exp exp='.Data[.idx + 6] = &apos; &apos;' />
															<exp exp='.Data[.idx + 7] = &apos; &apos;' />
														</command>
													</flowline>
													<flowline >
														<command class_type='calculation' title='Calculation' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' >
															<exp exp='.ifcount = .ifcount - 1' />
														</command>
														<command class_type='calculation' title='Calculation' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' >
															<exp exp='.inneridx = 0' />
														</command>
														<command class_type='loop' title='Loop' cmdcolor='16098537' cmdcolor_sec='9519237' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' type='0' exp='.inneridx &lt; (.len - .idx)' >
															<flowline >
																<command class_type='calculation' title='Calculation' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' >
																	<exp exp='.Data[.len - .inneridx] = .Data[.len - .inneridx - 5]' />
																</command>
																<command class_type='calculation' title='Calculation' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' >
																	<exp exp='.inneridx = .inneridx + 1' />
																</command>
															</flowline>
														</command>
														<command class_type='calculation' title='Calculation' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' >
															<exp exp='.Data[.idx] = &apos;#&apos;' />
															<exp exp='.Data[.idx + 1] = &apos;e&apos;' />
															<exp exp='.Data[.idx + 2] = &apos;n&apos;' />
															<exp exp='.Data[.idx + 3] = &apos;d&apos;' />
															<exp exp='.Data[.idx + 4] = &apos;i&apos;' />
															<exp exp='.Data[.idx + 5] = &apos;f&apos;' />
															<exp exp='.len = .len + 5' />
														</command>
													</flowline>
												</command>
											</flowline>
											<flowline />
										</command>
									</flowline>
									<flowline />
								</command>
							</flowline>
						</command>
						<command class_type='calculation' title='Calculation' cmdcolor='16772726' cmdcolor_sec='13404690' cmdcolor_txt='8355711' cmdgradient='1' cmdopacity='1' >
							<exp exp='.HandlerCode = .HandlerCode + .Data' />
