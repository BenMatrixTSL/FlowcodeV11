						<command class_type='comment' title='' comment='create the Scope group' textarea='12,0,12,0' />
						<command class_type='calculation' title='Calculation' >
							<exp exp='ScopeGroup = ::DataRecorder .AddGroup (&quot;UART &quot; + STRING CHANNEL)' />
