"%~dp0..\bin\avr-gcc.exe" -mmcu=%1 -Os -funsigned-char -o %2 %3 -lm

@IF %ERRORLEVEL% NEQ 0 GOTO Error1

"%~dp0..\bin\avr-objdump.exe" -S %2 > %4
@IF %ERRORLEVEL% NEQ 0 GOTO Error2

"%~dp0..\bin\avr-size.exe" -B -d %2

@ECHO.
@ECHO Compilation successful!
@GOTO End

:Error1
@ECHO.
@ECHO Error returned from [avr-gcc.exe]
@EXIT %ERRORLEVEL%

:Error2
@ECHO.
@ECHO Error returned from [avr-objdump.exe]
@EXIT %ERRORLEVEL%

:End
@EXIT 0
