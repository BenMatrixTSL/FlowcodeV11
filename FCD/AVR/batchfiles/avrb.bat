@REM params = $(compileravr) $(outdir)$(target).elf $(outdir)$(target).hex $(outdir)$(target).cof
@SET MX_COMPILER=%~1
@SET MX_ELF=%2
@SET MX_HEX=%3
@SET MX_COF=%4

"%MX_COMPILER%bin\avr-objcopy" -O ihex -R .eeprom  %MX_ELF% %MX_HEX%

@IF %ERRORLEVEL% NEQ 0 GOTO HexError

@ECHO.
@ECHO HEX file creation successful!
@GOTO End

:HexError
@ECHO.
@ECHO Hex error returned from [avr-objcopy.exe]
@EXIT %ERRORLEVEL%

:End
@EXIT 0
