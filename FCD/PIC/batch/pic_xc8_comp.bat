@REM "$(compilerpic)" $(chip) "$(outdir)" "$(target)"
@SET MX_COMPILER=%~1
@SET MX_CHIP=%~2
@SET MX_OUTDIR=%~3
@SET MX_PROJECT=%~4
@SET MX_ADDPARAM=%~5
@PUSHD %MX_OUTDIR%

@REM Original compiler call
@REM "%MX_COMPILER%bin\xc8-cc.exe" -mcpu=%MX_CHIP% "%MX_PROJECT%.c" -w=359,1273,1388 -fno-short-float -fno-short-double -std=c90 -Os

@REM New compiler call - allowing for additional parameters
"%MX_COMPILER%bin\xc8.exe" --chip=%MX_CHIP% "%MX_PROJECT%.c" --double=32 --float=32 --msgdisable=359,1273,1388 %MX_ADDPARAM%

@POPD
@IF %ERRORLEVEL% NEQ 0 GOTO Error
@ECHO.
@ECHO Compilation successful!
@EXIT 0
:Error
@ECHO.
@ECHO Error returned from [xc8.exe]
@EXIT 1
