@SET MX_PROJECT=%~1
@SET MX_OUTDIR=%~2
@SET MX_COMPILER=%~3
@SET MX_CC=arm-linux-gnueabihf-gcc
@SET MX_COPY=arm-linux-gnueabihf-objcopy
@SET MX_DUMP=arm-linux-gnueabihf-objdump
@SET MX_SIZE=arm-linux-gnueabihf-size
@PUSHD %MX_OUTDIR%
@ECHO Building %MX_PROJECT%
@"%MX_COMPILER%gcc\bin\%MX_CC%" -static -w "%MX_PROJECT%.c" -lm -o "%MX_PROJECT%"
@IF %ERRORLEVEL% NEQ 0 GOTO Error

@"%MX_COMPILER%gcc\bin\%MX_COPY%"  "%MX_PROJECT%" "%MX_PROJECT%.elf"

@ECHO Compilation successful!
@POPD
@EXIT 0

@:Error
@ECHO Error(s) in build!
@POPD
@EXIT 1
