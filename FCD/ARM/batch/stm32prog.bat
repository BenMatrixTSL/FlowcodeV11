@REM Setup the build parameters 
@SET MX_LOCATION=%~1
@SET MX_PROJECT=%~2
@SET MX_STARM=%~3

@REM Save the cuurent path as we are going to change it
@SET MX_PATH=%PATH%

@REM Setup the path for the tools ...
@PATH=%MX_STARM%ST-LINK Utility
ST-LINK_CLI.exe -P "%MX_LOCATION%%MX_PROJECT%.hex" -Run
@IF %ERRORLEVEL% NEQ 0 GOTO Error

@ECHO Download successful!
@PATH=%MX_PATH%
@EXIT 0

@:Error
@ECHO Error(s)!
@PATH=%MX_PATH%
@EXIT 1



