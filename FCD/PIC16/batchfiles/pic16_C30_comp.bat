@REM expected parameters are...
@REM "$(compilerpic16)" "$(target)" "$(outdir)" "$(chip:u)"
@SET MX_COMPILER=%~1
@SET MX_PROJECT=%2
@SET MX_OUTDIR=%~3
@SET MX_CHIP=%4
@set temppath=%path%
@path=%MX_COMPILER%bin;
@PUSHD %MX_OUTDIR%
xc16-gcc -c -mcpu=%MX_CHIP% -omf=coff -funsigned-char -fno-short-double -Os -I"%MX_COMPILER%support\h" -I"%MX_COMPILER%" -I"%MX_COMPILER%batchfiles\" -std=gnu99 %MX_PROJECT%.c -o %MX_PROJECT%.o
@POPD
@IF %ERRORLEVEL% NEQ 0 GOTO Error

@ECHO.
@ECHO Compilation successful!
@path=temppath
@EXIT 0

:Error
@ECHO.
@ECHO Error returned from [xc16-gcc.exe]
@path=temppath
@EXIT 1
